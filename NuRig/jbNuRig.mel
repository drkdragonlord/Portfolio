global proc jbCreateJawGuides()
{
	//creates the initial nodes and names them
	jbJntNode();
	select -r node01;
	string $jawNode01 = `rename node01 "jawNode01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("jawNodeShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("jawNodeShape"+($i+1));
			}						
		}	
	}
	
	select -r $jawNode01;
	
	setAttr ($jawNode01+".ty") 25;
	setAttr ($jawNode01+".tz") 1;
	
	//creates the nodes and places them accordingly
	string $jawNode02[] = `duplicate -rr -n "jawNode02"`;
	setAttr ($jawNode02[0]+".ty") 24.5;	
	setAttr ($jawNode02[0]+".tz") 3;	
	select -r $jawNode01 $jawNode02[0];
	string $jawNodesGrp[] = `ls -sl`;
	
	
	//Creates a variable to use to populate the arm nodes
	string $jawNullsGrp[];
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($jawNodesGrp);$i++)
	{
		string $jawNulls = `group -em -n ($jawNodesGrp[$i]+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $jawNodesGrp[$i] $jawNulls`;		
		delete`pointConstraint -o 0 0 0 -w 1 $jawNodesGrp[$i] $jawNulls`;
		$jawNullsGrp[$i] = $jawNulls;
	}
	
	//creates variables for us to use in the following for loop
	string $jawBone; 
	string $jawBones[]; //will hold the bones
	string $baseClstrs[]; //will hold the bone base clusters
	string $tipClstrs[]; //will hold the bone tip clusters
	
	//creates the clusters and connecting bone lines
	$jawBone = `curve -d 1 -p 0 25 1 -p 0 24.5 3 -k 0 -k 1 -n ("jawBone01")`;	
	select -r ($jawBone+".cv[0]");
	string $clstrBase[] = `cluster -n ($jawBone+"Base")`;
	select -r ($jawBone+".cv[1]");
	string $clstrTip[] = `cluster -n ($jawBone+"Tip")`;
	$baseClstrs[0] = $clstrBase[1];
	$tipClstrs[0] = $clstrTip[1];
	$jawBones[0] = $jawBone;
	
	//group for the jaw bone
	string $jawBoneGrp = `group -em -n "jawBoneGrp"`;
	parent $jawBone $jawBoneGrp;
		
	//parenting procedures
	parent $tipClstrs[0] $jawNodesGrp[1];
	parent $baseClstrs[0] $jawNodesGrp[0];
	
	parent $jawNodesGrp[0] $jawNullsGrp[0];
	parent $jawNodesGrp[1] $jawNullsGrp[1];
	parent $jawNullsGrp[1] $jawNodesGrp[0];
	
	//setting visibility
	setAttr ($tipClstrs[0]+".visibility") 0;
	setAttr ($baseClstrs[0]+".visibility") 0;
	setAttr ($jawBone+".overrideEnabled") 1;
	setAttr ($jawBone+".overrideDisplayType") 2;
	
	//------------------------------
	//creating the head nodes
	//------------------------------
	
	jbJntNode();
	select -r node01;
	string $headNode01 = `rename node01 "headNode01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("headNodeShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("headNodeShape"+($i+1));
			}						
		}	
	}
	
	select -r $headNode01;
	
	setAttr ($headNode01+".ty") 27;
	setAttr ($headNode01+".tz") 0;
	
	string $headNode02[] = `duplicate -rr -n "headNode02"`;
	setAttr ($headNode02[0]+".ty") 30;	
	setAttr ($headNode02[0]+".tz") -2;	
	select -r $headNode01 $headNode02[0];
	string $headNodesGrp[] = `ls -sl`;
	
	
	//Creates a variable to use to populate the arm nodes
	string $headNullsGrp[];
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($headNodesGrp);$i++)
	{
		string $headNulls = `group -em -n ($headNodesGrp[$i]+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $headNodesGrp[$i] $headNulls`;		
		delete`pointConstraint -o 0 0 0 -w 1 $headNodesGrp[$i] $headNulls`;
		$headNullsGrp[$i] = $headNulls;
	}
	
	//creates variables for us to use in the following for loop
	string $headBone; 
	string $headBones[]; //will hold the bones
	string $headBaseClstrs[]; //will hold the bone base clusters
	string $headTipClstrs[]; //will hold the bone tip clusters
	
	//creates the clusters and connecting bone lines
	$headBone = `curve -d 1 -p 0 27 0 -p 0 30 -2 -k 0 -k 1 -n ("headBone01")`;	
	select -r ($headBone+".cv[0]");
	string $headClstrBase[] = `cluster -n ($headBone+"Base")`;
	select -r ($headBone+".cv[1]");
	string $headClstrTip[] = `cluster -n ($headBone+"Tip")`;
	$headBaseClstrs[0] = $headClstrBase[1];
	$headTipClstrs[0] = $headClstrTip[1];
	$headBones[0] = $headBone;
	
	//group for the jaw bone
	string $headBoneGrp = `group -em -n "headBoneGrp"`;
	parent $headBone $headBoneGrp;
	
	//parenting of head parts
	parent $headBaseClstrs[0] $headNodesGrp[0];
	parent $headTipClstrs[0] $headNodesGrp[1];
	
	parent $headNodesGrp[0] $headNullsGrp[0];
	parent $headNodesGrp[1] $headNullsGrp[1];
	parent $headNullsGrp[1] $headNodesGrp[0];
	
	//setting head node visibilities
	setAttr ($headTipClstrs[0]+".visibility") 0;
	setAttr ($headBaseClstrs[0]+".visibility") 0;
	setAttr ($headBone+".overrideEnabled") 1;
	setAttr ($headBone+".overrideDisplayType") 2;
	
	//parenting jaw nodes to head nodes
	parent $jawNullsGrp[0] $headNodesGrp[0];
	
	//creating the head to jaw bone
	$headConnectBone = `curve -d 1 -p 0 27 0 -p 0 25 1 -k 0 -k 1 -n ("headConnectBone01")`;	
	select -r ($headConnectBone+".cv[0]");
	string $headConnectClstrBase[] = `cluster -n ($headConnectBone+"Base")`;
	select -r ($headConnectBone+".cv[1]");
	string $headConnectClstrTip[] = `cluster -n ($headConnectBone+"Tip")`;
	//$headBaseClstrs[0] = $headClstrBase[1];
	//$headTipClstrs[0] = $headClstrTip[1];
	//$headBones[0] = $headConnectBone;
	
	//string $headConnectBoneGrp = `group -em -n "headConnectBoneGrp"`;
	parent $headConnectBone $headBoneGrp;
	
	parent $headConnectClstrBase[1] $headNodesGrp[0];
	parent $headConnectClstrTip[1] $jawNodesGrp[0];
	
	//setting head connect node visibilities
	setAttr ($headConnectClstrTip[1]+".visibility") 0;
	setAttr ($headConnectClstrBase[1]+".visibility") 0;
	setAttr ($headConnectBone+".overrideEnabled") 1;
	setAttr ($headConnectBone+".overrideDisplayType") 2;
	
	//checking for neck guides
	if(`objExists neckGuides`)
	{
		select -r neckNode03;
		string $neckParentNode[] = `ls -sl`;
		
		parent $headNullsGrp[0] $neckParentNode[0];
		
		//creating the head to jaw bone
		string $headToNeckBone = `curve -d 1 -p 0 24 0 -p 0 27 0 -k 0 -k 1 -n ("headToNeckBone01")`;	
		select -r ($headToNeckBone+".cv[0]");
		string $headToNeckBoneClstrBase[] = `cluster -n ($headToNeckBone+"Base")`;
		select -r ($headToNeckBone+".cv[1]");
		string $headToNeckBoneClstrTip[] = `cluster -n ($headToNeckBone+"Tip")`;
		
		parent $headToNeckBone $headBoneGrp;
	
		parent $headToNeckBoneClstrBase[1] $neckParentNode[0];
		parent $headToNeckBoneClstrTip[1] $headNodesGrp[0];
		
		
		//setting head connect node visibilities
		setAttr ($headToNeckBoneClstrTip[1]+".visibility") 0;
		setAttr ($headToNeckBoneClstrBase[1]+".visibility") 0;
		setAttr ($headToNeckBone +".overrideEnabled") 1;
		setAttr ($headToNeckBone +".overrideDisplayType") 2;
		
	}		
	
	//creating the sets
	select -r $headNodesGrp;
	sets -n headNodesSet;
	
	select -r $headNullsGrp $headBoneGrp;
	sets -n headGroupsSet;
	
	select -r $headBone $headConnectBone;
	sets -n headBonesSet;
	if(`objExists headToNeckBone01`)
	{
		select -r headToNeckBone01;
		sets -add headBonesSet;
	}
	
	select -r $headBaseClstrs[0] $headTipClstrs[0] $headConnectClstrTip[1] $headConnectClstrBase[1];
	sets -n headClustersSet;
	if(`objExists headToNeckBone01`)
	{
		select -r headToNeckBone01BaseHandle headToNeckBone01TipHandle;
		sets -add headClustersSet;
	}
		
	select -r $jawNodesGrp;
	sets -n jawNodesSet;
	
	select -r $jawNullsGrp $jawBoneGrp;
	sets -n jawGroupsSet;
	
	select -r $jawBone;
	sets -n jawBonesSet;
	
	select -r $tipClstrs[0] $baseClstrs[0];
	sets -n jawClustersSet;
	
	select -r -ne jawNodesSet jawGroupsSet jawBonesSet jawClustersSet headNodesSet headGroupsSet headBonesSet headClustersSet;
	sets -n jawGuides;	
	
	//locking unecessary attributes
	select -r $headNodesGrp $jawNodesGrp;
	string $nodesSelect[] = `ls -sl`;
	for($i=0; $i<size($nodesSelect);$i++)
	{		
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".rx");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".ry");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".rz");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".sx");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".sy");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".sz");
		setAttr -lock true -keyable false -channelBox false ($nodesSelect[$i]+".v");
	}
	select -cl;
	
}//End global proc jbCreateJawGuides(END)

global proc jbCreateArmGuides()
{	
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//Starts the Left side nodes
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	
	//creates the initial nodes and names them
	jbJntNode();
	select -r node01;
	string $armLfBaseNode = `rename node01 "armNodeLf01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("lfArmShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("lfArmShape"+($i+1));
			}						
		}	
	}
	jbJntNode();
	select -r node01;
	string $armRtBaseNode = `rename node01 "armNodeRt01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("rtArmShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("rtArmShape"+($i+1));
			}						
		}	
	}
	setAttr ($armRtBaseNode+".ry") -180;
	jbPVNodes();
	select -r myPVNode;
	string $armLfPVNode = `rename myPVNode "armLfPVGuide"`;
	select -r $armLfPVNode;
	xform -cp;
	string $armRtPVNode[] = `duplicate -rr -n "armRtPVGuide"`;
	select -r $armLfBaseNode;
	
	//Creates a variable to use to populate the arm nodes
	string $nodesLfGrp[];
	
	//creates the nodes and places them accordingly
	for($i=0;$i<4;$i++)
	{
		string $nodesLf[] = `duplicate -rr -n ("armNodeLf0"+($i+2))`;
		if($i<1)
		{
			setAttr ($nodesLf[0]+".tx") ($i+3);	
		}
		else if($i<2)
		{
			setAttr ($nodesLf[0]+".tx") ($i+6);
			setAttr ($nodesLf[0]+".tz") ($i-2);
			setAttr ($armLfPVNode+".tx") ($i+6);
			setAttr ($armLfPVNode+".tz") ($i-6);
		}
		else if($i<3)
		{
			setAttr ($nodesLf[0]+".tx") ($i+9);
			setAttr ($nodesLf[0]+".tz") ($i-2);
		}
		else
		{
			setAttr ($nodesLf[0]+".tx") ($i+10);	
		}		
		$nodesLfGrp[$i] = $nodesLf[0];
						
	}
	move -a 1 0 0 $armLfBaseNode;
	
	select -cl;
	
	//creates a master array that holds all guides for the left arm
	select -add $armLfBaseNode $nodesLfGrp;
	string $armLfNodes[] = `ls -sl`;
	
	//Creates a variable to use to populate the arm nodes
	string $nullsLfGrp[];
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($armLfNodes);$i++)
	{
		string $nullsLf = `group -em -n ($armLfNodes[$i]+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $armLfNodes[$i] $nullsLf`;		
		delete`pointConstraint -o 0 0 0 -w 1 $armLfNodes[$i] $nullsLf`;
		$nullsLfGrp[$i] = $nullsLf;
	}
	
	//creates constraints between the pole vector nodes and its group fix, then it parents them together
	string $lfPvNull = `group -em -n ($armLfPVNode+"Grp")`;
	delete`orientConstraint -o 0 0 0 -w 1 $armLfPVNode $lfPvNull`;		
	delete`pointConstraint -o 0 0 0 -w 1 $armLfPVNode $lfPvNull`;
	parent $armLfPVNode $lfPvNull;
	
	//creates a variable for us to use in the following for loop
	string $boneLf;
	
	//creates the clusters and connecting bone lines
	for($i=0;$i<4;$i++)
	{
		if($i<1)
		{
			$boneLf = `curve -d 1 -p ($i+1) 0 0 -p ($i+3) 0 0 -k 0 -k 1 -n ("armBoneLf0"+($i+1))`;	
		}
		else if($i<2)
		{
			$boneLf = `curve -d 1 -p ($i+2) 0 0 -p ($i+6) 0 -1 -k 0 -k 1 -n ("armBoneLf0"+($i+1))`;	
		}
		else if($i<3)
		{
			$boneLf = `curve -d 1 -p ($i+5) 0 -1 -p ($i+9) 0 0 -k 0 -k 1 -n ("armBoneLf0"+($i+1))`;	
		}
		else if($i<4)
		{
			$boneLf = `curve -d 1 -p ($i+8) 0 0 -p ($i+10) 0 0 -k 0 -k 1 -n ("armBoneLf0"+($i+1))`;	
		}		
		select -r ($boneLf+".cv[0]");
		string $clstrBase[] = `cluster -n ($boneLf+"Base")`;
		select -r ($boneLf+".cv[1]");
		string $clstrTip[] = `cluster -n ($boneLf+"Tip")`;
	}
	
	//creates the bone and clusters for the Left PV guide and sets the proper parenting
	$boneLf = `curve -d 1 -p 7 0 -1 -p 7 0 -5 -k 0 -k 1 -n "armPVBoneLf01"`;			
	select -r ($boneLf+".cv[0]");
	string $clstrBase[] = `cluster -n ($boneLf+"Base")`;
	select -r ($boneLf+".cv[1]");
	string $clstrTip[] = `cluster -n ($boneLf+"Tip")`;
	
	select -cl;
	select -add armPVBoneLf01TipHandle;
	string $clusterPVTipHndl[] = `ls -sl`;
	select -cl;
	select -add armPVBoneLf01BaseHandle;
	string $clusterPVBaseHndl[] = `ls -sl`;
	
	parent -a $clusterPVTipHndl[0] $armLfPVNode;
	parent -a $clusterPVBaseHndl[0] $armLfNodes[2];
	setAttr ($clusterPVTipHndl[0]+".visibility") 0;
	setAttr ($clusterPVBaseHndl[0]+".visibility") 0;
	setAttr ("armPVBoneLf01.overrideEnabled") 1;
	setAttr ("armPVBoneLf01.overrideDisplayType") 2;	
	parent $lfPvNull $armLfNodes[2];
	
	//creates arrays of all the items created to be used in parenting procedures
	select -cl;	
	select -add armBoneLf01 armBoneLf02 armBoneLf03 armBoneLf04;
	string $armLfBoneGrp[] = `ls -sl`;
	
	select -cl;
	select -add armBoneLf01TipHandle armBoneLf02TipHandle armBoneLf03TipHandle armBoneLf04TipHandle;
	string $clusterTipHndls[] = `ls -sl`;
	
	select -cl;	
	select -add armBoneLf01BaseHandle armBoneLf02BaseHandle armBoneLf03BaseHandle armBoneLf04BaseHandle;
	string $clusterBaseHndls[] = `ls -sl`;
		
	select -cl;
	
	
	//parents the clusters to the nodes and sets some item visibilities and node states
	for($i=0;$i<4;$i++)
	{
		parent -a $clusterTipHndls[$i] $armLfNodes[$i+1];
		parent -a $clusterBaseHndls[$i] $armLfNodes[$i];
		setAttr ($clusterTipHndls[$i]+".visibility") 0;
		setAttr ($clusterBaseHndls[$i]+".visibility") 0;
		setAttr ($armLfBoneGrp[$i]+".overrideEnabled") 1;
		setAttr ($armLfBoneGrp[$i]+".overrideDisplayType") 2;				
	}
	//creates a group for the bone curves to be parented into
	select -cl;
	select -add $armLfBoneGrp armPVBoneLf01;
	string $armLfBoneContainer = `group -n "armBoneLfGrp"`;
	
	//parents the nodes to the groups and the greater group to the lower node for an FK type setup
	for($i=0;$i<5;$i++)
	{
		parent -a $armLfNodes[$i] $nullsLfGrp[$i];
		if($i<=3){
		parent -a $nullsLfGrp[$i+1] $armLfNodes[$i] ;
		}
		else
		{
		}
	}
	
	//Locks the scale and visibility attributes that are not needed by the guides
	setAttr -lock true -keyable false ($armLfNodes[0]+".sx");
	setAttr -lock true -keyable false ($armLfNodes[0]+".sy");
	setAttr -lock true -keyable false ($armLfNodes[0]+".sz");
	setAttr -lock true -keyable false ($armLfNodes[0]+".v");
	
	setAttr -lock true -keyable false ($armLfNodes[1]+".sx");
	setAttr -lock true -keyable false ($armLfNodes[1]+".sy");
	setAttr -lock true -keyable false ($armLfNodes[1]+".sz");
	setAttr -lock true -keyable false ($armLfNodes[1]+".v");
	
	setAttr -lock true -keyable false ($armLfNodes[2]+".sx");
	setAttr -lock true -keyable false ($armLfNodes[2]+".sy");
	setAttr -lock true -keyable false ($armLfNodes[2]+".sz");
	setAttr -lock true -keyable false ($armLfNodes[2]+".v");
	
	setAttr -lock true -keyable false ($armLfNodes[3]+".sx");
	setAttr -lock true -keyable false ($armLfNodes[3]+".sy");
	setAttr -lock true -keyable false ($armLfNodes[3]+".sz");
	setAttr -lock true -keyable false ($armLfNodes[3]+".v");
	
	setAttr -lock true -keyable false ($armLfNodes[4]+".sx");
	setAttr -lock true -keyable false ($armLfNodes[4]+".sy");
	setAttr -lock true -keyable false ($armLfNodes[4]+".sz");
	setAttr -lock true -keyable false ($armLfNodes[4]+".v");
	
	setAttr -lock true -keyable false ($armLfPVNode+".tx");
	setAttr -lock true -keyable false ($armLfPVNode+".ty");
	setAttr -lock true -keyable false ($armLfPVNode+".rx");
	setAttr -lock true -keyable false ($armLfPVNode+".ry");
	setAttr -lock true -keyable false ($armLfPVNode+".rz");
	setAttr -lock true -keyable false ($armLfPVNode+".sx");
	setAttr -lock true -keyable false ($armLfPVNode+".sy");
	setAttr -lock true -keyable false ($armLfPVNode+".sz");
	setAttr -lock true -keyable false ($armLfPVNode+".v");
	
	//Locks the translate Y attributes of the Elbow and wrist joint to maintain co-planar effects. As well as locking the rotate X and Z of the Elbow Node
	setAttr -lock true -keyable false ($armLfNodes[2]+".ty");
	setAttr -lock true -keyable false ($armLfNodes[2]+".rx");
	setAttr -lock true -keyable false ($armLfNodes[2]+".rz");
	setAttr -lock true -keyable false ($armLfNodes[3]+".ty");
	
	//Creates sets to hold the Left Side Nodes for easy script access
	select -cl;
	select -r $armLfNodes;
	sets -n "lfArmGuideNodes";
	
	select -cl;
	select -r $armLfPVNode;
	sets -n "lfArmPVGuideNodes";
	
	select -cl;
	select -add $nullsLfGrp $lfPvNull;
	sets -n "lfArmGuideNodeGrps";
	
	select -cl;
	select -add $armLfBoneGrp $armLfBoneContainer $clusterTipHndls $clusterBaseHndls $clusterPVTipHndl[0] $clusterPVBaseHndl[0] armPVBoneLf01;
	sets -n "lfArmGuideElements";
	
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//Ends the Left side nodes
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	
	
	
	
	
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//Starts the Right side nodes
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	//-----------------------------------------------------------------------------
	
	select -r $armRtBaseNode;
	//Creates a variable to use to populate the arm nodes
	string $nodesRtGrp[];
	
	//creates the nodes and places them accordingly
	for($i=0;$i<4;$i++)
	{
		string $nodesRt[] = `duplicate -rr -n ("armNodeRt0"+($i+2))`;
		if($i<1)
		{
			setAttr ($nodesRt[0]+".tx") ($i-3);	
		}
		else if($i<2)
		{
			setAttr ($nodesRt[0]+".tx") ($i-8);
			setAttr ($nodesRt[0]+".tz") ($i-2);
			setAttr ($armRtPVNode[0]+".tx") ($i-8);
			setAttr ($armRtPVNode[0]+".tz") ($i-6);
		}
		else if($i<3)
		{
			setAttr ($nodesRt[0]+".tx") ($i-13);
			setAttr ($nodesRt[0]+".tz") ($i-2);
		}
		else
		{
			setAttr ($nodesRt[0]+".tx") ($i-16);	
			setAttr ($nodesRt[0]+".rx") 180;
		}		
		$nodesRtGrp[$i] = $nodesRt[0];
						
	}
	move -a -1 0 0 $armRtBaseNode;
	
	select -cl;
	
	select -add $armRtBaseNode $nodesRtGrp;
	string $armRtNodes[] = `ls -sl`;
	
	//Creates a variable to use to populate the arm nodes
	string $nullsRtGrp[];
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($armRtNodes);$i++)
	{
		string $nullsRt = `group -em -n ($armRtNodes[$i]+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $armRtNodes[$i] $nullsRt`;		
		delete`pointConstraint -o 0 0 0 -w 1 $armRtNodes[$i] $nullsRt`;
		$nullsRtGrp[$i] = $nullsRt;
	}
	
	//creates constraints between the pole vector nodes and its group fix, then it parents them together
	string $rtPvNull = `group -em -n ($armRtPVNode[0]+"Grp")`;
	delete`orientConstraint -o 0 0 0 -w 1 $armRtPVNode[0] $rtPvNull`;		
	delete`pointConstraint -o 0 0 0 -w 1 $armRtPVNode[0] $rtPvNull`;
	parent $armRtPVNode[0] $rtPvNull;
	
	//creates a variable for us to use in the following for loop
	string $boneRt;
	
	//creates the clusters and connecting bone lines
	for($i=0;$i<4;$i++)
	{
		if($i<1)
		{
			$boneRt = `curve -d 1 -p ($i-1) 0 0 -p ($i-3) 0 0 -k 0 -k 1 -n ("armBoneRt0"+($i+1))`;	
		}
		else if($i<2)
		{
			$boneRt = `curve -d 1 -p ($i-4) 0 0 -p ($i-8) 0 -1 -k 0 -k 1 -n ("armBoneRt0"+($i+1))`;	
		}
		else if($i<3)
		{
			$boneRt = `curve -d 1 -p ($i-9) 0 -1 -p ($i-13) 0 0 -k 0 -k 1 -n ("armBoneRt0"+($i+1))`;	
		}
		else if($i<4)
		{
			$boneRt = `curve -d 1 -p ($i-14) 0 0 -p ($i-16) 0 0 -k 0 -k 1 -n ("armBoneRt0"+($i+1))`;	
		}		
		select -r ($boneRt+".cv[0]");
		string $clstrBase[] = `cluster -n ($boneRt+"Base")`;
		select -r ($boneRt+".cv[1]");
		string $clstrTip[] = `cluster -n ($boneRt+"Tip")`;
	}
	
	//creates the bone and clusters for the Left PV guide and sets the proper parenting
	$boneRt = `curve -d 1 -p -7 0 -1 -p -7 0 -5 -k 0 -k 1 -n "armPVBoneRt01"`;			
	select -r ($boneRt+".cv[0]");
	string $clstrBase[] = `cluster -n ($boneRt+"Base")`;
	select -r ($boneRt+".cv[1]");
	string $clstrTip[] = `cluster -n ($boneRt+"Tip")`;
	
	select -cl;
	select -add armPVBoneRt01TipHandle;
	string $clusterRtPVTipHndl[] = `ls -sl`;
	select -cl;
	select -add armPVBoneRt01BaseHandle;
	string $clusterRtPVBaseHndl[] = `ls -sl`;
	
	parent -a $clusterRtPVTipHndl[0] $armRtPVNode;
	parent -a $clusterRtPVBaseHndl[0] $armRtNodes[2];
	setAttr ($clusterRtPVTipHndl[0]+".visibility") 0;
	setAttr ($clusterRtPVBaseHndl[0]+".visibility") 0;
	setAttr ("armPVBoneRt01.overrideEnabled") 1;
	setAttr ("armPVBoneRt01.overrideDisplayType") 2;	
	parent $rtPvNull $armRtNodes[2];
	
	//creates arrays of all the items created to be used in parenting procedures
	select -cl;	
	select -add armBoneRt01 armBoneRt02 armBoneRt03 armBoneRt04;
	string $armRtBoneGrp[] = `ls -sl`;
	
	select -cl;
	select -add armBoneRt01TipHandle armBoneRt02TipHandle armBoneRt03TipHandle armBoneRt04TipHandle;
	string $clusterTipHndls[] = `ls -sl`;
	
	select -cl;	
	select -add armBoneRt01BaseHandle armBoneRt02BaseHandle armBoneRt03BaseHandle armBoneRt04BaseHandle;
	string $clusterBaseHndls[] = `ls -sl`;
		
	select -cl;
	
	
	//parents the clusters to the nodes and sets some item visibilities and node states
	for($i=0;$i<4;$i++)
	{
		parent -a $clusterTipHndls[$i] $armRtNodes[$i+1];
		parent -a $clusterBaseHndls[$i] $armRtNodes[$i];
		setAttr ($clusterTipHndls[$i]+".visibility") 0;
		setAttr ($clusterBaseHndls[$i]+".visibility") 0;
		setAttr ($armRtBoneGrp[$i]+".overrideEnabled") 1;
		setAttr ($armRtBoneGrp[$i]+".overrideDisplayType") 2;				
	}
	//creates a group for the bone curves to be parented into
	select -cl;
	select -add $armRtBoneGrp armPVBoneRt01;
	string $armRtBoneContainer = `group -n "armBoneRtGrp"`;
	
	//parents the nodes to the groups and the greater group to the lower node for an FK type setup
	for($i=0;$i<5;$i++)
	{
		parent -a $armRtNodes[$i] $nullsRtGrp[$i];
		if($i<=3){
		parent -a $nullsRtGrp[$i+1] $armRtNodes[$i] ;
		}
		else
		{
		}
	}
	
	//Locks the scale and visibility attributes that are not needed by the guides
	setAttr -lock true -keyable false ($armRtNodes[0]+".sx");
	setAttr -lock true -keyable false ($armRtNodes[0]+".sy");
	setAttr -lock true -keyable false ($armRtNodes[0]+".sz");
	setAttr -lock true -keyable false ($armRtNodes[0]+".v");
	
	setAttr -lock true -keyable false ($armRtNodes[1]+".sx");
	setAttr -lock true -keyable false ($armRtNodes[1]+".sy");
	setAttr -lock true -keyable false ($armRtNodes[1]+".sz");
	setAttr -lock true -keyable false ($armRtNodes[1]+".v");
	
	setAttr -lock true -keyable false ($armRtNodes[2]+".sx");
	setAttr -lock true -keyable false ($armRtNodes[2]+".sy");
	setAttr -lock true -keyable false ($armRtNodes[2]+".sz");
	setAttr -lock true -keyable false ($armRtNodes[2]+".v");
	
	setAttr -lock true -keyable false ($armRtNodes[3]+".sx");
	setAttr -lock true -keyable false ($armRtNodes[3]+".sy");
