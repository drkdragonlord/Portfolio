	setAttr ($rtFkThigh[0]+"Shape.overrideColor") 13;
	setAttr ($rtFkKnee[0]+"Shape.overrideEnabled") 1;
	setAttr ($rtFkKnee[0]+"Shape.overrideColor") 13;
	setAttr ($rtFkAnkle[0]+"Shape.overrideEnabled") 1;
	setAttr ($rtFkAnkle[0]+"Shape.overrideColor") 13;
	
	setAttr ($rtFootCtrl[0]+"Shape.overrideEnabled") 1;
	setAttr ($rtFootCtrl[0]+"Shape1.overrideEnabled") 1;
	setAttr ($rtFootCtrl[0]+"Shape.overrideColor") 13;
	setAttr ($rtFootCtrl[0]+"Shape1.overrideColor") 13; 
	
	setAttr ($rtPVIkCtrlRf[0]+".overrideEnabled") 1;
	setAttr ($rtPVIkCtrlRf[0]+".overrideColor") 13;
	setAttr ($rtIkAnkleCtrl+".overrideEnabled") 1;
	setAttr ($rtIkAnkleCtrl+".overrideColor") 13;
	
	//-------------------------------
