global proc jbCreateCharSet()
{
	if(`objExists nuRigComponents`)
	{
		select -r controls;
		select -hi;
		string $sel[] = `ls -sl -typ "nurbsCurve"`;
		select -r $sel;
		pickWalk -d up;
		
		select -add characterMainCtrl;
		
		string $controls[] = `ls -sl`;
		
		character -name "nuRigCharacterSet";
		
		setCurrentCharacters( { "nuRigCharacterSet" } );
		
		select -cl;
		
		warning ("Nu-Rig character set created successfully!");
		
	}
	else
	{
		error("No Nu-Rig setup exists in the current scene.");
	}

}//End global proc jbCreateCharSet(END)

