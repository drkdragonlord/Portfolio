	delete`orientConstraint -o 0 0 0 -w 1 $lfCalfBaseLoc $lfBaseCalfBendCtrl[0]`;
	select -r $lfBaseCalfBendCtrl[0];
	string $lfBaseCalfBendCtrlRf[] = `duplicate -rr -n ($lfBaseCalfBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfBaseCalfBendCtrl;
