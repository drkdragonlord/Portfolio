	parent $rtMidCalfBendCtrlRf[0] $rtMidCalfBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($rtMidCalfBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($rtMidCalfBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($rtMidCalfBendCtrl[0]+".sz");
