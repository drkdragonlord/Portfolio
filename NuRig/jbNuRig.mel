	setAttr -lock true -keyable false -channelBox false ($rtFkShoulder[0]+".ty");
	setAttr -lock true -keyable false -channelBox false ($rtFkShoulder[0]+".tz");
	setAttr -lock true -keyable false -channelBox false ($rtFkShoulder[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($rtFkShoulder[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($rtFkShoulder[0]+".sz");
