	string $rtRing02Jnt = `joint -p $rtRing02JntPos[0] $rtRing02JntPos[1] $rtRing02JntPos[2] -rad 0.2 -n "rtRing02Jnt"`;
	string $rtRing03Jnt = `joint -p $rtRing03JntPos[0] $rtRing03JntPos[1] $rtRing03JntPos[2] -rad 0.2 -n "rtRing03Jnt"`;
	string $rtRing04Jnt = `joint -p $rtRing04JntPos[0] $rtRing04JntPos[1] $rtRing04JntPos[2] -rad 0.2 -n "rtRing04Jnt"`;
	string $rtRing05Jnt = `joint -p $rtRing05JntPos[0] $rtRing05JntPos[1] $rtRing05JntPos[2] -rad 0.2 -n "rtRing05Jnt"`;
	
	select -cl;
	
	string $rtPinky01Jnt = `joint -p $rtPinky01JntPos[0] $rtPinky01JntPos[1] $rtPinky01JntPos[2] -rad 0.2 -n "rtPinky01Jnt"`;
	string $rtPinky02Jnt = `joint -p $rtPinky02JntPos[0] $rtPinky02JntPos[1] $rtPinky02JntPos[2] -rad 0.2 -n "rtPinky02Jnt"`;
	string $rtPinky03Jnt = `joint -p $rtPinky03JntPos[0] $rtPinky03JntPos[1] $rtPinky03JntPos[2] -rad 0.2 -n "rtPinky03Jnt"`;
	string $rtPinky04Jnt = `joint -p $rtPinky04JntPos[0] $rtPinky04JntPos[1] $rtPinky04JntPos[2] -rad 0.2 -n "rtPinky04Jnt"`;
	string $rtPinky05Jnt = `joint -p $rtPinky05JntPos[0] $rtPinky05JntPos[1] $rtPinky05JntPos[2] -rad 0.2 -n "rtPinky05Jnt"`;
	string $rtPinky06Jnt = `joint -p $rtPinky06JntPos[0] $rtPinky06JntPos[1] $rtPinky06JntPos[2] -rad 0.2 -n "rtPinky06Jnt"`;
	
	select -cl;
	
	string $rtThumb01Jnt = `joint -p $rtThumb01JntPos[0] $rtThumb01JntPos[1] $rtThumb01JntPos[2] -rad 0.2 -n "rtThumb01Jnt"`;
	string $rtThumb02Jnt = `joint -p $rtThumb02JntPos[0] $rtThumb02JntPos[1] $rtThumb02JntPos[2] -rad 0.2 -n "rtThumb02Jnt"`;
	string $rtThumb03Jnt = `joint -p $rtThumb03JntPos[0] $rtThumb03JntPos[1] $rtThumb03JntPos[2] -rad 0.2 -n "rtThumb03Jnt"`;
	string $rtThumb04Jnt = `joint -p $rtThumb04JntPos[0] $rtThumb04JntPos[1] $rtThumb04JntPos[2] -rad 0.2 -n "rtThumb04Jnt"`;
	
	//--------------------
	//Orienting the Joints
	//--------------------
	
	select $rtIndex01Jnt;
	
	// Orients the Joints along the yzx axis.
	
	joint -e -oj yzx -sao zdown -ch -zso;
	
	select -r ($rtIndex05Jnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $rtHeart01Jnt;
	
	// Orients the Joints along the yzx axis.
	
	joint -e -oj yzx -sao zdown -ch -zso;
	
	select -r ($rtHeart05Jnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $rtRing01Jnt;
	
	// Orients the Joints along the yzx axis.
	
	joint -e -oj yzx -sao zdown -ch -zso;
	
	select -r ($rtRing05Jnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $rtPinky01Jnt;
	
	// Orients the Joints along the yzx axis.
	
	joint -e -oj yzx -sao zdown -ch -zso;
	
	select -r ($rtPinky06Jnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $rtThumb01Jnt;
	
	// Orients the Joints along the yzx axis.
	
	joint -e -oj yzx -sao zdown -ch -zso;
	
	select -r ($rtThumb04Jnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	rotate -r -os 27 0 0 ;
	
	//parenting the finger base joints to their respective hand joint
	
	parent -a $rtIndex01Jnt $rtHeart01Jnt $rtRing01Jnt $rtPinky01Jnt $rtThumb01Jnt $parentJoints[1];
	
	select -cl;
	
	select -add $rtIndex01Jnt $rtIndex02Jnt $rtIndex03Jnt $rtIndex04Jnt $rtIndex05Jnt $rtHeart01Jnt $rtHeart02Jnt $rtHeart03Jnt $rtHeart04Jnt $rtHeart05Jnt $rtRing01Jnt $rtRing02Jnt $rtRing03Jnt $rtRing04Jnt $rtRing05Jnt $rtPinky01Jnt $rtPinky02Jnt $rtPinky03Jnt $rtPinky04Jnt $rtPinky05Jnt $rtPinky06Jnt $rtThumb01Jnt $rtThumb02Jnt $rtThumb03Jnt $rtThumb04Jnt;
	string $rtFingers[] = `ls -sl`;
	//select -r $parentJoints[0];
	
	//string $rtFingers[] = `listRelatives -ad -typ "joint"`;
	
	string $rtCrcl[];
	string $rtGrp01;
	string $rtGrp02;
	string $rtMasterGrps[];
	string $rtDirtyCtrls[];
	
	for ($i=0;$i<size($rtFingers);$i++)
	{
		//if($i==5 | $i == 11 | $i == 16 | $i == 22 | $i == 26)		
		$rtCrcl = `circle -nr 0 1 0 -r 0.6 -ch 0 -n ($rtFingers[$i]+"_Ctrl")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtCrcl[0])`;
		delete`orientConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtCrcl[0])`;
		orientConstraint -o 0 0 0 -w 1 ($rtCrcl[0]) ($rtFingers[$i]);			
		$rtGrp01 = `group -em -n ($rtCrcl[0]+"_RF")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtGrp01)`;
		delete`orientConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtGrp01)`;
		parent $rtCrcl[0] $rtGrp01;
		$rtGrp02 = `group -em -n ($rtCrcl[0]+"_RF_Master")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtGrp02)`;
		delete`orientConstraint -o 0 0 0 -w 1 ($rtFingers[$i]) ($rtGrp02)`;
		parent $rtGrp01 $rtGrp02;
		$rtMasterGrps[$i] = $rtGrp02;	
		$rtDirtyCtrls[$i] = $rtCrcl[0];	
	}
	
	select -cl;
	
	string $list[]  = $rtMasterGrps;
  string $items[] = { $rtMasterGrps[4], $rtMasterGrps[9], $rtMasterGrps[14], $rtMasterGrps[20], $rtMasterGrps[24] };
  string $rtCleanMasterGrps[] = stringArrayRemove($items, $list);
  
  select -cl;
  
  //making a group for the inner groups to use later
  select -r $rtCleanMasterGrps;
  pickWalk -d down;
  string $rtRollGrps[] = `ls -sl`;
  
  select -cl;
  
  string $list[]  = $rtDirtyCtrls;
  string $items[] = { $rtDirtyCtrls[4], $rtDirtyCtrls[9], $rtDirtyCtrls[14], $rtDirtyCtrls[20], $rtDirtyCtrls[24] };
  string $rtCleanCtrls[] = stringArrayRemove($items, $list);
  
  select -add $rtMasterGrps[4] $rtMasterGrps[9] $rtMasterGrps[14] $rtMasterGrps[20] $rtMasterGrps[24];
	doDelete;
  
	//select -r $rtCleanMasterGrps;
	
	//parenting the finger controls in an FK hierarchy
	parent $rtCleanMasterGrps[3] $rtCleanCtrls[2];
	parent $rtCleanMasterGrps[2] $rtCleanCtrls[1];
	parent $rtCleanMasterGrps[1] $rtCleanCtrls[0];
	
	parent $rtCleanMasterGrps[7] $rtCleanCtrls[6];
	parent $rtCleanMasterGrps[6] $rtCleanCtrls[5];
	parent $rtCleanMasterGrps[5] $rtCleanCtrls[4];
	
	parent $rtCleanMasterGrps[11] $rtCleanCtrls[10];
	parent $rtCleanMasterGrps[10] $rtCleanCtrls[9];
	parent $rtCleanMasterGrps[9] $rtCleanCtrls[8];
	
	parent $rtCleanMasterGrps[16] $rtCleanCtrls[15];
	parent $rtCleanMasterGrps[15] $rtCleanCtrls[14];
	parent $rtCleanMasterGrps[14] $rtCleanCtrls[13];
	parent $rtCleanMasterGrps[13] $rtCleanCtrls[12];
	
	parent $rtCleanMasterGrps[19] $rtCleanCtrls[18];
	parent $rtCleanMasterGrps[18] $rtCleanCtrls[17];
	
	select -r $rtHandCtrl[0];
	string $rtHandCtrlRf[] = `duplicate -rr -n ($rtHandCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtHandCtrl[0];
	string $rtHandCtrlRf1[] = `duplicate -rr -n ($rtHandCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	string $rtHandPntConst[] = `pointConstraint -o 0 0 0 -w 1 $parentJoints[1] $rtHandCtrlRf1[0]`;
	string $rtHandOrtConst[] = `orientConstraint -o 0 0 0 -w 1 $parentJoints[1] $rtHandCtrlRf1[0]`;
	
	parent $rtHandCtrl[0] $rtHandCtrlRf[0];
	parent $rtHandCtrlRf[0] $rtHandCtrlRf1[0];		
	
	//Finger Controls prenting
	parent $rtCleanMasterGrps[0] $rtCleanMasterGrps[4] $rtCleanMasterGrps[8] $rtCleanMasterGrps[12] $rtCleanMasterGrps[17] $rtHandCtrl[0];
	
	//Locking and disabling visibility of the standard attributes
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".tx");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".ty");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".tz");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".rx");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".ry");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".rz");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($rtHandCtrl[0]+".v");
	
	//print $rtRollGrps;
	
	//Connecting the additional attributes to the local controls
	connectAttr -f ($rtHandCtrl[0]+".indexRot") ($rtRollGrps[0]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".indexSpread") ($rtRollGrps[0]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".indexCurl") ($rtRollGrps[1]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".indexCurl") ($rtRollGrps[2]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".indexCurl") ($rtRollGrps[3]+".rotateZ");
	
	connectAttr -f ($rtHandCtrl[0]+".heartRot") ($rtRollGrps[4]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".heartSpread") ($rtRollGrps[4]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".heartCurl") ($rtRollGrps[5]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".heartCurl") ($rtRollGrps[6]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".heartCurl") ($rtRollGrps[7]+".rotateZ");
	
	connectAttr -f ($rtHandCtrl[0]+".ringRot") ($rtRollGrps[8]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".ringSpread") ($rtRollGrps[8]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".ringCurl") ($rtRollGrps[9]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".ringCurl") ($rtRollGrps[10]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".ringCurl") ($rtRollGrps[11]+".rotateZ");
	
	connectAttr -f ($rtHandCtrl[0]+".pinkyRot") ($rtRollGrps[13]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".pinkySpread") ($rtRollGrps[13]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".pinkyCurl") ($rtRollGrps[14]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".pinkyCurl") ($rtRollGrps[15]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".pinkyCurl") ($rtRollGrps[16]+".rotateZ");
	
	connectAttr -f ($rtHandCtrl[0]+".thumbRot") ($rtRollGrps[17]+".rotateY");
	connectAttr -f ($rtHandCtrl[0]+".thumbSpread") ($rtRollGrps[17]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".thumbCup") ($rtRollGrps[17]+".rotateZ");
	connectAttr -f ($rtHandCtrl[0]+".thumbCurl") ($rtRollGrps[18]+".rotateX");
	connectAttr -f ($rtHandCtrl[0]+".thumbCurl") ($rtRollGrps[19]+".rotateX");
	
	connectAttr -f ($rtHandCtrl[0]+".localControls") ($rtCleanMasterGrps[0]+".visibility");
	connectAttr -f ($rtHandCtrl[0]+".localControls") ($rtCleanMasterGrps[4]+".visibility");
	connectAttr -f ($rtHandCtrl[0]+".localControls") ($rtCleanMasterGrps[8]+".visibility");
	connectAttr -f ($rtHandCtrl[0]+".localControls") ($rtCleanMasterGrps[12]+".visibility");
	connectAttr -f ($rtHandCtrl[0]+".localControls") ($rtCleanMasterGrps[17]+".visibility");
	
	//connecting the local attributes to the main switch control
	connectAttr -f ($rtHandCtrl[0]+".ikBlend") switchCtrl.rtIkBlend;
	connectAttr -f ($rtHandCtrl[0]+".armStretch") switchCtrl.rtArmStretch;
	connectAttr -f ($rtHandCtrl[0]+".armBendy") switchCtrl.rtArmBendy;
	
	//Creating the pinky cup connections
	setAttr ($rtHandCtrl[0]+".pinkyCup") 10;
	setAttr ($rtRollGrps[12]+".rotateX") -2.5;
	setAttr ($rtRollGrps[12]+".rotateY") 23;
	setAttr ($rtRollGrps[12]+".rotateZ") -13;	
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateX");
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateY");
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateZ");
	
	setAttr ($rtHandCtrl[0]+".pinkyCup") 0;
	setAttr ($rtRollGrps[12]+".rotateX") 0;
	setAttr ($rtRollGrps[12]+".rotateY") 0;
	setAttr ($rtRollGrps[12]+".rotateZ") 0;	
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateX");
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateY");
	setDrivenKeyframe -currentDriver ($rtHandCtrl[0]+".pinkyCup") ($rtRollGrps[12]+".rotateZ");
	
	//Hiding the switch Control
	select -r switchCtrl;
	pickWalk -d down;
	HideSelectedObjects;
	
	//Creating controls colors
	for($i=0;$i<size($rtFingers);$i++)
	{
		if($i<20)
		{
			setAttr ($lfCleanCtrls[$i]+"Shape.overrideEnabled") 1;
			setAttr ($lfCleanCtrls[$i]+"Shape.overrideColor") 9;//Fuschia
			setAttr ($rtCleanCtrls[$i]+"Shape.overrideEnabled") 1;
			setAttr ($rtCleanCtrls[$i]+"Shape.overrideColor") 9;//Fuschia
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".tx");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".ty");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".tz");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".sx");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".sy");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".sz");
			setAttr -lock true -keyable false -channelBox false ($lfCleanCtrls[$i]+".v");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".tx");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".ty");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".tz");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".sx");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".sy");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".sz");
			setAttr -lock true -keyable false -channelBox false ($rtCleanCtrls[$i]+".v");
			
		}
		else
		{
			break;
		}
	}
	
	select -r $lfHandCtrl;
	pickWalk -d down;
	string $lfHandCtrlShape[] = `ls -sl`;
	setAttr ($lfHandCtrlShape[0]+".overrideEnabled") 1;
	setAttr ($lfHandCtrlShape[0]+".overrideColor") 17;
	
	select -r $rtHandCtrl;
	pickWalk -d down;
	string $rtHandCtrlShape[] = `ls -sl`;
	setAttr ($rtHandCtrlShape[0]+".overrideEnabled") 1;
	setAttr ($rtHandCtrlShape[0]+".overrideColor") 17;
	
	//create sets of all nodes made	
	select -r $lfFingers $rtFingers;
	sets -n fingerJointsSet;
	
	select -r $lfHandCtrl $rtHandCtrl $lfCleanCtrls $rtCleanCtrls;
	sets -n fingerControlsSet;
	
	select -r $lfHandCtrlRf[0] $lfHandCtrlRf1[0] $rtHandCtrlRf[0] $rtHandCtrlRf1[0] $lfCleanMasterGrps $rtCleanMasterGrps;
	sets -n fingerGroupsSet;
	select -r $lfCleanMasterGrps $rtCleanMasterGrps;
	pickWalk -d down;
	sets -add fingerGroupsSet;
	
	select -r $lfHandPntConst[0] $lfHandOrtConst[0] $rtHandPntConst[0] $rtHandOrtConst[0];
	sets -n fingerConstraintsSet;	
	select -r $lfFingers $rtFingers;
	pickWalk -d down;
	pickWalk -d left;
	sets -add fingerConstraintsSet;	
	
	select -r -ne fingerJointsSet fingerControlsSet fingerGroupsSet fingerConstraintsSet;
	sets -n fingerComponentsSet;
	
	//rename hand shape nodes
	rename $lfHandCtrlShape[0] "lfHandCtrlShape";
	rename $rtHandCtrlShape[0] "rtHandCtrlShape";
	
}//End of global proc jbCreateNewFingers(END)





global proc jbCreateNewNeck()
{
	select -r neckNodesSet;
	string $neckPosSel[] = `ls -sl`;
	
	select -cl;
	
	// Querry of node postions on world space.	
	float $neckJnt01Pos[] = `xform -q -ws -t $neckPosSel[0]`;
	float $neckJnt02Pos[] = `xform -q -ws -t $neckPosSel[1]`;
	float $neckJnt03Pos[] = `xform -q -ws -t $neckPosSel[2]`;
	
	select -cl;
	
	// Creates the neck joint chain based on the positions of the nodes moved by the user.	
	string $neckJnt01 = `joint -p $neckJnt01Pos[0] $neckJnt01Pos[1] $neckJnt01Pos[2] -rad 0.5 -n "neckJnt01"`;
	string $neckJnt02 = `joint -p $neckJnt02Pos[0] $neckJnt02Pos[1] $neckJnt02Pos[2] -rad 0.5 -n "neckJnt02"`;
	string $neckJnt03 = `joint -p $neckJnt03Pos[0] $neckJnt03Pos[1] $neckJnt03Pos[2] -rad 0.5 -n "neckJnt03"`;
	
	//array for the joints
	select -r $neckJnt01 $neckJnt02 $neckJnt03;
	string $neckJnts[] = `ls -sl`;
	
	//variables for the controls
	string $neckCtrl[];
	string $neckJntCtrlRf;
	string $neckJntCtrlRf1;	
	string $neckMasterGrps[];
	string $neckCtrls[];
	
	for($i=0;$i<size($neckJnts);$i++)
	{	
		$neckCtrl = `circle -nr 0 1 0 -r 0.6 -ch 0 -n ($neckJnts[$i]+"Ctrl")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckCtrl[0])`;
		delete`orientConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckCtrl[0])`;
		orientConstraint -o 0 0 0 -w 1 ($neckCtrl[0]) ($neckJnts[$i]);			
		$neckJntCtrlRf = `group -em -n ($neckCtrl[0]+"_RF")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckJntCtrlRf)`;
		delete`orientConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckJntCtrlRf)`;
		parent $neckCtrl[0] $neckJntCtrlRf;
		$neckJntCtrlRf1 = `group -em -n ($neckCtrl[0]+"_RF_Master")`;
		delete`pointConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckJntCtrlRf1)`;
		delete`orientConstraint -o 0 0 0 -w 1 ($neckJnts[$i]) ($neckJntCtrlRf1)`;
		parent $neckJntCtrlRf $neckJntCtrlRf1;
		$neckMasterGrps[$i] = $neckJntCtrlRf1;	
		$neckCtrls[$i] = $neckCtrl[0];				
	}
		
	//parent the groups to the previous controls
	parent $neckMasterGrps[2] $neckCtrls[1];
	parent $neckMasterGrps[1] $neckCtrls[0];
	//parent $lfFkShoulderRf1[0] $lfFkClav;
	
	//setting the color of the controls
	for($i=0;$i<size($neckCtrls);$i++)
	{
		select -r $neckCtrls;
		pickWalk -d down;
		string $neckCtrlsShape[] = `ls -sl`;
		setAttr ($neckCtrlsShape[$i]+".overrideEnabled") 1;
		setAttr ($neckCtrlsShape[$i]+".overrideColor") 17;
	}
		
	//sets for the several parts of the neck rig
	select -r $neckJnts;
	sets -n neckJointsSet;
	
	select -r $neckJnts;
	pickWalk -d down;
	pickWalk -d left;
	sets -n neckConstraintsSet;
	
	select -r $neckCtrls;
	sets -n neckControlsSet;
	
	select -r $neckMasterGrps;
	sets -n neckGroupsSet;
	select -r $neckMasterGrps;
	pickWalk -d down;
	sets -add neckGroupsSet;
	
	select -r -ne neckJointsSet neckControlsSet neckConstraintsSet neckGroupsSet;
	sets -n neckComponents;
	
	//Checking for the Spine Joints
	if(`objExists spineJnt09`)
	{
		select -r spineJnt09;
		string $spineParentJnt[] = `ls -sl`;
		
		select -r spineHiCtrl;
		string $spineControlParent[] = `ls -sl`;
		
		parent $neckMasterGrps[0] $spineControlParent[0];
		parent $neckJnts[0] $spineParentJnt[0];
	}
	
	//Checking for jaw guides to create jaw parts
	if(`objExists jawGuides`)
	{
		jbCreateNewJaw();
	}
	
	//Checking for jaw rig parts to parent to the neck joints
	if(`objExists headJointsSet`)
	{
		select -r headJnt01;
		string $headChildJnt[] = `ls -sl`;
		parent 	$headChildJnt[0] $neckJnts[2];
	}
	
	//deleting the guides
	select -r neckGuides;
	doDelete;	
	
	//--------------------------------
	//Cleaning the Controls Attributes
	//--------------------------------
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".tx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".ty");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".tz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[0]+".v");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".tx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".ty");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".tz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".sx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".sy");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".sz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[1]+".v");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".tx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".ty");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".tz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".sx");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".sy");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".sz");
	setAttr -lock true -keyable false -channelBox false ($neckCtrls[2]+".v");
	
}//End of global proc jbCreateNewNeck(END)





global proc jbCreateNewJaw()
{
	select -r jawNode01 jawNode02;
	string $jawPosSel[] = `ls -sl`;
	
	select -cl;
	
	select -r headNodesSet;
	string $headPosSel[] = `ls -sl`;
	
	select -cl;
	
	// Querry of node postions on world space.	
	float $jawJnt01Pos[] = `xform -q -ws -t $jawPosSel[0]`;
	float $jawJnt02Pos[] = `xform -q -ws -t $jawPosSel[1]`;
	float $headJnt01Pos[] = `xform -q -ws -t $headPosSel[0]`;
	float $headJnt02Pos[] = `xform -q -ws -t $headPosSel[1]`;
	
	select -cl;
	
	// Creates the jaw joint chain based on the positions of the nodes moved by the user.	
	string $jawJnt01 = `joint -p $jawJnt01Pos[0] $jawJnt01Pos[1] $jawJnt01Pos[2] -rad 0.5 -n "jawJnt01"`;
	string $jawJnt02 = `joint -p $jawJnt02Pos[0] $jawJnt02Pos[1] $jawJnt02Pos[2] -rad 0.5 -n "jawJnt02"`;
	
	select -cl;
	
	// Creates the head joint chain based on the positions of the nodes moved by the user.	
	string $headJnt01 = `joint -p $headJnt01Pos[0] $headJnt01Pos[1] $headJnt01Pos[2] -rad 0.5 -n "headJnt01"`;
	string $headJnt02 = `joint -p $headJnt02Pos[0] $headJnt02Pos[1] $headJnt02Pos[2] -rad 0.5 -n "headJnt02"`;
	
	//parenting of the joints
	parent $jawJnt01 $headJnt01;
	
	//creating the sets
	select -r $headJnt01 $headJnt02;
	sets -n headJointsSet;
	
	select -r $jawJnt01 $jawJnt02;
	sets -n jawJointsSet;
	
	select -r -ne headJointsSet jawJointsSet;
	sets -n headComponents;
	
	//deleting the guides
	select -r jawGuides;
	doDelete;
	
}//End global proc jbCreateNewJaw(END)

global proc jbCheckForJawGuides()
{
	if(`objExists jawGuides`)
	{
		if(`objExists neckGuides`)
		{
			jbCreateNewJaw();
			jbCreateNewNeck();
		}
		else
		{
			error ("No neck guides found! Please create neck guides first.");
		}
	}
	else if(`objExists neckGuides`)
	{
		jbCreateNewNeck();
	}
	else
	{
		error ("No neck guides found! Please create neck guides first.");
	}
	
}//End global proc jbCheckForJawGuides(END)

global proc jbCreateNuRig()
{
	if(`objExists spineGuides`)
	{
		if(`objExists armGuides`)
		{
			jbCreateNewArms();
		}
		jbCreateNewSpine();
		jbCreateNewLegs();
	
	}
	else
	{
		error "No guides found!";
	}
}//End global proc jbCreateNuRig(END)

global proc jbCreateSkinJoints(string $rootJnt)
{
	string $skinJntsGrp = `group -em -n "skin_jntsGrp"`;
	string $rigJntsGrp = `group -em -n "rig_jntsGrp"`;
	select -hi $rootJnt;
	string $rigJnts[] = `ls -sl -typ "joint"`;
	
	int $jntCnt = size($rigJnts);
	string $skinJnts[];
	string $ptConst[];
	string $otConst[];
	select -cl;
	
	for($i=0; $i < $jntCnt; $i++)
	{
		float $jntPos[] = `xform -q -ws -t $rigJnts[$i]`;
		string $joint = `joint -p $jntPos[0] $jntPos[1] $jntPos[2] -rad 0.5 -n ("skin_"+$rigJnts[$i])`;
		$skinJnts[$i] = $joint;	
		if($i==6)
		{
			select -r $skinJnts[1];//Skin_Pelvic Jnt
		}
		if($i==11)
		{
			select -r $skinJnts[0];//Skin_RootJoint
		}
		if($i==25)
		{
			select -r $skinJnts[24];//Skin_HeadJoint
		}
		if($i==27)
		{
			select -r $skinJnts[20];//Skin_SpineJoint09
		}
		if($i==37)
		{
			select -r $skinJnts[32];//Skin_LfHandJoint
		}
		if($i==42)
		{
			select -r $skinJnts[32];//Skin_LfHandJoint
		}
		if($i==47)
		{
			select -r $skinJnts[32];//Skin_LfHandJoint
		}
		if($i==53)
		{
			select -r $skinJnts[32];//Skin_LfHandJoint
		}
		if($i==57)
		{
			select -r $skinJnts[20];//Skin_SpineJoint09
		}
		if($i==67)
		{
			select -r $skinJnts[62];//Skin_RtHandJoint
		}
			if($i==72)
		{
			select -r $skinJnts[62];//Skin_RtHandJoint
		}
		if($i==77)
		{
			select -r $skinJnts[62];//Skin_RtHandJoint
		}
		if($i==83)
		{
			select -r $skinJnts[62];//Skin_RtHandJoint
		}	
		string $point[] = `pointConstraint -o 0 0 0 -w 1 $rigJnts[$i] $skinJnts[$i]`;
		string $orient[] = `orientConstraint -o 0 0 0 -w 1 $rigJnts[$i] $skinJnts[$i]`;
		$ptConst[$i] = $point[0];
		$otConst[$i] = $orient[0];
	}
	
	//making the bendy skin joints
	string $skinBendyJntsGrp;
	string $bendyJntsGrp[];	
	string $bendySkinJnts[];
	string $ptConst01[];
	string $otConst01[];
	if(`objExists bendyJntsGrp`)
	{
		$skinBendyJntsGrp = `group -em -n "skin_bendyJntsGrp"`;
		select -r bendyJntsGrp;
		$bendyJntsGrp = `ls -sl`;
		select -hi;
		string $rigBendyJnts[] = `ls -sl -typ "joint"`;
		
		int $bendyJntCnt = size($rigBendyJnts);
	
		select -cl;
		
		for($i=0; $i < $bendyJntCnt; $i++)
		{
			float $jntPos[] = `xform -q -ws -t $rigBendyJnts[$i]`;
			string $joint = `joint -p $jntPos[0] $jntPos[1] $jntPos[2] -rad 0.5 -n ("skin_"+$rigBendyJnts[$i])`;
			$bendySkinJnts[$i] = $joint;
			select -cl;
			string $point[] = `pointConstraint -o 0 0 0 -w 1 $rigBendyJnts[$i] $bendySkinJnts[$i]`;
			string $orient[] = `orientConstraint -o 0 0 0 -w 1 $rigBendyJnts[$i] $bendySkinJnts[$i]`;
			$ptConst01[$i] = $point[0];
			$otConst01[$i] = $orient[0];
		}
	}
		
	//parenting procedures
	parent $bendySkinJnts $skinBendyJntsGrp;
	parent $skinJnts[0] $skinBendyJntsGrp $skinJntsGrp;	
	
	parent $rootJnt $bendyJntsGrp $rigJntsGrp;
	parent $rigJntsGrp $skinJntsGrp skeletons;
	
	setAttr ($rigJntsGrp+".visibility") 0;
	
	//creating sets for the Skin Joints
	select -r $skinJnts $bendySkinJnts;
	sets -n skinJoints;
	
	select -r $ptConst $otConst	$ptConst01 $otConst01;
	sets -n skinJointsConstraintsSet;
	
	select -r -ne skinJointsConstraintsSet;
	sets -add mainRigComponents;
	select -r $skinJntsGrp $skinBendyJntsGrp $rigJntsGrp;
	sets -add mainRigComponents;
	
	select -r -ne skinJoints;
	sets -add nuRigComponents;
	
}//End global proc jbCreateSkinJoints(END)

global proc jbSelectSkinJoints()
{
	if(`objExists nuRigComponents`)
	{
		select -r skinJoints;
		
		warning ("Nu-Rig skin joints selected successfully!");
	}
	else
	{
		error ("Could not find any Nu-Rig skin joints! Make sure Nu-Rig setup has been created.");
	}
}//End global proc jbSelectSkinJoints(END)