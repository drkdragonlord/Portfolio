	select -r $rtIkBlendCond;
	
	//Connect the attributes to the shading nodes
	connectAttr -f ($switchCtrl+".rtIkBlend") ($rtIkBlendCond+".firstTerm");
	connectAttr -f ($switchCtrl+".rtIkBlend") ($rtIkBlendCond+".colorIfTrueR");
	connectAttr -f ($switchCtrl+".rtIkBlend") ($rtIkBlendRev+".inputX");
	
	connectAttr -f ($rtIkBlendRev+".outputX") ($rtWristOrientConstraint+"."+$rtFkWristCtrl+"W0");
	connectAttr -f ($rtIkBlendRev+".outputX") ($rtWristPointConstraint+"."+$rtFkWristCtrl+"W0");
	connectAttr -f ($rtIkBlendRev+".outputX") ($rtPVConstraint+"."+$rtFkPvCtrl+"W0");
	
	connectAttr -f ($rtIkBlendCond+".outColorR") ($rtWristOrientConstraint+"."+$rtIkWristCtrl+"W1");
	connectAttr -f ($rtIkBlendCond+".outColorR") ($rtWristPointConstraint+"."+$rtIkWristCtrl+"W1");
	connectAttr -f ($rtIkBlendCond+".outColorR") ($rtPVConstraint+"."+$rtIkPvCtrl+"W1");

	select -r $switchCtrl $switchCtrlRf $lfIkBlendRev $lfIkBlendCond $rtIkBlendRev $rtIkBlendCond;
	sets -n armSwitchPartsSet;
			
	//select -r $lfWristOrientConstraint;	
	return $switchCtrl;
	
}//End of global proc jbBuildSwitch(END)

global proc jbCreateNewLegs()
{
	clearCache -all;
	
	select -r lfLegGuideNodes;
	
	string $lfPosSel[] = `ls -sl`;
	print $lfPosSel;
	
	select -r lfLegPVGuideNodes;
	
	string $lfPvSel[] = `ls -sl`;
	
	// Querry of node postions on world space.
	
	float $lfThighJntPos[] = `xform -q -ws -t $lfPosSel[0]`;
	float $lfKneeJntPos[] = `xform -q -ws -t $lfPosSel[1]`;
	float $lfAnkleJntPos[] = `xform -q -ws -t $lfPosSel[2]`;
	float $lfBallJntPos[] = `xform -q -ws -t $lfPosSel[3]`;
	float $lfToeJntPos[] = `xform -q -ws -t $lfPosSel[4]`;
	float $lfPVCtrlPos[] = `xform -q -ws -t $lfPvSel[0]`;
	
	select -cl;
	
	// Creates the leg joint chain based on the positions of the nodes moved by the user.
	
	string $lfThighJnt = `joint -p $lfThighJntPos[0] $lfThighJntPos[1] $lfThighJntPos[2] -n "lfThighJnt"`;
	string $lfKneeJnt = `joint -p $lfKneeJntPos[0] $lfKneeJntPos[1] $lfKneeJntPos[2] -n "lfKneeJnt"`;
	string $lfAnkleJnt = `joint -p $lfAnkleJntPos[0] $lfAnkleJntPos[1] $lfAnkleJntPos[2] -n "lfAnkleJnt"`;
	string $lfBallJnt = `joint -p $lfBallJntPos[0] $lfBallJntPos[1] $lfBallJntPos[2] -n "lfBallJnt"`;
	string $lfToeJnt = `joint -p $lfToeJntPos[0] $lfToeJntPos[1] $lfToeJntPos[2] -n "lfToeJnt"`;
	
	select $lfThighJnt;
	
	// Orients the lfJoints along the yzx axis.
	
	joint -e -oj yzx -sao zup -ch -zso;
	
	select -r ($lfToeJnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $lfThighJnt;
	
	// This script assumes character symmetry as joints will be mirrored now from the left leg to the right.	
	mirrorJoint -mirrorYZ -mirrorBehavior -searchReplace "lf" "rt";
	
	//----------------------------------
	// Creating Fk controls for left leg
	//----------------------------------
	select -hi $lfThighJnt;
	string $lfJoints[] = `ls -sl -type "joint"`;
	
	string $lfFkThigh[];
	string $lfFkKnee[];
	string $lfFkAnkle[];
	string $lfFkBall[];
	string $lfFkToe[];
	string $lfFkThighRf[];
	string $lfFkThighRf1[];
	string $lfFkKneeRf[];
	string $lfFkKneeRf1[];
	string $lfFkAnkleRf[];
	string $lfFkAnkleRf1[];
	string $lfFkBallRf1[];
	string $lfFkToeRf1[];
	
	//--------------------------------
	//Create controls for the left leg
	//--------------------------------
	for($i=0;$i<(size($lfJoints)-1);$i++)
	{
		if($i<1)
		{						
			$lfFkThigh = `circle -nr 0 1 0 -n ($lfJoints[0]+"FkCtrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[0] $lfFkThigh[0]`;
			delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[0] $lfFkThigh[0]`;
			select -r $lfFkThigh[0];
			$lfFkThighRf = `duplicate -rr -n ($lfFkThigh[0]+"GRP")`;
			pickWalk -d down;
			doDelete;
			select -r $lfFkThigh[0];
			$lfFkThighRf1 = `duplicate -rr -n ($lfFkThigh[0]+"MasterGRP")`;
			pickWalk -d down;
			doDelete;
			parent $lfFkThigh[0] $lfFkThighRf[0];
			parent $lfFkThighRf[0] $lfFkThighRf1[0];	
			//select -r ($lfFkClav+".cv[0:18]");
			//move -r -os -wd 0 1.5 0;		
		}
		if($i<2)
		{			
			$lfFkKnee = `circle -nr 0 1 0 -n ($lfJoints[1]+"FkCtrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[1] $lfFkKnee[0]`;
			delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[1] $lfFkKnee[0]`;
			select -r $lfFkKnee[0];
			$lfFkKneeRf = `duplicate -rr -n ($lfFkKnee[0]+"GRP")`;
			pickWalk -d down;
			doDelete;
			select -r $lfFkKnee[0];
			$lfFkKneeRf1 = `duplicate -rr -n ($lfFkKnee[0]+"MasterGRP")`;
			pickWalk -d down;
			doDelete;
			parent $lfFkKnee[0] $lfFkKneeRf[0];
			parent $lfFkKneeRf[0] $lfFkKneeRf1[0];	
		}
		if($i<3)
		{
			$lfFkAnkle = `circle -nr 0 1 0 -n ($lfJoints[2]+"FkCtrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfFkAnkle[0]`;
			delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfFkAnkle[0]`;
			select -r ($lfFkAnkle[0]+".cv[0:7]");
			rotate -r -os -30 0 0;
			select -r $lfFkAnkle[0];
			$lfFkAnkleRf = `duplicate -rr -n ($lfFkAnkle[0]+"GRP")`;
			pickWalk -d down;
			doDelete;
			select -r $lfFkAnkle[0];
			$lfFkAnkleRf1 = `duplicate -rr -n ($lfFkAnkle[0]+"MasterGRP")`;
			pickWalk -d down;
			doDelete;
			parent $lfFkAnkle[0] $lfFkAnkleRf[0];
			parent $lfFkAnkleRf[0] $lfFkAnkleRf1[0];			
			break;
		}						
	}
	//parent $lfFkBallRf1[0] $lfFkAnkle[0];
	parent $lfFkAnkleRf1[0] $lfFkKnee[0];
	parent $lfFkKneeRf1[0] $lfFkThigh[0];
	
	jbPVNodes();
	//select -r myPVNode;
	string $legLfPVFkNode = `rename myPVNode "legLfPvFkCtrl"`;
	setAttr ($legLfPVFkNode+".ry") -180;
	select -r ($legLfPVFkNode+".cv[0:4]");
	scale -r -p 0cm 0cm 0cm 1.5 1.5 1.5;
	select -cl;
	move $lfPVCtrlPos[0] $lfPVCtrlPos[1] $lfPVCtrlPos[2] $legLfPVFkNode;
	select -r $legLfPVFkNode;
	string $lfPVFkCtrlRf[] = `duplicate -rr -n ($legLfPVFkNode+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $legLfPVFkNode;
	string $lfPVIkCtrl[] = `duplicate -rr -n "legLfPvIkCtrl"`;
	setAttr ($lfPVIkCtrl[0]+".ry") -180;
	select -r $lfPVIkCtrl[0];
	string $lfPVIkCtrlRf[] = `duplicate -rr -n ($lfPVIkCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	setAttr ($legLfPVFkNode+".visibility") 0;
	
	parent $legLfPVFkNode $lfPVFkCtrlRf[0];
	parent $lfPVIkCtrl[0] $lfPVIkCtrlRf[0];
	parent $lfPVFkCtrlRf[0] $lfFkThigh[0];
	
	//----------------------------------
	// Creating IK controls for left leg
	//----------------------------------
	string $lfIkAnkleCtrl = `curve -d 3 -p 0.3926462374 5.804408662e-017 -1.000012564 -p 2.432377081e-016 6.282646663e-017 -1.000026164 -p -0.3926462374 5.804408662e-017 -1.000012564 -p -0.9994664527 4.442502059e-017 -0.9999738361 -p -1.000256218 2.404264789e-017 -0.3926462374 -p -1.000533547 -2.898033216e-033 4.73284741e-017 -p -1.000256218 -2.404264789e-017 0.3926462374 -p -0.9994664527 -4.442502059e-017 0.9999839861 -p -0.3926462374 -5.804408662e-017 1.00000769 -p 7.753705713e-017 -6.282646663e-017 1.000016014 -p 0.3926462374 -5.804408662e-017 1.00000769 -p 0.9999871991 -4.442502059e-017 0.9999839861 -p 1.000006147 -2.404264789e-017 0.3926462374 -p 1.000012801 1.079327323e-032 -1.762675285e-016 -p 1.000006147 2.404264789e-017 -0.3926462374 -p 0.9999871991 4.442502059e-017 -0.9999738361 -p 0.3926462374 5.804408662e-017 -1.000012564 -p 2.432377081e-016 6.282646663e-017 -1.000026164 -p -0.3926462374 5.804408662e-017 -1.000012564 -k -2 -k -1 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -k 13 -k 14 -k 15 -k 16 -k 17 -k 18 -n ($lfAnkleJnt+"IkCtrl")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfIkAnkleCtrl`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfIkAnkleCtrl`;
	select -r ($lfIkAnkleCtrl+".cv[0:18]");
	rotate -r -os -30 0 0;
	select -r $lfIkAnkleCtrl;
	string $lfIkAnkleRf[] = `duplicate -rr -n ($lfIkAnkleCtrl+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfIkAnkleCtrl;
	$lfIkAnkleRf1 = `duplicate -rr -n ($lfIkAnkleCtrl+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfIkAnkleCtrl $lfIkAnkleRf[0];
	parent $lfIkAnkleRf[0] $lfIkAnkleRf1[0];	
	parent $lfPVIkCtrlRf[0] $lfIkAnkleCtrl;
	
	//---------------------------------
	// Creating Ik Handles for left leg
	//---------------------------------
	string $lfAnkleIk[] = `ikHandle -sol ikRPsolver -sj $lfJoints[0] -ee $lfJoints[2] -n ("lfAnkleIkHandle")`;
	string $lfBallIk[] = `ikHandle -sol ikSCsolver -sj $lfJoints[2] -ee $lfJoints[3] -n ("lfBallIkHandle")`;
	string $lfToeIk[] = `ikHandle -sol ikSCsolver -sj $lfJoints[3] -ee $lfJoints[4] -n ("lfToeIkHandle")`;
	float $lfAnkleIkPosi[] = `xform -q -ws -t $lfAnkleIk[0]`;
	string $lfAnkleIkRf2 = `group -em -n ($lfAnkleIk[0]+"GRP")`;
	string $lfBallIkRf2 = `group -em -n ($lfBallIk[0]+"GRP")`;
	string $lfToeIkRf2 = `group -em -n ($lfToeIk[0]+"GRP")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfAnkleIkRf2`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfBallIkRf2`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfToeIkRf2`;
	parent $lfAnkleIk[0] $lfAnkleIkRf2;
	parent $lfBallIk[0] $lfBallIkRf2;
	parent $lfToeIk[0] $lfToeIkRf2;
	string $lfAnkleIkRf1 = `group -em -n ($lfAnkleIk[0]+"MasterGRP")`;
	string $lfBallIkRf1 = `group -em -n ($lfBallIk[0]+"MasterGRP")`;
	string $lfToeIkRf1 = `group -em -n ($lfToeIk[0]+"MasterGRP")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfAnkleIkRf1`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfBallIkRf1`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfToeIkRf1`;
	parent $lfAnkleIkRf2 $lfAnkleIkRf1;
	parent $lfBallIkRf2 $lfBallIkRf1;
	parent $lfToeIkRf2 $lfToeIkRf1;
	
	setAttr ($lfAnkleIk[0]+".visibility") 0;
	setAttr ($lfBallIk[0]+".visibility") 0;
	setAttr ($lfToeIk[0]+".visibility") 0;
	
	//---------------------------------
	//Foot roll groups for the left leg
	//---------------------------------
	string $lfToeTapGrp = `group -em -n ("lfToeTapGrp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfToeTapGrp`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfToeTapGrp`;
	makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
	
	string $lfHeelPeelGrp = `group -em -n ("lfHeelPeelGrp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfHeelPeelGrp`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[3] $lfHeelPeelGrp`;
	makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
	
	string $lfToePivotGrp = `group -em -n ("lfToePivotGrp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[4] $lfToePivotGrp`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[4] $lfToePivotGrp`;
	makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
	
	string $lfHeelPivotGrp = `group -em -n ("lfHeelPivotGrp")`;
	delete `pointConstraint -o 0 0 0 -skip y -w 1 $lfJoints[2] $lfHeelPivotGrp`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfHeelPivotGrp`;
	makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
	
	string $lfIkRollGrp = `group -em -n ("lfLegIkHandlesMasterGrp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfIkRollGrp`;
	delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[2] $lfIkRollGrp`;
	makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
	

	parent $lfToeIkRf1 $lfToeTapGrp;
	parent $lfBallIkRf1 $lfAnkleIkRf1 $lfHeelPeelGrp;
	parent $lfToeTapGrp $lfHeelPeelGrp $lfToePivotGrp;
	parent $lfToePivotGrp $lfHeelPivotGrp;
	parent $lfHeelPivotGrp $lfIkRollGrp;
	
	//------------------
	//Foot Roll Controls
	//------------------
	jbFootCtrls();
	string $lfFootCtrl = `rename "footNode" "lfFootCtrl"`;
	delete `pointConstraint -o 0 0 0 -skip y -w 1 $lfJoints[2] $lfFootCtrl`;
	select -r $lfFootCtrl;
	string $lfFootCtrlRf[] = `duplicate -rr -n ($lfFootCtrl+"Grp")`;
	pickWalk -d down;
	doDelete;
	select -r $lfFootCtrlRf[0];
	pickWalk -d down;
	doDelete;
	select -r $lfFootCtrlRf[0];
	string $lfFootCtrlRf1[] = `duplicate -rr -n ($lfFootCtrl+"MasterGrp")`;
	parent $lfFootCtrl $lfFootCtrlRf[0];
	parent $lfFootCtrlRf[0] $lfFootCtrlRf1[0];
	
	//---------------------------------------------
	//Creation of right foot roll control and fixes
	//---------------------------------------------
	select -r $lfFootCtrl;
	string $rtFootCtrl[] = `duplicate -rr -n "rtFootCtrl"`;
	string $dupGrp = `group -em -n "rtDupGrp"`;
	parent $rtFootCtrl[0] $dupGrp;
	setAttr ($dupGrp+".scaleX") -1;
	parent -w $rtFootCtrl[0];
	select -r $dupGrp;
	doDelete;
	select -r $rtFootCtrl[0];
	string $rtFootCtrlRf[] = `duplicate -rr -n ($rtFootCtrl[0]+"Grp")`;
	pickWalk -d down;
	doDelete;
	select -r $rtFootCtrlRf[0];
	pickWalk -d down;
	doDelete;
	select -r $rtFootCtrlRf[0];
	string $rtFootCtrlRf1[] = `duplicate -rr -n ($rtFootCtrl[0]+"MasterGrp")`;
	parent $rtFootCtrl[0] $rtFootCtrlRf[0];
	parent $rtFootCtrlRf[0] $rtFootCtrlRf1[0];
	
	//-----------------------------------------------------------
	//Adding attributes to the foot roll control for the left leg
	//-----------------------------------------------------------
	select -r $lfFootCtrl;
	addAttr -k 1 -ln "ikBlend"  -at double  -min 0 -max 1 -dv 0;
	addAttr -k 1 -ln "toeTap"  -at double -dv 0;
	addAttr -k 1 -ln "toePivot"  -at double -dv 0;
	addAttr -k 1 -ln "toeTwist"  -at double -dv 0;
	addAttr -k 1 -ln "toeBank"  -at double -dv 0;
	addAttr -k 1 -ln "heelPeel"  -at double -dv 0;
	addAttr -k 1 -ln "heelPivot"  -at double -dv 0;
	addAttr -k 1 -ln "heelTwist"  -at double -dv 0;
	addAttr -k 1 -ln "heelBank"  -at double -dv 0;
	
	//------------------------------------------------------------
	//Locking attributes on the foot roll control for the left leg
	//------------------------------------------------------------
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".tx");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".ty");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".tz");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".rx");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".ry");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".rz");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfFootCtrl+".v");
	
	//--------------------------------------------------------
	// Creating Constraints for the IK Handles of the left leg
	//--------------------------------------------------------
	string $lfIkHandleParent[] = `parentConstraint -mo -w 1 $lfFkAnkle[0] $lfIkAnkleCtrl $lfIkRollGrp`;
	string $lfFkFootRollParent[] = `parentConstraint -mo -w 1 $lfFkAnkle[0] $lfFootCtrlRf1[0]`;
	string $lfIkFootRollParent[] = `parentConstraint -mo -w 1 $lfIkAnkleCtrl $lfFootCtrlRf1[0]`;
	//string $lfIkIkHandleOrient[] = `orientConstraint -o 0 0 0 -w 1 $lfIkAnkleCtrl $lfAnkleIkRf1`;
	//string $lfIkIkHandlePoint[] = `pointConstraint -o 0 0 0 -w 1 $lfIkAnkleCtrl $lfAnkleIkRf1`;
	string $lfFkPVCons[] = `poleVectorConstraint -w 1 $legLfPVFkNode $lfAnkleIk[0]`;
	string $lfIkPVCons[] = `poleVectorConstraint -w 1 $lfPVIkCtrl[0] $lfAnkleIk[0]`;
	
	//----------------------------------------------------------------------
	//Connecting foot roll attributes to the IK roll groups for the left leg
	//----------------------------------------------------------------------
	connectAttr -f  ($lfFootCtrl+".toeTap") ($lfToeTapGrp+".rotateX");
	connectAttr -f  ($lfFootCtrl+".toePivot") ($lfToePivotGrp+".rotateX");
	connectAttr -f  ($lfFootCtrl+".toeTwist") ($lfToePivotGrp+".rotateY");
	connectAttr -f  ($lfFootCtrl+".toeBank") ($lfToePivotGrp+".rotateZ");
	connectAttr -f  ($lfFootCtrl+".heelPeel") ($lfHeelPeelGrp+".rotateX");
	connectAttr -f  ($lfFootCtrl+".heelPivot") ($lfHeelPivotGrp+".rotateX");
	connectAttr -f  ($lfFootCtrl+".heelTwist") ($lfHeelPivotGrp+".rotateY");
	connectAttr -f  ($lfFootCtrl+".heelBank") ($lfHeelPivotGrp+".rotateZ");
	
	//********************************
	//--------------------------------
	//Creation of right side leg parts
	//--------------------------------
	//********************************
	select -r rtLegGuideNodes;
	
	string $rtPosSel[] = `ls -sl`;
	print $rtPosSel;
	
	select -r rtLegPVGuideNodes;
	
	string $rtPvSel[] = `ls -sl`;
	
	// Querry of node postions on world space.
	
	float $rtThighJntPos[] = `xform -q -ws -t $rtPosSel[0]`;
	float $rtKneeJntPos[] = `xform -q -ws -t $rtPosSel[1]`;
	float $rtAnkleJntPos[] = `xform -q -ws -t $rtPosSel[2]`;
	float $rtBallJntPos[] = `xform -q -ws -t $rtPosSel[3]`;
	float $rtToeJntPos[] = `xform -q -ws -t $rtPosSel[4]`;
	float $rtPVCtrlPos[] = `xform -q -ws -t $rtPvSel[0]`;
	
	select -cl;
	
	//----------------------------------
	// Creating Fk controls for right leg
	//----------------------------------
	string $rtThighJnt = "rtThighJnt";
	select -hi $rtThighJnt;
	string $rtJoints[] = `ls -sl -type "joint"`;
	
	string $rtFkThigh[];
	string $rtFkKnee[];
	string $rtFkAnkle[];
	string $rtFkBall[];
	string $rtFkToe[];
	string $rtFkThighRf[];
	string $rtFkThighRf1[];
	string $rtFkKneeRf[];
	string $rtFkKneeRf1[];
	string $rtFkAnkleRf[];
	string $rtFkAnkleRf1[];
	string $rtFkBallRf1[];
	string $rtFkToeRf1[];
	
	//--------------------------------
	//Create controls for the right leg
	//--------------------------------
	for($i=0;$i<(size($rtJoints)-1);$i++)
	{
		if($i<1)
		{						
			$rtFkThigh = `circle -nr 0 1 0 -n ($rtJoints[0]+"FkCtrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $rtJoints[0] $rtFkThigh[0]`;
			delete `orientConstraint -o 0 0 0 -w 1 $rtJoints[0] $rtFkThigh[0]`;
			select -r $rtFkThigh[0];
			$rtFkThighRf = `duplicate -rr -n ($rtFkThigh[0]+"GRP")`;
			pickWalk -d down;
			doDelete;
			select -r $rtFkThigh[0];
			$rtFkThighRf1 = `duplicate -rr -n ($rtFkThigh[0]+"MasterGRP")`;
			pickWalk -d down;
			doDelete;
			parent $rtFkThigh[0] $rtFkThighRf[0];
			parent $rtFkThighRf[0] $rtFkThighRf1[0];	
			//select -r ($rtFkClav+".cv[0:18]");
			//move -r -os -wd 0 1.5 0;		
		}
		if($i<2)
		{			
			$rtFkKnee = `circle -nr 0 1 0 -n ($rtJoints[1]+"FkCtrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $rtJoints[1] $rtFkKnee[0]`;
			delete `orientConstraint -o 0 0 0 -w 1 $rtJoints[1] $rtFkKnee[0]`;
			select -r $rtFkKnee[0];
			$rtFkKneeRf = `duplicate -rr -n ($rtFkKnee[0]+"GRP")`;
			pickWalk -d down;
			doDelete;
