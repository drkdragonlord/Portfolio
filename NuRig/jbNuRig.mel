	//string $rtCalf = `distanceDimension -sp $rtThighPos[0] $rtThighPos[1] $rtThighPos[2] -ep $rtKneePos[0] $rtKneePos[1] $rtKneePos[2]`;
	//float $rtCalfDist = `getAttr ($rtCalf+".distance")`;
	//setAttr ($rtCalfSurf[0]+".scaleY") $rtCalfDist;
