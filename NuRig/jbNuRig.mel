	orientConstraint -o 0 0 0 -w 1 $rtTipCalfBendCtrl[0] $rtCalfTipBendyJnt;
	select -cl;	
	
	//----------------------------
	//Creating Ulna Twist Behavior
	//----------------------------
	
	string $lfUlnaTwistMDN = `shadingNode -asUtility multiplyDivide -n "lfUlnaTwistMDN"`;
	setAttr ($lfUlnaTwistMDN+".operation") 2; // Divide Operation
	setAttr ($lfUlnaTwistMDN+".input2X") 5; 
	setAttr ($lfUlnaTwistMDN+".input2Y") 10;
	setAttr ($lfUlnaTwistMDN+".input2Z") 20; 
	connectAttr -f ($lfAnkle+".rotateY") ($lfUlnaTwistMDN+".input1X");
	connectAttr -f ($lfAnkle+".rotateY") ($lfUlnaTwistMDN+".input1Y");
	connectAttr -f ($lfAnkle+".rotateY") ($lfUlnaTwistMDN+".input1Z");
	
	connectAttr -f ($lfUlnaTwistMDN+".outputZ") ($lfBaseCalfBendCtrlRf[0]+".rotateZ");
	connectAttr -f ($lfUlnaTwistMDN+".outputY") ($lfMidCalfBendCtrlRf[0]+".rotateZ");
	connectAttr -f ($lfUlnaTwistMDN+".outputX") ($lfTipCalfBendCtrlRf[0]+".rotateZ");
	
