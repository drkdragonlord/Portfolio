	for($i=0;$i<2;$i++)
	{
		if($i<1)
		{
			$neckBone = `curve -d 1 -p 0 ($i+22) 0 -p 0 ($i+23) 0 -k 0 -k 1 -n ("neckBone0"+($i+1))`;	
		}
		else if($i<2)
		{
			$neckBone = `curve -d 1 -p 0 ($i+22) 0 -p 0 ($i+23) 0 -k 0 -k 1 -n ("neckBone0"+($i+1))`;	
		}
		select -r ($neckBone+".cv[0]");
		string $clstrBase[] = `cluster -n ($neckBone+"Base")`;
		select -r ($neckBone+".cv[1]");
		string $clstrTip[] = `cluster -n ($neckBone+"Tip")`;
		$baseClstrs[$i] = $clstrBase[1];
		$tipClstrs[$i] = $clstrTip[1];
		$neckBones[$i] = $neckBone;
	}
	
	//parenting procedures
	for($i=0;$i<3;$i++)
	{
		parent $mainNeckNodes[$i] $neckNullsGrp[$i];
		//parent -a $baseClstrs[1] $mainNeckNodes[1];
		if($i<1)
		{
			parent -a $baseClstrs[$i] $mainNeckNodes[$i];
		}
		if($i<2)
		{
			parent $neckNullsGrp[$i+1] $mainNeckNodes[$i];	
			parent -a $tipClstrs[$i] $mainNeckNodes[$i+1];		
		}				
	}
	
	parent -a $baseClstrs[1] $mainNeckNodes[1];
	
	//visibility procedures
	setAttr ($tipClstrs[0]+".visibility") 0;
	setAttr ($tipClstrs[1]+".visibility") 0;
	setAttr ($baseClstrs[0]+".visibility") 0;
	setAttr ($baseClstrs[1]+".visibility") 0;
	setAttr ($neckBones[0]+".overrideEnabled") 1;
	setAttr ($neckBones[0]+".overrideDisplayType") 2;	
	setAttr ($neckBones[1]+".overrideEnabled") 1;
	setAttr ($neckBones[1]+".overrideDisplayType") 2;	
	
	//making a group for the neck bones
	select -r $neckBones[0] $neckBones[1];
	string $neckBoneGrp = `group -n "neckBoneGrp"`;
	
	//Looking for spine guides	
	//variables to use within the search loop
	string $neckConnectBone;
	string $neckConnectBaseClstr[];
	string $neckConnectTipClstr[];
	
	if(`objExists spineNode09`)
	{
		$neckConnectBone = `curve -d 1 -p 0 21 0 -p 0 22 0 -k 0 -k 1 -n ("neckConnectBone")`;	
		select -r ($neckConnectBone+".cv[0]");
		$neckConnectBaseClstr = `cluster -n ($neckConnectBone+"Base")`;
		select -r ($neckConnectBone+".cv[1]");
		$neckConnectTipClstr = `cluster -n ($neckConnectBone+"Tip")`;
		parent -a $neckConnectBaseClstr[1] spineNode09;
		parent -a $neckConnectTipClstr[1] $mainNeckNodes[0];
		parent $neckNullsGrp[0] spineNode09;
		
		setAttr ($neckConnectTipClstr[1]+".visibility") 0;
		setAttr ($neckConnectBaseClstr[1]+".visibility") 0;
		setAttr ($neckConnectBone+".overrideEnabled") 1;
		setAttr ($neckConnectBone+".overrideDisplayType") 2;	
		setAttr ($neckConnectBone+".overrideEnabled") 1;
		setAttr ($neckConnectBone+".overrideDisplayType") 2;	
		
		parent $neckConnectBone $neckBoneGrp;
		
		select -r $neckBones[0] $neckBones[1] $neckConnectBone;
		sets -n neckBonesSet;
	
		select -r $baseClstrs[0] $baseClstrs[1] $tipClstrs[0] $tipClstrs[1] $neckConnectBaseClstr[1] $neckConnectTipClstr[1];
		sets -n neckClstrSet;
	}
	else
	{
		select -r $neckBones[0] $neckBones[1];
		sets -n neckBonesSet;
	
		select -r $baseClstrs[0] $baseClstrs[1] $tipClstrs[0] $tipClstrs[1];
		sets -n neckClstrSet;
	}
	
	select -r $mainNeckNodes;
	sets -n neckNodesSet;
	
	select -r $neckBoneGrp $neckNullsGrp;
	sets -n neckGrpsSet;
	
	select -r -ne neckNodesSet neckGrpsSet neckBonesSet neckClstrSet;
	sets -n neckGuides;
	
	//locking unecessary attributes
	select -r $mainNeckNodes;
	for($i=0; $i<size($mainNeckNodes);$i++)
	{		
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".rx");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".ry");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".rz");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".sx");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".sy");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".sz");
		setAttr -lock true -keyable false -channelBox false ($mainNeckNodes[$i]+".v");
	}
	select -cl;
	
}//End of global proc jbCreateNeckGuides(END)

global proc jbCreateLegGuides()
