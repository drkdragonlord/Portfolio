	sets -n legJointsSet;
	
	select -r $pelvicJnt $rootJnt;
	sets -n cogJointsSet;
	
	select -r $lfPVIkCtrlRf[0] $rtPVIkCtrlRf[0] $pelvicCtrlRf[0] $pelvicCtrlRf1[0] $rootCtrlRf $rootCtrlRf1[0] $rtToeTapGrp $rtToePivotGrp $rtHeelPeelGrp $rtHeelPivotGrp $rtIkRollGrp $lfToeTapGrp $lfToePivotGrp $lfHeelPeelGrp $lfHeelPivotGrp $lfIkRollGrp $rtAnkleIkRf2 $rtAnkleIkRf1 $rtBallIkRf2 $rtBallIkRf1 $rtToeIkRf2 $rtToeIkRf1 $lfAnkleIkRf2 $lfAnkleIkRf1 $lfBallIkRf2 $lfBallIkRf1 $lfToeIkRf2 $lfToeIkRf1 $rtFootCtrlRf[0] $rtFootCtrlRf1[0] $lfFootCtrlRf[0] $lfFootCtrlRf1[0] $rtIkAnkleRf[0] $rtIkAnkleRf1[0] $lfIkAnkleRf[0] $lfIkAnkleRf1[0] $rtFkThighRf[0] $rtFkThighRf1[0] $rtFkKneeRf[0] $rtFkKneeRf1[0] $rtFkAnkleRf[0] $rtFkAnkleRf1[0] $lfFkThighRf[0] $lfFkThighRf1[0] $lfFkKneeRf[0] $lfFkKneeRf1[0] $lfFkAnkleRf[0] $lfFkAnkleRf1[0] $lfPVFkCtrlRf[0] $rtPVFkCtrlRf[0] ;
	sets -n legGroupsSet;
	
	select -r $lfIkHandleParent[0] $lfFkFootRollParent[0] $lfIkFootRollParent[0] $lfFkPVCons[0] $lfIkPVCons[0] $rtIkHandleParent[0] $rtFkFootRollParent[0] $rtIkFootRollParent[0] $rtFkPVCons[0] $rtIkPVCons[0] $rootPntConst[0] $rootOrtConst[0] $pelvicPntConst[0] $pelvicOrtConst[0];
	sets -n legConstraintsSet;
	
	select -r $lfAnkleIk[0] $lfBallIk[0] $lfToeIk[0] $rtAnkleIk[0] $rtBallIk[0] $rtToeIk[0];
	sets -n legIkHandlesSet;
	
	select -r $lfAnkleIk[1] $lfBallIk[1] $lfToeIk[1] $rtAnkleIk[1] $rtBallIk[1] $rtToeIk[1];
	sets -n legIkEffectorsSet;	
	
	select -r $pelvicCtrl $rootCtrl $lfFkThigh[0] $lfFkKnee[0] $lfFkAnkle[0] $lfFootCtrl $lfPVIkCtrl[0] $lfIkAnkleCtrl $rtFkThigh[0] $rtFkKnee[0] $rtFkAnkle[0] $rtFootCtrl[0] $rtPVIkCtrl[0] $rtIkAnkleCtrl $legLfPVFkNode $legRtPVFkNode;
	sets -n legControlsSet;
	
	select -r -ne legJointsSet cogJointsSet legGroupsSet legConstraintsSet legIkHandlesSet legIkEffectorsSet legControlsSet legBendyComponents legStretchComponents legSwitchPartsSet;
	sets -n legComponents;
	
	select -r ($rootCtrl+"Shape.cv[0:7]") ($rootCtrl+"Shape1.cv[0:6]") ($rootCtrl+"Shape2.cv[0:6]") ($rootCtrl+"Shape3.cv[0:6]") ($rootCtrl+"Shape4.cv[0:6]") ($rootCtrl+"Shape5.cv[0:7]") ;
	scale -r -p 0cm 12cm 0cm 3.5 3.5 3.5 ;
	
	//Run procedures for cleanup
	jbFinalizeSetup();
	
}//End of global proc jbCreateNewLegs(END)

global proc jbBuildLegBendy(string $lfThigh, string $lfKnee, string $lfAnkle, string $rtThigh, string $rtKnee, string $rtAnkle)
{
	clearCache -all;
	float $lfThighPos[] = `xform -q -ws -t $lfThigh`;
	float $lfKneePos[] = `xform -q -ws -t $lfKnee`;
	float $lfAnklePos[] = `xform -q -ws -t $lfAnkle`;
	
	string $lfUpperLegSurf[] = `nurbsPlane -p 0 0 0 -ax 1 0 0 -w 1 -lr 1 -d 1 -u 1 -v 1 -ch 1 -n "lfUpperLegBendSurf"`;
	move -r 0 0.5 0 ;
	move 0 0 0 ($lfUpperLegSurf[0]+".scalePivot") ($lfUpperLegSurf[0]+".rotatePivot") ;
	setAttr ($lfUpperLegSurf[0]+".visibility") 0;
		
	//string $lfUpperLeg = `distanceDimension -sp $lfThighPos[0] $lfThighPos[1] $lfThighPos[2] -ep $lfKneePos[0] $lfKneePos[1] $lfKneePos[2]`;
	//float $lfUpperLegDist = `getAttr ($lfUpperLeg+".distance")`;
	//setAttr ($lfUpperLegSurf[0]+".scaleY") $lfUpperLegDist;
	delete`pointConstraint -o 0 0 0 -w 1 $lfThigh $lfUpperLegSurf[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfThigh $lfUpperLegSurf[0]`;
	
	select -r ($lfUpperLegSurf[0]+".cv[0:3][0]");
	string $lfUpperLegBaseCl[] = `cluster -n "lfUpperLegBaseCluster"`;
	setAttr ($lfUpperLegBaseCl[1]+".visibility") 0;

	select -r ($lfUpperLegSurf[0]+".cv[0:3][3]");
	string $lfUpperLegTipCl[] = `cluster -n "lfUpperLegTipCluster"`;
	setAttr ($lfUpperLegTipCl[1]+".visibility") 0;
	
	pointConstraint -o 0 0 0 -w 1 $lfThigh $lfUpperLegBaseCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfThigh $lfUpperLegBaseCl[1];
	
	pointConstraint -o 0 0 0 -w 1 $lfKnee $lfUpperLegTipCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfKnee $lfUpperLegTipCl[1];
	
	//--------------------
	//Creation of Locators
	//--------------------
	
	select -r ($lfUpperLegSurf[0]+".uv[0.5][0.25]");
	jbRivet();
	string $lfUpperLegBaseLoc = `rename "lfUpperLegBaseLoc"`;
	setAttr ($lfUpperLegBaseLoc+".visibility") 0;
	select -r ($lfUpperLegSurf[0]+".uv[0.5][0.5]");
	jbRivet();
	string $lfUpperLegMidLoc = `rename "lfUpperLegMidLoc"`;
	setAttr ($lfUpperLegMidLoc+".visibility") 0;
	select -r ($lfUpperLegSurf[0]+".uv[0.5][0.75]");
	jbRivet();
	string $lfUpperLegTipLoc = `rename "lfUpperLegTipLoc"`;
	setAttr ($lfUpperLegTipLoc+".visibility") 0;
	
	//------------------------------------
	//Creation of Upper Leg Bendy Controls
	//------------------------------------
	string $lfBaseLegBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfBaseLegBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfBaseLegBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfBaseLegBendCtrl[0]`;
	select -r $lfBaseLegBendCtrl[0];
	string $lfBaseLegBendCtrlRf[] = `duplicate -rr -n ($lfBaseLegBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfBaseLegBendCtrl;
	string $lfBaseLegBendCtrlRf1[] = `duplicate -rr -n ($lfBaseLegBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfBaseLegBendCtrl[0] $lfBaseLegBendCtrlRf[0];
	parent $lfBaseLegBendCtrlRf[0] $lfBaseLegBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfBaseLegBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfBaseLegBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfBaseLegBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfBaseLegBendCtrl[0]+".v");
	
	string $lfMidLegBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfMidLegBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfMidLegBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfMidLegBendCtrl[0]`;
	select -r $lfMidLegBendCtrl[0];
	string $lfMidLegBendCtrlRf[] = `duplicate -rr -n ($lfMidLegBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfMidLegBendCtrl;
	string $lfMidLegBendCtrlRf1[] = `duplicate -rr -n ($lfMidLegBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfMidLegBendCtrl[0] $lfMidLegBendCtrlRf[0];
	parent $lfMidLegBendCtrlRf[0] $lfMidLegBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfMidLegBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfMidLegBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfMidLegBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfMidLegBendCtrl[0]+".v");
	
	string $lfTipLegBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfTipLegBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfTipLegBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfTipLegBendCtrl[0]`;
	select -r $lfTipLegBendCtrl[0];
	string $lfTipLegBendCtrlRf[] = `duplicate -rr -n ($lfTipLegBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfTipLegBendCtrl;
	string $lfTipLegBendCtrlRf1[] = `duplicate -rr -n ($lfTipLegBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfTipLegBendCtrl[0] $lfTipLegBendCtrlRf[0];
	parent $lfTipLegBendCtrlRf[0] $lfTipLegBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfTipLegBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfTipLegBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfTipLegBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfTipLegBendCtrl[0]+".v");
	
	//------------------------------------
	//Constraints of Control Master Groups
	//------------------------------------
	pointConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfBaseLegBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfBaseLegBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfMidLegBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfMidLegBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfTipLegBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfTipLegBendCtrlRf1[0];
	
	//------------------------
	//Creation of Bendy Joints
	//------------------------
	select -cl;
	string $lfLegBaseBendyJnt = `joint -n "lfLegBaseBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfLegBaseBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegBaseLoc $lfLegBaseBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfBaseLegBendCtrl[0] $lfLegBaseBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfBaseLegBendCtrl[0] $lfLegBaseBendyJnt;
	
	select -cl;
	string $lfLegMidBendyJnt = `joint -n "lfLegMidBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfLegMidBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegMidLoc $lfLegMidBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfMidLegBendCtrl[0] $lfLegMidBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfMidLegBendCtrl[0] $lfLegMidBendyJnt;
	
	select -cl;
	string $lfLegTipBendyJnt = `joint -n "lfLegTipBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfLegTipBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperLegTipLoc $lfLegTipBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfTipLegBendCtrl[0] $lfLegTipBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfTipLegBendCtrl[0] $lfLegTipBendyJnt;
	
	//--------------
	//Calf Bendys
	//--------------
	string $lfCalfSurf[] = `nurbsPlane -p 0 0 0 -ax 1 0 0 -w 1 -lr 1 -d 1 -u 1 -v 1 -ch 1 -n "lfCalfBendSurf"`;
	move -r 0 0.5 0 ;
	move 0 0 0 ($lfCalfSurf[0]+".scalePivot") ($lfCalfSurf[0]+".rotatePivot") ;
	setAttr ($lfCalfSurf[0]+".visibility") 0;
		
	//string $lfCalf = `distanceDimension -sp $lfThighPos[0] $lfThighPos[1] $lfThighPos[2] -ep $lfKneePos[0] $lfKneePos[1] $lfKneePos[2]`;
	//float $lfCalfDist = `getAttr ($lfCalf+".distance")`;
	//setAttr ($lfCalfSurf[0]+".scaleY") $lfCalfDist;
	delete`pointConstraint -o 0 0 0 -w 1 $lfKnee $lfCalfSurf[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfKnee $lfCalfSurf[0]`;
	
	select -r ($lfCalfSurf[0]+".cv[0:3][0]");
	string $lfCalfBaseCl[] = `cluster -n "lfCalfBaseCluster"`;
	setAttr ($lfCalfBaseCl[1]+".visibility") 0;

	select -r ($lfCalfSurf[0]+".cv[0:3][3]");
	string $lfCalfTipCl[] = `cluster -n "lfCalfTipCluster"`;
	setAttr ($lfCalfTipCl[1]+".visibility") 0;
	
	pointConstraint -o 0 0 0 -w 1 $lfKnee $lfCalfBaseCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfKnee $lfCalfBaseCl[1];
	
	pointConstraint -o 0 0 0 -w 1 $lfAnkle $lfCalfTipCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfAnkle $lfCalfTipCl[1];
	
	//--------------------
	//Creation of Locators
	//--------------------	
	select -r ($lfCalfSurf[0]+".uv[0.5][0.25]");
	jbRivet();
	string $lfCalfBaseLoc = `rename "lfCalfBaseLoc"`;
	setAttr ($lfCalfBaseLoc+".visibility") 0;
	select -r ($lfCalfSurf[0]+".uv[0.5][0.5]");
	jbRivet();
	string $lfCalfMidLoc = `rename "lfCalfMidLoc"`;
	setAttr ($lfCalfMidLoc+".visibility") 0;
	select -r ($lfCalfSurf[0]+".uv[0.5][0.75]");
	jbRivet();
	string $lfCalfTipLoc = `rename "lfCalfTipLoc"`;
	setAttr ($lfCalfTipLoc+".visibility") 0;
	
	//------------------------------------
	//Creation of Calf Bendy Controls
	//------------------------------------
	string $lfBaseCalfBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfBaseCalfBendCtrl"`;
