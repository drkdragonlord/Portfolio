	parent $rtTipCalfBendCtrl[0] $rtTipCalfBendCtrlRf[0];
	parent $rtTipCalfBendCtrlRf[0] $rtTipCalfBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($rtTipCalfBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($rtTipCalfBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($rtTipCalfBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($rtTipCalfBendCtrl[0]+".v");
	
	//------------------------------------
	//Constraints of Control Master Groups
	//------------------------------------
	pointConstraint -o 0 0 0 -w 1 $rtCalfBaseLoc $rtBaseCalfBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $rtCalfBaseLoc $rtBaseCalfBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $rtCalfMidLoc $rtMidCalfBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $rtCalfMidLoc $rtMidCalfBendCtrlRf1[0];
	
