		delete`orientConstraint -o 0 0 0 -w 1 $mainNeckNodes[$i] $neckNulls`;		
		delete`pointConstraint -o 0 0 0 -w 1 $mainNeckNodes[$i] $neckNulls`;
		$neckNullsGrp[$i] = $neckNulls;
	}
	
	//creates variables for us to use in the following for loop
	string $neckBone; 
