	delete`pointConstraint -o 0 0 0 -w 1 $rtCalfTipLoc $rtTipCalfBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $rtCalfTipLoc $rtTipCalfBendCtrl[0]`;
	select -r $rtTipCalfBendCtrl[0];
	string $rtTipCalfBendCtrlRf[] = `duplicate -rr -n ($rtTipCalfBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtTipCalfBendCtrl;
	string $rtTipCalfBendCtrlRf1[] = `duplicate -rr -n ($rtTipCalfBendCtrl[0]+"MasterGRP")`;
