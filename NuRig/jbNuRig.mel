global proc jbCheckForLegGuides()
{
	if(`objExists armGuides `)
	{
		if(`objExists spineGuides`)
		{
			if(`objExists legGuides`)
			{	
				jbCreateNewArms();
				jbCreateNewSpine();			
				jbCreateNewLegs();
			}
			else
			{
				error ("No leg guides found. Please create leg guides first.");
			}	
		}
		if(`objExists legGuides`)
		{
			jbCreateNewArms();
			jbCreateNewLegs();
		}
		else
		{
			error ("No leg guides found. Please create leg guides first.");
		}	
	}
	else if(`objExists legGuides`)
	{
		jbCreateNewLegs();	
	}
	else
	{
		error ("No leg guides found. Please create leg guides first.");
	}
}//End of global proc jbCheckForLegGuides(END)

global proc jbCheckForArmGuides()
{
	if(`objExists armGuides`)
	{
		if(`objExists spineGuides`)
		{
			jbCreateNewArms();
			jbCreateNewSpine();
		}
		else
		{
			error ("No spine guides found! Please create spine guides first.");
		}
	}
	else if(`objExists spineGuides`)
	{
		jbCreateNewSpine();	
	}
	else
	{
		error ("No spine guides found! Please create spine guides first.");
	}

}//End of global proc jbCheckForArmGuides(END)

global proc jbCreateNewSpine()
{	
	//selects a set in the helpers scene, that holds our guide nodes, and creates an array of it for us to use
	select -r spineGuideNodes;
	string $spinePosSel[] = `ls -sl`;	
	select -cl;
	
	//creates a character main control and adds the character scale attribute, and the spineStretch attribute
	jbRootCtrls();
	string $mainCtrl = `rename cogNode "characterMainCtrl"`;
	select -r ($mainCtrl+"Shape.cv[0:7]") ($mainCtrl+"Shape1.cv[0:6]") ($mainCtrl+"Shape2.cv[0:6]") ($mainCtrl+"Shape3.cv[0:6]") ($mainCtrl+"Shape4.cv[0:6]") ($mainCtrl+"Shape5.cv[0:7]") ;
	scale -r -p 0cm 0cm 0cm 7.5 7.5 7.5 ;
	setAttr ($mainCtrl+"Shape.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape.overrideColor") 16;
	setAttr ($mainCtrl+"Shape1.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape1.overrideColor") 16;
	setAttr ($mainCtrl+"Shape2.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape2.overrideColor") 16;
	setAttr ($mainCtrl+"Shape3.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape3.overrideColor") 16;
	setAttr ($mainCtrl+"Shape4.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape4.overrideColor") 16;
	setAttr ($mainCtrl+"Shape5.overrideEnabled") 1;
	setAttr ($mainCtrl+"Shape5.overrideColor") 16;
	setAttr -lock true -keyable false ($mainCtrl+".v");
	select -cl;
	
	string $mainCtrlRf = `group -em -n ($mainCtrl+"Grp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $mainCtrl $mainCtrlRf`;
	delete `orientConstraint -o 0 0 0 -w 1 $mainCtrl $mainCtrlRf`;
	parent $mainCtrl $mainCtrlRf;
	addAttr -ln "characterSize" -k 1 -at double -min 0.4 -dv 1 $mainCtrl;
	addAttr -ln "spineStretch" -k 1 -at double -min 0 -max 1 -dv 1 $mainCtrl;
	connectAttr -f ($mainCtrl+".characterSize") ($mainCtrl+".sx");
	connectAttr -f ($mainCtrl+".characterSize") ($mainCtrl+".sy");
	connectAttr -f ($mainCtrl+".characterSize") ($mainCtrl+".sz");
	setAttr -lock true -keyable false ($mainCtrl+".sx");
	setAttr -lock true -keyable false ($mainCtrl+".sy");
	setAttr -lock true -keyable false ($mainCtrl+".sz");
	select -cl;	
	
	//creates an array to be filled within our joint creation loop
	string $spineArray[];
	
	//loop to create spine joints using the positions of our guides, but not the orientation
	for($i=0;$i<size($spinePosSel);$i++)
	{		
	float $spineJntPos[] = `xform -q -ws -t $spinePosSel[$i]`;
	string $spineJnts = `joint -p $spineJntPos[0] $spineJntPos[1] $spineJntPos[2] -rad 0.5 -n ("spineJnt"+0+($i+1))`;
	$spineArray[$i] = $spineJnts;
	}
	
	//Creates an array that contains the Base Spine Joint
	select $spineArray[0];
	string $baseSpineJnt[] = `ls -sl`;
	
	//orients all spine joints
	select $spineArray[0];
	joint -e -oj yzx -sao zup -ch -zso;
	
	//Fixes axis orientation of tip joint to that of previous joint.
	float $spineTipParentOrientation[]= `xform -q -ws -ro $spineArray[7]`;
	select -r ($spineArray[8]+".rotateAxis") ;
	rotate -r -os ($spineTipParentOrientation[0]*-1) 0 ($spineTipParentOrientation[2]*-1) ;
	
	//Creates the Spine Ik Handle and renames its parts
	string $spineIkParts[] = `ikHandle -sol ikSplineSolver -sj $spineArray[0] -ee $spineArray[8] -pcv false -ns 4`;
	
	select -cl;
	select -r $spineIkParts[0];
	string $spineHandle = `rename $spineIkParts[0] "spineIkHandle"`;
	
	select -cl;
	select -r $spineIkParts[1];
	string $spineEffector = `rename $spineIkParts[1] "spineIkEffector"`;		
	
	select -cl;
	select -r $spineIkParts[2];
	string $spineCrv = `rename $spineIkParts[2] "spineIkCurve"`;	

	// Creates and renames the clusters
	select -cl;
	select -r ($spineCrv+".cv[0:1]") ;
	string $loCluster[] = `cluster -en 1 -n "spineLoCluster"`;
	select -r ($spineCrv+".cv[2:4]") ;
	string $midCluster[] = `cluster -en 1 -n "spineMdCluster"`;
	select -r ($spineCrv+".cv[5:6]") ;
	string $hiCluster[] = `cluster -en 1 -n "spineHiCluster"`;

	//spine main control
	string $spineMainCtrl = `curve -d 1 -p -6 0 -1 -p -6 0 1 -p -1 0 1 -p -1 0 6 -p 1 0 6 -p 1 0 1 -p 6 0 1 -p 6 0 -1 -p 1 0 -1 -p 1 0 -6 -p -1 0 -6 -p -1 0 -1 -p -6 0 -1
	-k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -n "spineMainCtrl"`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[0] $spineMainCtrl`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[0] $spineMainCtrl`;
	string $spineMainCtrlRf = `group -em -n ($spineMainCtrl+"Grp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[0] $spineMainCtrlRf`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[0] $spineMainCtrlRf`;
	parent $spineMainCtrl $spineMainCtrlRf;	
	select -r $spineMainCtrl;
	pickWalk -d down;
	string $spineMainCtrlShape[] = `ls -sl`;
	setAttr ($spineMainCtrlShape[0]+".overrideEnabled") 1;
	setAttr ($spineMainCtrlShape[0]+".overrideColor") 14;
	select -cl;	
	
	//Locks and Hides attributes not used on the Spine Main Control
	setAttr -lock true -keyable false ($spineMainCtrl+".sx");
	setAttr -lock true -keyable false ($spineMainCtrl+".sy");
	setAttr -lock true -keyable false ($spineMainCtrl+".sz");
	setAttr -lock true -keyable false ($spineMainCtrl+".v");
	
	//Creates the Spine Base Control and parents the tip cluster to it
	string $spineBaseCtrl[] = `circle -r 5 -nr 0 1 0 -n "spineLoCtrl"`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[0] $spineBaseCtrl[0]`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[0] $spineBaseCtrl[0]`;
	string $spineBaseCtrlRf = `group -em -n ($spineBaseCtrl[0]+"Grp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[0] $spineBaseCtrlRf`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[0] $spineBaseCtrlRf`;
	parent $spineBaseCtrl[0] $spineBaseCtrlRf;	
	parent $loCluster[1] $spineBaseCtrl[0] ;
	setAttr ($spineBaseCtrl[0]+"Shape.overrideEnabled") 1;
	setAttr ($spineBaseCtrl[0]+"Shape.overrideColor") 14;
	
	//Places the Pivot of the Spine Base Control to the second joint of the spine chain
	float $spineBaseChildPos[] = `xform -q -ws -t $spineArray[1]`;
	move $spineBaseChildPos[0] $spineBaseChildPos[1] $spineBaseChildPos[2] ($spineBaseCtrl[0]+".scalePivot") ($spineBaseCtrl[0]+".rotatePivot") ;
	
	//select -r ($spineBaseCtrl[0]+"Shape.cv[0:7]") ;
	//scale -r -p 0cm 0cm 0cm 7.0 7.0 7.0 ;

	//Locks and Hides attributes not used on the Spine Base Control
	setAttr -lock true -keyable false ($spineBaseCtrl[0]+".sx");
	setAttr -lock true -keyable false ($spineBaseCtrl[0]+".sy");
	setAttr -lock true -keyable false ($spineBaseCtrl[0]+".sz");
	setAttr -lock true -keyable false ($spineBaseCtrl[0]+".v");

	//Creates the Spine Tip Control and parents the tip cluster to it
	string $spineTipCtrl[] = `circle -r 5 -nr 0 1 0 -n "spineHiCtrl"`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[8] $spineTipCtrl[0]`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[8] $spineTipCtrl[0]`;
	string $spineTipCtrlRf = `group -em -n ($spineTipCtrl[0]+"Grp")`;
	delete `pointConstraint -o 0 0 0 -w 1 $spineArray[8] $spineTipCtrlRf`;
	delete `orientConstraint -o 0 0 0 -w 1 $spineArray[8] $spineTipCtrlRf`;
	parent $spineTipCtrl[0] $spineTipCtrlRf;	
	parent $hiCluster[1] $spineTipCtrl[0] ;
	setAttr ($spineTipCtrl[0]+"Shape.overrideEnabled") 1;
	setAttr ($spineTipCtrl[0]+"Shape.overrideColor") 14;
	
	//select -r ($spineTipCtrl[0]+"Shape.cv[0:7]") ;
	//scale -r -p 0cm 24cm 0cm 7.0 7.0 7.0 ;
	
	//Places the Pivot of the Spine Tip Control to the center joint of the spine chain
	float $spineMidPos[] = `xform -q -ws -t $spineArray[5]`;
	move $spineMidPos[0] $spineMidPos[1] $spineMidPos[2] ($spineTipCtrl[0]+".scalePivot") ($spineTipCtrl[0]+".rotatePivot") ;
	
	//Locks and Hides attributes not used on the Spine Tip Control
	setAttr -lock true -keyable false ($spineTipCtrl[0]+".sx");
	setAttr -lock true -keyable false ($spineTipCtrl[0]+".sy");
	setAttr -lock true -keyable false ($spineTipCtrl[0]+".sz");
	setAttr -lock true -keyable false ($spineTipCtrl[0]+".v");
	
	//Constraint for the Mid Spine Cluster
	string $midClstrConst[] = `parentConstraint -mo -weight 1 $spineBaseCtrl[0] $spineTipCtrl[0] $midCluster[1]`;
	
	//Connects the Ik Spline Handle Twist attribute to the SpineTipCtrl rotate Y
	connectAttr -f ($spineTipCtrl[0]+".rotateY") ($spineHandle+".twist");

	//Hides the cluster handles
	select -cl;
	select -add $loCluster[1] $midCluster[1] $hiCluster[1];
	HideSelectedObjects;
	
	//select -r ($spineTipCtrl[0]+"Shape.cv[0:7]");
	//move -r $spineTipPos[0] $spineTipPos[1] $spineTipPos[2] ;
	
	//Creates the RIG_DNT group and parents to it the necessary nodes
	select -cl;
	select -add $midCluster[1] $spineHandle $spineCrv;
	string $rigGrp = `group -n "RIG_DNT"`;
	select -r $rigGrp;
	HideSelectedObjects;
	
	//Creates some variables to be used later in the creation of controls inside loop statements
	string $mainSpineCtrl;
	string $mainSpineCtrlRf;
	
	//creates and renames the curve info node on the spine curve
	string $crvInfo = `arclen -ch 1 $spineCrv`;
	string $spineCrvInfo = `rename $crvInfo "spineCrvInfoNode"`;
	
	//creates the world scale Multiply Divide node
	string $mainMDN = `shadingNode -asUtility -n "worldScaleMDN" multiplyDivide`;
	setAttr ($mainMDN+".operation") 2;
	
	//connects the world size attribute of the main control to the world scale MDN
	connectAttr -f ($spineCrvInfo+".arcLength") ($mainMDN+".input1X");
	connectAttr -f ($mainCtrl+".characterSize") ($mainMDN+".input2X");
	
	//gets the default value of the spine curve and inputs it on the second attr of the spine scale MDN
	float $defaultCrvValue = `getAttr ($spineCrvInfo+".arcLength")`;
	
	//creates the spine scale MDN
	string $spineScaleMDN = `shadingNode -asUtility -n "spineLengthMDN" multiplyDivide`;
	
	//changes the operation of the spine scale MDN to division and connects it to the world scale MDN
	setAttr ($spineScaleMDN+".operation") 2;
	setAttr ($spineScaleMDN+".input2X") $defaultCrvValue;
	
	//Creates the Stretch Switch Nodes and its connections
	string $stretchSwitch = `shadingNode -asUtility -n "stretchSwitch" condition`;
	connectAttr -f ($mainMDN+".outputX") ($stretchSwitch+".colorIfTrueR");
	connectAttr -f ($mainCtrl+".spineStretch") ($stretchSwitch+".firstTerm");
	setAttr ($stretchSwitch+".operation") 0;
	setAttr ($stretchSwitch+".secondTerm") 1;
	setAttr ($stretchSwitch+".colorIfFalseR") $defaultCrvValue;
	connectAttr -f ($stretchSwitch+".outColorR") ($spineScaleMDN+".input1X");
	
	
	//selects the required stretchy joints and puts them in a new array
	select -cl;
	select -add $spineArray[1] $spineArray[2] $spineArray[3] $spineArray[4] $spineArray[5] $spineArray[6] $spineArray[7] $spineArray[8];
	string $stretchJnts[] = `ls -sl`;
	
	//creates the attributes for the wave compression on the tip spine control.
	addAttr -ln "singlewave" -k 1 -at double -min -1 -max 1 -dv 0 $spineTipCtrl[0];
	addAttr -ln "doublewave" -k 1 -at double -min -3 -max 3 -dv 0 $spineTipCtrl[0];
	
	//creates the singlewave MDN
	string $sWaveMDN = `shadingNode -asUtility -n "singlewaveMDN" multiplyDivide`;
	
	//creates the doublewave MDN's
	string $dWavePosMDN = `shadingNode -asUtility -n "positiveDoublewaveMDN" multiplyDivide`;
	string $dWaveNegMDN = `shadingNode -asUtility -n "negativeDoublewaveMDN" multiplyDivide`;
	
	//set operation to division
	setAttr ($dWaveNegMDN+".operation") 2;
	string $dWavePosCond = `shadingNode -asUtility -n "positiveDWCond" condition`;
	
	//set operation to less than
	setAttr ($dWavePosCond+".operation") 4;	
	string $dWaveNegCond = `shadingNode -asUtility -n "negativeDWCond" condition`;
	
	//set operation to greater than and set some attributes values
	setAttr ($dWaveNegCond+".operation") 2;
	setAttr ($dWaveNegCond+".colorIfTrueR") -3;
	setAttr ($dWaveNegCond+".colorIfFalseR") 3;
	string $dWaveMainCond = `shadingNode -asUtility -n "mainDoublewaveMDN" condition`;
	
	//set operation to greater than, set some attributes values, and connect the attributes
	setAttr ($dWaveMainCond+".operation") 2;						
	setAttr ($sWaveMDN+".input2X") -1;
	setAttr ($dWavePosMDN+".input2X") -1;
	connectAttr -f ($spineTipCtrl[0]+".singlewave") ($sWaveMDN+".input1X");
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWavePosMDN+".input1X");
	
	//Creates a set for all the Hypershade utility nodes used, including any constraints
	select -cl;
	select -add $mainMDN $spineScaleMDN $stretchSwitch $sWaveMDN $dWaveMainCond $dWavePosMDN $dWaveNegMDN $dWavePosCond $dWaveNegCond $midClstrConst;
	sets -n spineUtilities;
		
		//loop that creates the stretchy connections
		for($i=0;$i<size($stretchJnts);$i++)
		{
			//gets the value of the translate Y of the joints in the array.
			float $jntTYVal = `getAttr ($stretchJnts[$i]+".ty")`;
			//creates a new MDN for each joint with their respective names.
			string $jointMDN = `shadingNode -asUtility -n ($stretchJnts[$i]+"MDN") multiplyDivide`;	
			string $jointPMA = `shadingNode -asUtility -n ($stretchJnts[$i]+"PMA") plusMinusAverage`;	
			connectAttr -f ($jointPMA+".output1D") ($jointMDN+".input1X");			
			//sets the first attribute of each joints MDN to their default Translate Y position.
			setAttr ($jointMDN+".input2X") $jntTYVal;
			//connects the spine scale MDN to the joints MDN and the joints MDN to the joints Translate Y value.
			connectAttr -f ($spineScaleMDN+".outputX") ($jointPMA+".input1D[0]");
			connectAttr -f ($jointMDN+".outputX") ($stretchJnts[$i]+".ty");
			select -cl;
			select -add $jointMDN $jointPMA;
			sets -add  spineUtilities;
		}
		
	//singlewave connections
	connectAttr -f ($sWaveMDN+".outputX") ($stretchJnts[0]+"PMA"+".input1D[1]");
	connectAttr -f ($sWaveMDN+".outputX") ($stretchJnts[1]+"PMA"+".input1D[1]");
	connectAttr -f ($sWaveMDN+".outputX") ($stretchJnts[2]+"PMA"+".input1D[1]");
	connectAttr -f ($sWaveMDN+".outputX") ($stretchJnts[3]+"PMA"+".input1D[1]");
	connectAttr -f ($spineTipCtrl[0]+".singlewave")  ($stretchJnts[4]+"PMA"+".input1D[1]");
	connectAttr -f ($spineTipCtrl[0]+".singlewave")  ($stretchJnts[5]+"PMA"+".input1D[1]");
	connectAttr -f ($spineTipCtrl[0]+".singlewave")  ($stretchJnts[6]+"PMA"+".input1D[1]");	
	connectAttr -f ($spineTipCtrl[0]+".singlewave")  ($stretchJnts[7]+"PMA"+".input1D[1]");			
	
	//doublewave connections
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWavePosCond+".firstTerm");
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWavePosCond+".colorIfFalseR");
	connectAttr -f ($dWavePosMDN+".outputX") ($dWavePosCond+".colorIfTrueR");
	
	connectAttr -f ($dWavePosCond+".outColorR") ($dWaveMainCond+".colorIfTrueG");
	connectAttr -f ($dWavePosCond+".outColorR") ($dWaveMainCond+".colorIfFalseR");
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWaveMainCond+".firstTerm");
	
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWaveNegMDN+".input1X");
	connectAttr -f ($spineTipCtrl[0]+".doublewave") ($dWaveNegCond+".firstTerm");
	connectAttr -f ($dWaveNegCond+".outColorR") ($dWaveNegMDN+".input2X");
	connectAttr -f ($dWaveNegMDN+".outputX") ($dWaveMainCond+".colorIfTrueR");
	connectAttr -f ($dWaveNegMDN+".outputX") ($dWaveMainCond+".colorIfFalseG");
	
	connectAttr -f ($dWaveMainCond+".outColorR") ($stretchJnts[7]+"PMA"+".input1D[2]");
	connectAttr -f ($dWaveMainCond+".outColorR") ($stretchJnts[0]+"PMA"+".input1D[2]");
	
	connectAttr -f ($dWaveMainCond+".outColorG") ($stretchJnts[3]+"PMA"+".input1D[2]");
	connectAttr -f ($dWaveMainCond+".outColorG") ($stretchJnts[4]+"PMA"+".input1D[2]");
	
	connectAttr -f ($dWaveNegMDN+".outputX") ($stretchJnts[1]+"PMA"+".input1D[2]");
	connectAttr -f ($dWaveNegMDN+".outputX") ($stretchJnts[6]+"PMA"+".input1D[2]");
	
	connectAttr -f ($dWaveNegMDN+".outputX") ($stretchJnts[5]+"PMA"+".input1D[2]");
	connectAttr -f ($dWaveNegMDN+".outputX") ($stretchJnts[2]+"PMA"+".input1D[2]");
	
	//Parents the spine controls into its proper hierarchy
	parent $spineMainCtrlRf $mainCtrl;
	parent $spineBaseCtrlRf $spineMainCtrl;
	parent $spineTipCtrlRf $spineMainCtrl;
	
	//Creates sets for all the components of the spine and places them under one set for easy access
	select -cl;
	select -add $spineMainCtrl $spineTipCtrl[0] $spineBaseCtrl[0] $mainCtrl;
	sets -n "spineControls";
	
	select -cl;
	select -add $spineArray;
	sets -n "spineJoints";
	
	select -cl;
	select -add $spineCrv $spineHandle $spineEffector;
	sets -n "spineIkComponents";
	
	select -cl;
	select -add $spineTipCtrlRf $spineBaseCtrlRf $spineMainCtrlRf $mainCtrlRf $rigGrp;
	sets -n "spineGroups";
	
	select -cl;
	select -add $hiCluster[1] $loCluster[1] $midCluster[1];
	sets -n "spineClusters";
	
	select -cl;
	select -ne spineClusters spineGroups spineIkComponents spineJoints spineControls spineUtilities;
	sets -n "spineComponents";
	select -cl;
	
	if(`objExists neckGuides`)
	{
		jbCreateNewNeck();	
	}
	
	//Selects all the contents of the guide nodes and deletes them as they are no longer required
	select -cl;
	select -r spineGuides;
	doDelete;
	
	
}//End of global proc jbCreateNewSpine(END)	

global proc jbCreateNewArms()
{
	clearCache -all;
	
	select -r lfArmGuideNodes;
	
	string $lfPosSel[] = `ls -sl`;
	print $lfPosSel;
	
	select -r lfArmPVGuideNodes;
	
	string $lfPvSel[] = `ls -sl`;
	
	// Querry of node postions on world space.
	
	float $lfClavJntPos[] = `xform -q -ws -t $lfPosSel[0]`;
	float $lfShldJntPos[] = `xform -q -ws -t $lfPosSel[1]`;
	float $lfElbowJntPos[] = `xform -q -ws -t $lfPosSel[2]`;
	float $lfWristJntPos[] = `xform -q -ws -t $lfPosSel[3]`;
	float $lfHandJntPos[] = `xform -q -ws -t $lfPosSel[4]`;
	float $lfPVCtrlPos[] = `xform -q -ws -t $lfPvSel[0]`;
	
	select -cl;
	
	// Creates the arm joint chain based on the positions of the nodes moved by the user.
	
	string $lfClavJnt = `joint -p $lfClavJntPos[0] $lfClavJntPos[1] $lfClavJntPos[2] -rad 0.5 -n "lfClavJnt"`;
	string $lfShldJnt = `joint -p $lfShldJntPos[0] $lfShldJntPos[1] $lfShldJntPos[2] -rad 0.5 -n "lfShldJnt"`;
	string $lfElbowJnt = `joint -p $lfElbowJntPos[0] $lfElbowJntPos[1] $lfElbowJntPos[2] -rad 0.5 -n "lfElbowJnt"`;
	string $lfWristJnt = `joint -p $lfWristJntPos[0] $lfWristJntPos[1] $lfWristJntPos[2] -rad 0.5 -n "lfWristJnt"`;
	string $lfHandJnt = `joint -p $lfHandJntPos[0] $lfHandJntPos[1] $lfHandJntPos[2] -rad 0.5 -n "lfHandJnt"`;
	
	select $lfShldJnt;
	
	// Orients the lfJoints along the yzx axis.
	
	joint -e -oj yzx -sao zup -ch -zso;
	
	select -r ($lfHandJnt + ".rotateAxis");
	rotate -r -os 0 0 90 ;
	
	select $lfClavJnt;
	
	// This script assumes character symmetry as joints will be mirrored now from the left arm to the right.	
	mirrorJoint -mirrorYZ -mirrorBehavior -searchReplace "lf" "rt";
	
	string $rtClavJnt[] = `ls -sl`;	
	
	//
	// Creating Fk controls for left arm
	//
	select -hi $lfClavJnt;
	string $lfJoints[] = `ls -sl -type "joint"`;
	
	string $lfFkClav;
	string $lfFkShoulder[];
	string $lfFkElbow[];
	string $lfFkWrist[];
	string $lfFkClavRf[];
	string $lfFkClavRf1[];
	string $lfFkShoulderRf[];
	string $lfFkShoulderRf1[];
	string $lfFkElbowRf[];
	string $lfFkElbowRf1[];
	string $lfFkWristRf[];
	string $lfFkWristRf1[];
	
	for($i=0;$i<size($lfJoints);$i++)
	{
		if($i<1)
		{			
			jbClavCtrls();
			select -r clavCtrl;			
			$lfFkClav = `rename clavCtrl ($lfJoints[$i]+"Ctrl")`;
			delete `pointConstraint -o 0 0 0 -w 1 $lfJoints[0] $lfFkClav`;
			delete `orientConstraint -o 0 0 0 -w 1 $lfJoints[0] $lfFkClav`;
			select -r $lfFkClav;
			$lfFkClavRf = `duplicate -rr -n ($lfFkClav+"GRP")`;
			pickWalk -d down;
			doDelete;
			select -r $lfFkClav;
			$lfFkClavRf1 = `duplicate -rr -n ($lfFkClav+"MasterGRP")`;
			pickWalk -d down;
			doDelete;
			parent $lfFkClav $lfFkClavRf[0];
