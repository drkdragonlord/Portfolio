	orientConstraint -o 0 0 0 -w 1 $lfCalfBaseLoc $lfBaseCalfBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfCalfMidLoc $lfMidCalfBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfCalfMidLoc $lfMidCalfBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfCalfTipLoc $lfTipCalfBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfCalfTipLoc $lfTipCalfBendCtrlRf1[0];
	
	//------------------------
	//Creation of Bendy Joints
	//------------------------
	select -cl;
	string $lfCalfBaseBendyJnt = `joint -n "lfCalfBaseBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfCalfBaseLoc $lfCalfBaseBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfCalfBaseLoc $lfCalfBaseBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfBaseCalfBendCtrl[0] $lfCalfBaseBendyJnt;
