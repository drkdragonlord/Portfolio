global proc jbBuildBendy(string $lfShoulder, string $lfElbow, string $lfWrist, string $rtShoulder, string $rtElbow, string $rtWrist)
{
	clearCache -all;
	float $lfShoulderPos[] = `xform -q -ws -t $lfShoulder`;
	float $lfElbowPos[] = `xform -q -ws -t $lfElbow`;
	float $lfWristPos[] = `xform -q -ws -t $lfWrist`;
	
	string $lfUpperArmSurf[] = `nurbsPlane -p 0 0 0 -ax 1 0 0 -w 1 -lr 1 -d 1 -u 1 -v 1 -ch 1 -n "lfUpperArmBendSurf"`;
	move -r 0 0.5 0 ;
	move 0 0 0 ($lfUpperArmSurf[0]+".scalePivot") ($lfUpperArmSurf[0]+".rotatePivot") ;
	setAttr ($lfUpperArmSurf[0]+".visibility") 0;
		
	//string $lfUpperArm = `distanceDimension -sp $lfShoulderPos[0] $lfShoulderPos[1] $lfShoulderPos[2] -ep $lfElbowPos[0] $lfElbowPos[1] $lfElbowPos[2]`;
	//float $lfUpperArmDist = `getAttr ($lfUpperArm+".distance")`;
	//setAttr ($lfUpperArmSurf[0]+".scaleY") $lfUpperArmDist;
	delete`pointConstraint -o 0 0 0 -w 1 $lfShoulder $lfUpperArmSurf[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfShoulder $lfUpperArmSurf[0]`;
	
	select -r ($lfUpperArmSurf[0]+".cv[0:3][0]");
	string $lfUpperArmBaseCl[] = `cluster -n "lfUpperArmBaseCluster"`;
	setAttr ($lfUpperArmBaseCl[1]+".visibility") 0;

	select -r ($lfUpperArmSurf[0]+".cv[0:3][3]");
	string $lfUpperArmTipCl[] = `cluster -n "lfUpperArmTipCluster"`;
	setAttr ($lfUpperArmTipCl[1]+".visibility") 0;
	
	pointConstraint -o 0 0 0 -w 1 $lfShoulder $lfUpperArmBaseCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfShoulder $lfUpperArmBaseCl[1];
	
	pointConstraint -o 0 0 0 -w 1 $lfElbow $lfUpperArmTipCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfElbow $lfUpperArmTipCl[1];
	
	//--------------------
	//Creation of Locators
	//--------------------
	
	select -r ($lfUpperArmSurf[0]+".uv[0.5][0.25]");
	jbRivet();
	string $lfUpperArmBaseLoc = `rename "lfUpperArmBaseLoc"`;
	setAttr ($lfUpperArmBaseLoc+".visibility") 0;
	select -r ($lfUpperArmSurf[0]+".uv[0.5][0.5]");
	jbRivet();
	string $lfUpperArmMidLoc = `rename "lfUpperArmMidLoc"`;
	setAttr ($lfUpperArmMidLoc+".visibility") 0;
	select -r ($lfUpperArmSurf[0]+".uv[0.5][0.75]");
	jbRivet();
	string $lfUpperArmTipLoc = `rename "lfUpperArmTipLoc"`;
	setAttr ($lfUpperArmTipLoc+".visibility") 0;
	
	//------------------------------------
	//Creation of Upper Arm Bendy Controls
	//------------------------------------
	string $lfBaseArmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfBaseArmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfBaseArmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfBaseArmBendCtrl[0]`;
	select -r $lfBaseArmBendCtrl[0];
	string $lfBaseArmBendCtrlRf[] = `duplicate -rr -n ($lfBaseArmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfBaseArmBendCtrl;
	string $lfBaseArmBendCtrlRf1[] = `duplicate -rr -n ($lfBaseArmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfBaseArmBendCtrl[0] $lfBaseArmBendCtrlRf[0];
	parent $lfBaseArmBendCtrlRf[0] $lfBaseArmBendCtrlRf1[0];
	
	string $lfMidArmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfMidArmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfMidArmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfMidArmBendCtrl[0]`;
	select -r $lfMidArmBendCtrl[0];
	string $lfMidArmBendCtrlRf[] = `duplicate -rr -n ($lfMidArmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfMidArmBendCtrl;
	string $lfMidArmBendCtrlRf1[] = `duplicate -rr -n ($lfMidArmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfMidArmBendCtrl[0] $lfMidArmBendCtrlRf[0];
	parent $lfMidArmBendCtrlRf[0] $lfMidArmBendCtrlRf1[0];
	
	string $lfTipArmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfTipArmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfTipArmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfTipArmBendCtrl[0]`;
	select -r $lfTipArmBendCtrl[0];
	string $lfTipArmBendCtrlRf[] = `duplicate -rr -n ($lfTipArmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfTipArmBendCtrl;
	string $lfTipArmBendCtrlRf1[] = `duplicate -rr -n ($lfTipArmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfTipArmBendCtrl[0] $lfTipArmBendCtrlRf[0];
	parent $lfTipArmBendCtrlRf[0] $lfTipArmBendCtrlRf1[0];
	
	//------------------------------------
	//Constraints of Control Master Groups
	//------------------------------------
	pointConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfBaseArmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfBaseArmBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfMidArmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfMidArmBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfTipArmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfTipArmBendCtrlRf1[0];
	
	//------------------------
	//Creation of Bendy Joints
	//------------------------
	select -cl;
	string $lfArmBaseBendyJnt = `joint -n "lfArmBaseBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfArmBaseBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmBaseLoc $lfArmBaseBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfBaseArmBendCtrl[0] $lfArmBaseBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfBaseArmBendCtrl[0] $lfArmBaseBendyJnt;
	
	select -cl;
	string $lfArmMidBendyJnt = `joint -n "lfArmMidBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfArmMidBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmMidLoc $lfArmMidBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfMidArmBendCtrl[0] $lfArmMidBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfMidArmBendCtrl[0] $lfArmMidBendyJnt;
	
	select -cl;
	string $lfArmTipBendyJnt = `joint -n "lfArmTipBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfArmTipBendyJnt`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfUpperArmTipLoc $lfArmTipBendyJnt`;
	pointConstraint -o 0 0 0 -w 1 $lfTipArmBendCtrl[0] $lfArmTipBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfTipArmBendCtrl[0] $lfArmTipBendyJnt;
	
	//--------------
	//Forearm Bendys
	//--------------
	string $lfForeArmSurf[] = `nurbsPlane -p 0 0 0 -ax 1 0 0 -w 1 -lr 1 -d 1 -u 1 -v 1 -ch 1 -n "lfForearmBendSurf"`;
	move -r 0 0.5 0 ;
	move 0 0 0 ($lfForeArmSurf[0]+".scalePivot") ($lfForeArmSurf[0]+".rotatePivot") ;
	setAttr ($lfForeArmSurf[0]+".visibility") 0;
		
	//string $lfForeArm = `distanceDimension -sp $lfShoulderPos[0] $lfShoulderPos[1] $lfShoulderPos[2] -ep $lfElbowPos[0] $lfElbowPos[1] $lfElbowPos[2]`;
	//float $lfForeArmDist = `getAttr ($lfForeArm+".distance")`;
	//setAttr ($lfForeArmSurf[0]+".scaleY") $lfForeArmDist;
	delete`pointConstraint -o 0 0 0 -w 1 $lfElbow $lfForeArmSurf[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfElbow $lfForeArmSurf[0]`;
	
	select -r ($lfForeArmSurf[0]+".cv[0:3][0]");
	string $lfForeArmBaseCl[] = `cluster -n "lfForeArmBaseCluster"`;
	setAttr ($lfForeArmBaseCl[1]+".visibility") 0;

	select -r ($lfForeArmSurf[0]+".cv[0:3][3]");
	string $lfForeArmTipCl[] = `cluster -n "lfForeArmTipCluster"`;
	setAttr ($lfForeArmTipCl[1]+".visibility") 0;
	
	pointConstraint -o 0 0 0 -w 1 $lfElbow $lfForeArmBaseCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfElbow $lfForeArmBaseCl[1];
	
	pointConstraint -o 0 0 0 -w 1 $lfWrist $lfForeArmTipCl[1];
	orientConstraint -o 0 0 0 -w 1 $lfWrist $lfForeArmTipCl[1];
	
	//--------------------
	//Creation of Locators
	//--------------------	
	select -r ($lfForeArmSurf[0]+".uv[0.5][0.25]");
	jbRivet();
	string $lfForeArmBaseLoc = `rename "lfForeArmBaseLoc"`;
	setAttr ($lfForeArmBaseLoc+".visibility") 0;
	select -r ($lfForeArmSurf[0]+".uv[0.5][0.5]");
	jbRivet();
	string $lfForeArmMidLoc = `rename "lfForeArmMidLoc"`;
	setAttr ($lfForeArmMidLoc+".visibility") 0;
	select -r ($lfForeArmSurf[0]+".uv[0.5][0.75]");
	jbRivet();
	string $lfForeArmTipLoc = `rename "lfForeArmTipLoc"`;
	setAttr ($lfForeArmTipLoc+".visibility") 0;
	
	//------------------------------------
	//Creation of Upper Arm Bendy Controls
	//------------------------------------
	string $lfBaseForearmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfBaseForearmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfForeArmBaseLoc $lfBaseForearmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfForeArmBaseLoc $lfBaseForearmBendCtrl[0]`;
	select -r $lfBaseForearmBendCtrl[0];
	string $lfBaseForearmBendCtrlRf[] = `duplicate -rr -n ($lfBaseForearmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfBaseForearmBendCtrl;
	string $lfBaseForearmBendCtrlRf1[] = `duplicate -rr -n ($lfBaseForearmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfBaseForearmBendCtrl[0] $lfBaseForearmBendCtrlRf[0];
	parent $lfBaseForearmBendCtrlRf[0] $lfBaseForearmBendCtrlRf1[0];
	
	string $lfMidForearmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfMidForearmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfForeArmMidLoc $lfMidForearmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfForeArmMidLoc $lfMidForearmBendCtrl[0]`;
	select -r $lfMidForearmBendCtrl[0];
	string $lfMidForearmBendCtrlRf[] = `duplicate -rr -n ($lfMidForearmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfMidForearmBendCtrl;
	string $lfMidForearmBendCtrlRf1[] = `duplicate -rr -n ($lfMidForearmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfMidForearmBendCtrl[0] $lfMidForearmBendCtrlRf[0];
	parent $lfMidForearmBendCtrlRf[0] $lfMidForearmBendCtrlRf1[0];
	
	string $lfTipForearmBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfTipForearmBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfForeArmTipLoc $lfTipForearmBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfForeArmTipLoc $lfTipForearmBendCtrl[0]`;
	select -r $lfTipForearmBendCtrl[0];
	string $lfTipForearmBendCtrlRf[] = `duplicate -rr -n ($lfTipForearmBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfTipForearmBendCtrl;
	string $lfTipForearmBendCtrlRf1[] = `duplicate -rr -n ($lfTipForearmBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfTipForearmBendCtrl[0] $lfTipForearmBendCtrlRf[0];
	parent $lfTipForearmBendCtrlRf[0] $lfTipForearmBendCtrlRf1[0];
	
	//------------------------------------
	//Constraints of Control Master Groups
	//------------------------------------
	pointConstraint -o 0 0 0 -w 1 $lfForeArmBaseLoc $lfBaseForearmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfForeArmBaseLoc $lfBaseForearmBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfForeArmMidLoc $lfMidForearmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfForeArmMidLoc $lfMidForearmBendCtrlRf1[0];
	
	pointConstraint -o 0 0 0 -w 1 $lfForeArmTipLoc $lfTipForearmBendCtrlRf1[0];
	orientConstraint -o 0 0 0 -w 1 $lfForeArmTipLoc $lfTipForearmBendCtrlRf1[0];
	
	//------------------------
	//Creation of Bendy Joints
	//------------------------
	select -cl;
	string $lfForearmBaseBendyJnt = `joint -n "lfForearmBaseBendyJnt"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfForeArmBaseLoc $lfForearmBaseBendyJnt`;
