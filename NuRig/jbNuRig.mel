	connectAttr -f ($rtUlnaTwistMDN+".outputY") ($rtMidCalfBendCtrlRf[0]+".rotateZ");
	connectAttr -f ($rtUlnaTwistMDN+".outputX") ($rtTipCalfBendCtrlRf[0]+".rotateZ");
	
	
	//---------------------------------------------------
	//Creating a visibility switch for the Bendy Controls
	//---------------------------------------------------
	string $rtSwitchCtrl = "rtFootCtrl";
	select -r $rtSwitchCtrl;
	addAttr -ln "legBendy" -at "enum" -en "off:on:" -k 1;
	connectAttr -f ($rtSwitchCtrl+".legBendy") ($rtTipCalfBendCtrlRf1[0]+".visibility");
