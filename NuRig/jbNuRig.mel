global proc jbNuRigAboutUI()
{
	if(`window -ex jbNuRigAboutWin`)
		deleteUI jbNuRigAboutWin;
	if(`windowPref -ex jbNuRigAboutWin`)
		windowPref -e -w 200 -h 300 -tlc 300 500 jbNuRigAboutWin;
				
	window -w 200 -h 300 -tlb 1 -t "Nu-Rig About UI" jbNuRigAboutWin;
		columnLayout -w 200 -h 300 -co "left" 10 jbNuRigAboutColumn;
			text -w 180 -ww 1 -al "center" -fn "boldLabelFont" -l "\nNu-Rig facilitates the bipedal character setup creation process. Moreover, it facilitates control selection and easy access to common rig switches through a graphical UI.\n\nNu-Rig is the brainchild of \nJuan Borrero. It is a tool created from a technical artists perspective, taking into account the most common requests he encountered throughout his career. Through constant skill updating and peer review it has evolved to what you see here today. \n\nNu-Rig is copyrighted to Juan Borrero © 2013" jbNuRigAboutTxt;
			text -w 180 -ww 1 -al "center" -fn "boldLabelFont" -hl 1 -l "www.juanborrero.com" jbNuRigSiteTxt;
		setParent..;
	
	showWindow jbNuRigAboutWin;
}//End global proc jbNuRigAboutUI(END)

