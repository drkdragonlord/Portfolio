	setAttr -lock true -keyable false -channelBox false ($rtBaseCalfBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($rtBaseCalfBendCtrl[0]+".v");
	
	string $rtMidCalfBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "rtMidCalfBendCtrl"`;
