	string $rtBaseCalfBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "rtBaseCalfBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $rtCalfBaseLoc $rtBaseCalfBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $rtCalfBaseLoc $rtBaseCalfBendCtrl[0]`;
	select -r $rtBaseCalfBendCtrl[0];
	string $rtBaseCalfBendCtrlRf[] = `duplicate -rr -n ($rtBaseCalfBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtBaseCalfBendCtrl;
	string $rtBaseCalfBendCtrlRf1[] = `duplicate -rr -n ($rtBaseCalfBendCtrl[0]+"MasterGRP")`;
