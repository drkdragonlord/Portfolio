	connectAttr -f ($rtSwitchCtrl+".legBendy") ($rtBaseCalfBendCtrlRf1[0]+".visibility");
	connectAttr -f ($rtSwitchCtrl+".legBendy") ($rtTipLegBendCtrlRf1[0]+".visibility");
	connectAttr -f ($rtSwitchCtrl+".legBendy") ($rtMidLegBendCtrlRf1[0]+".visibility");
	connectAttr -f ($rtSwitchCtrl+".legBendy") ($rtBaseLegBendCtrlRf1[0]+".visibility");
	
	string $lfSwitchCtrl = "lfFootCtrl";
	select -r $lfSwitchCtrl;
	addAttr -ln "legBendy" -at "enum" -en "off:on:" -k 1;
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfTipCalfBendCtrlRf1[0]+".visibility");
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfMidCalfBendCtrlRf1[0]+".visibility");
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfBaseCalfBendCtrlRf1[0]+".visibility");
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfTipLegBendCtrlRf1[0]+".visibility");
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfMidLegBendCtrlRf1[0]+".visibility");
	connectAttr -f ($lfSwitchCtrl+".legBendy") ($lfBaseLegBendCtrlRf1[0]+".visibility");
	
	
	//--------------------------------------
	//Cleaning up the nodes and the outliner
	//--------------------------------------
	string $bendyLegParts;
	if(`objExists DO_NOT_TOUCH_GRP`)
	{
		string $dntGrp = "DO_NOT_TOUCH_GRP";
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		$bendyLegParts = `group -em -n "bendyLegParts"`;
		parent $lfCalfBaseLoc $lfCalfMidLoc $lfCalfTipLoc $lfUpperLegBaseLoc $lfUpperLegMidLoc $lfUpperLegTipLoc $lfCalfSurf[0] $lfCalfBaseCl[1] $lfCalfTipCl[1] $lfUpperLegSurf[0] $lfUpperLegBaseCl[1] $lfUpperLegTipCl[1] $rtCalfSurf[0] $rtCalfBaseCl[1] $rtCalfTipCl[1] $rtUpperLegSurf[0] $rtUpperLegBaseCl[1] $rtUpperLegTipCl[1] $rtCalfBaseLoc $rtCalfMidLoc $rtCalfTipLoc $rtUpperLegBaseLoc $rtUpperLegMidLoc $rtUpperLegTipLoc     $bendyLegParts;
		parent $bendyLegParts $dntGrp;
	}
	else
	{
		//
		string $dntGrp = `group -em -n "DO_NOT_TOUCH_GRP"`;
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		$bendyLegParts = `group -em -n "bendyLegParts"`;
		parent $lfCalfBaseLoc $lfCalfMidLoc $lfCalfTipLoc $lfUpperLegBaseLoc $lfUpperLegMidLoc $lfUpperLegTipLoc $lfCalfSurf[0] $lfCalfBaseCl[1] $lfCalfTipCl[1] $lfUpperLegSurf[0] $lfUpperLegBaseCl[1] $lfUpperLegTipCl[1] $rtCalfSurf[0] $rtCalfBaseCl[1] $rtCalfTipCl[1] $rtUpperLegSurf[0] $rtUpperLegBaseCl[1] $rtUpperLegTipCl[1] $rtCalfBaseLoc $rtCalfMidLoc $rtCalfTipLoc $rtUpperLegBaseLoc $rtUpperLegMidLoc $rtUpperLegTipLoc     $bendyLegParts;
		parent $bendyLegParts $dntGrp;
	}
	
	//creation of sets for the various bendy parts
	select -r $lfLegBaseBendyJnt $lfLegMidBendyJnt $lfLegTipBendyJnt $rtLegBaseBendyJnt $rtLegMidBendyJnt $rtLegTipBendyJnt $lfCalfBaseBendyJnt $lfCalfMidBendyJnt $lfCalfTipBendyJnt $rtCalfBaseBendyJnt $rtCalfMidBendyJnt $rtCalfTipBendyJnt;
	sets -n legBendyJointsSet;
	
	select -r $lfBaseLegBendCtrl[0] $lfMidLegBendCtrl[0] $lfTipLegBendCtrl[0] $lfBaseCalfBendCtrl[0] $lfMidCalfBendCtrl[0] $lfTipCalfBendCtrl[0] $rtBaseLegBendCtrl[0] $rtMidLegBendCtrl[0] $rtTipLegBendCtrl[0] $rtBaseCalfBendCtrl[0] $rtMidCalfBendCtrl[0] $rtTipCalfBendCtrl[0];
	sets -n legBendyControlsSet;
	
	select -r $lfBaseLegBendCtrlRf[0] $lfBaseLegBendCtrlRf1[0] $lfMidLegBendCtrlRf[0] $lfMidLegBendCtrlRf1[0] $lfTipLegBendCtrlRf[0] $lfTipLegBendCtrlRf1[0] $lfBaseCalfBendCtrlRf[0] $lfBaseCalfBendCtrlRf1[0] $lfMidCalfBendCtrlRf[0] $lfMidCalfBendCtrlRf1[0] $lfTipCalfBendCtrlRf[0] $lfTipCalfBendCtrlRf1[0] $rtBaseLegBendCtrlRf[0] $rtBaseLegBendCtrlRf1[0] $rtMidLegBendCtrlRf[0] $rtMidLegBendCtrlRf1[0] $rtTipLegBendCtrlRf[0] $rtTipLegBendCtrlRf1[0] $rtBaseCalfBendCtrlRf[0] $rtBaseCalfBendCtrlRf1[0] $rtMidCalfBendCtrlRf[0] $rtMidCalfBendCtrlRf1[0] $rtTipCalfBendCtrlRf[0] $rtTipCalfBendCtrlRf1[0];
	sets -n legBendyGroupsSet;
	
	select -r $lfBaseLegBendCtrlRf1[0] $lfMidLegBendCtrlRf1[0] $lfTipLegBendCtrlRf1[0] $lfBaseCalfBendCtrlRf1[0] $lfMidCalfBendCtrlRf1[0] $lfTipCalfBendCtrlRf1[0] $rtBaseLegBendCtrlRf1[0] $rtMidLegBendCtrlRf1[0] $rtTipLegBendCtrlRf1[0] $rtBaseCalfBendCtrlRf1[0] $rtMidCalfBendCtrlRf1[0] $rtTipCalfBendCtrlRf1[0] $lfLegBaseBendyJnt $lfLegMidBendyJnt $lfLegTipBendyJnt $rtLegBaseBendyJnt $rtLegMidBendyJnt $rtLegTipBendyJnt $lfCalfBaseBendyJnt $lfCalfMidBendyJnt $lfCalfTipBendyJnt $rtCalfBaseBendyJnt $rtCalfMidBendyJnt $rtCalfTipBendyJnt;
	pickWalk -d down;
	pickWalk -d left;
	sets -n legBendyConstraintsSet;
	select -r $lfBaseLegBendCtrlRf1[0] $lfMidLegBendCtrlRf1[0] $lfTipLegBendCtrlRf1[0] $lfBaseCalfBendCtrlRf1[0] $lfMidCalfBendCtrlRf1[0] $lfTipCalfBendCtrlRf1[0] $rtBaseLegBendCtrlRf1[0] $rtMidLegBendCtrlRf1[0] $rtTipLegBendCtrlRf1[0] $rtBaseCalfBendCtrlRf1[0] $rtMidCalfBendCtrlRf1[0] $rtTipCalfBendCtrlRf1[0] $lfLegBaseBendyJnt $lfLegMidBendyJnt $lfLegTipBendyJnt $rtLegBaseBendyJnt $rtLegMidBendyJnt $rtLegTipBendyJnt $lfCalfBaseBendyJnt $lfCalfMidBendyJnt $lfCalfTipBendyJnt $rtCalfBaseBendyJnt $rtCalfMidBendyJnt $rtCalfTipBendyJnt;
	pickWalk -d down;
	pickWalk -d left;
	pickWalk -d left;
	sets -add legBendyConstraintsSet;
	
	select -r $bendyLegParts;
	select -hi;
	sets -n legAdditionalBendyPartsSet;
	select -r $lfUlnaTwistMDN $rtUlnaTwistMDN;
	sets -add legAdditionalBendyPartsSet;
	
	select -r -ne legBendyConstraintsSet legBendyGroupsSet legBendyControlsSet legBendyJointsSet legAdditionalBendyPartsSet;
	sets -n legBendyComponents;
	
	//cleaning the control channel box
	select -r legBendyControlsSet;
	string $ctrlSel[] = `ls -sl`;
	
	for($i=0; $i<size($ctrlSel);$i++)
	{
		setAttr -lock true -keyable false -channelBox false ($ctrlSel[$i]+".sx");
		setAttr -lock true -keyable false -channelBox false ($ctrlSel[$i]+".sy");
		setAttr -lock true -keyable false -channelBox false ($ctrlSel[$i]+".sz");
		setAttr -lock true -keyable false -channelBox false ($ctrlSel[$i]+".v");
	}
	
}//End of global proc jbBuildLegBendy(END)

global proc jbBuildLegStretchy(string $lfThigh, string $lfKnee, string $lfAnkle, string $lfIkHandle, string $lfSwitchCtrl, string $rtThigh, string $rtKnee, string $rtAnkle, string $rtIkHandle, string $rtSwitchCtrl)
{
	clearCache -all;
	
	float $lfThighPos[] = `xform -q -ws -t $lfThigh`;
	float $lfKneePos[] = `xform -q -ws -t $lfKnee`;
	float $lfAnklePos[] = `xform -q -ws -t $lfAnkle`;
		
	distanceDimension -sp $lfThighPos[0] $lfThighPos[1] $lfThighPos[2] -ep $lfAnklePos[0] $lfAnklePos[1] $lfAnklePos[2];
	string $lfStretchDistTool = `rename distanceDimension1 "lfLegDistDimension"`;
	float $lfStretchDist = `getAttr ($lfStretchDistTool+".distance")`;
	
	//------------------------------------------
	//Clearing the Distance Base locator for use
	//------------------------------------------
	string $lfLegStretchBaseLoc = `rename locator1 "lfLegStretchBaseLoc"`;
	string $lfLegStretchBaseLocRf[] = `duplicate -rr -n ($lfLegStretchBaseLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfLegStretchBaseLoc;
	string $lfLegStretchBaseLocRf1[] = `duplicate -rr -n ($lfLegStretchBaseLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfLegStretchBaseLoc $lfLegStretchBaseLocRf[0];
	parent $lfLegStretchBaseLocRf[0] $lfLegStretchBaseLocRf1[0];	
	
	//------------------------------------------
	//Clearing the Distance Tip locator for use
	//------------------------------------------
	string $lfLegStretchTipLoc = `rename locator2 "lfLegStretchTipLoc"`;	
	string $lfLegStretchTipLocRf[] = `duplicate -rr -n ($lfLegStretchTipLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfLegStretchTipLoc;
	string $lfLegStretchTipLocRf1[] = `duplicate -rr -n ($lfLegStretchTipLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfLegStretchTipLoc $lfLegStretchTipLocRf[0];
	parent $lfLegStretchTipLocRf[0] $lfLegStretchTipLocRf1[0];	
	
	delete ($lfLegStretchBaseLoc+"GRP1");
	
	//----------------------------
	//Constraints for the locators
	//----------------------------
	pointConstraint -o 0 0 0 -w 1 $lfThigh $lfLegStretchBaseLocRf1[0];
	pointConstraint -o 0 0 0 -w 1 $lfIkHandle $lfLegStretchTipLocRf1[0];
	
	//--------------------------------------------------------------------
	//Adding a global scale attribute to the left switch command for the moment
	//--------------------------------------------------------------------
	select -r $lfSwitchCtrl;
	addAttr -ln "globalScale" -at double -min 0.1 -dv 1 -k 1;
	addAttr -ln "legStretch" -at "enum" -en "off:on:" -k 1;
	//addAttr -ln "rtLegStretch" -at "enum" -en "off:on:" -k 1;
	
	//----------------------
	//Global scale fix nodes
	//----------------------
	string $globalMDN;
	if(`objExists "globalCharScaleMDN"`)
	{
		select -r globalCharScaleMDN;
		$globalMDN = "globalCharScaleMDN";
		//connectAttr -f ($lfSwitchCtrl+".globalScale") ($globalMDN+".input1X");
		setAttr ($globalMDN+".operation") 1; // Multiply Operation
		setAttr ($globalMDN+".input2X") $lfStretchDist;
	}
	else
	{
		$globalMDN = `shadingNode -asUtility multiplyDivide -n "globalCharScaleMDN"`;
		//connectAttr -f ($lfSwitchCtrl+".globalScale") ($globalMDN+".input1X");
		setAttr ($globalMDN+".operation") 1; // Multiply Operation
		setAttr ($globalMDN+".input2X") $lfStretchDist;
	}
	
	//------------------
	//Stretch Main Nodes
	//------------------
	string $lfLegStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfLegStretchMDN"`;
	setAttr ($lfLegStretchMDN+".operation") 2; // Divide Operation
	connectAttr -f ($lfStretchDistTool+".distance") ($lfLegStretchMDN+".input1X");
	connectAttr -f ($globalMDN+".outputX") ($lfLegStretchMDN+".input2X");
	
	string $lfLegStretchCond = `shadingNode -asUtility condition -n "lfLegStretchCond"`;
	setAttr ($lfLegStretchCond+".operation") 3; // Greater than or Equal Operation
	connectAttr -f ($lfStretchDistTool+".distance") ($lfLegStretchCond+".firstTerm");
	connectAttr -f ($globalMDN+".outputX") ($lfLegStretchCond+".secondTerm");
	connectAttr -f ($lfLegStretchMDN+".outputX") ($lfLegStretchCond+".colorIfTrueR");
	
	//--------------------
	//Stretch Enable Nodes
	//--------------------
	string $lfLegStretchSwitch = `shadingNode -asUtility condition -n "lfLegStretchSwitch"`;
	setAttr ($lfLegStretchSwitch+".operation") 0; // Equal Operation
	setAttr ($lfLegStretchSwitch+".secondTerm") 1; 
	connectAttr -f ($lfSwitchCtrl+".legStretch") ($lfLegStretchSwitch+".firstTerm");
	//connectAttr -f ($lfLegStretchCond+".outColorR") ($lfLegStretchSwitch+".firstTerm");
	connectAttr -f ($lfLegStretchCond+".outColorR") ($lfLegStretchSwitch+".colorIfTrueR");
	
	//-------------------------------------------
	//Creates the local Nodes per joint to stretch
	//-------------------------------------------
	float $elbowTransY = `getAttr ($lfKnee+".ty")`;
	string $lfKneeStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfKneeStretchMDN"`;
	setAttr ($lfKneeStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($lfLegStretchSwitch+".outColorR") ($lfKneeStretchMDN+".input1X");
	setAttr ($lfKneeStretchMDN+".input2X") $elbowTransY;
	connectAttr -f ($lfKneeStretchMDN+".outputX") ($lfKnee+".ty");
	
	float $wristTransY = `getAttr ($lfAnkle+".ty")`;
	string $lfAnkleStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfAnkleStretchMDN"`;
	setAttr ($lfAnkleStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($lfLegStretchSwitch+".outColorR") ($lfAnkleStretchMDN+".input1X");
	setAttr ($lfAnkleStretchMDN+".input2X") $wristTransY;
	connectAttr -f ($lfAnkleStretchMDN+".outputX") ($lfAnkle+".ty");
	
	//-------------------
	//-------------------
	//Right side stretchy
	//-------------------
	//-------------------
	
	//--------------------------------------------------------------------
	//Adding a global scale attribute to the left switch command for the moment
	//--------------------------------------------------------------------
	select -r $rtSwitchCtrl;
	//addAttr -ln "globalScale" -at double -min 0.1 -dv 1 -k 1;
	addAttr -ln "legStretch" -at "enum" -en "off:on:" -k 1;
	
	float $rtThighPos[] = `xform -q -ws -t $rtThigh`;
	float $rtKneePos[] = `xform -q -ws -t $rtKnee`;
	float $rtAnklePos[] = `xform -q -ws -t $rtAnkle`;
	
	distanceDimension -sp $rtThighPos[0] $rtThighPos[1] $rtThighPos[2] -ep $rtAnklePos[0] $rtAnklePos[1] $rtAnklePos[2];
	string $rtStretchDistTool = `rename distanceDimension1 "rtLegDistDimension"`;
	float $rtStretchDist = `getAttr ($rtStretchDistTool+".distance")`;
	
	//------------------------------------------
	//Clearing the Distance Base locator for use
	//------------------------------------------
	string $rtLegStretchBaseLoc = `rename locator1 "rtLegStretchBaseLoc"`;
	string $rtLegStretchBaseLocRf[] = `duplicate -rr -n ($rtLegStretchBaseLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtLegStretchBaseLoc;
	string $rtLegStretchBaseLocRf1[] = `duplicate -rr -n ($rtLegStretchBaseLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $rtLegStretchBaseLoc $rtLegStretchBaseLocRf[0];
	parent $rtLegStretchBaseLocRf[0] $rtLegStretchBaseLocRf1[0];	
	
	//------------------------------------------
	//Clearing the Distance Tip locator for use
	//------------------------------------------
	string $rtLegStretchTipLoc = `rename locator2 "rtLegStretchTipLoc"`;	
	string $rtLegStretchTipLocRf[] = `duplicate -rr -n ($rtLegStretchTipLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtLegStretchTipLoc;
	string $rtLegStretchTipLocRf1[] = `duplicate -rr -n ($rtLegStretchTipLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $rtLegStretchTipLoc $rtLegStretchTipLocRf[0];
	parent $rtLegStretchTipLocRf[0] $rtLegStretchTipLocRf1[0];
	
	delete ($rtLegStretchBaseLoc+"GRP1");
	
	//----------------------------
	//Constraints for the locators
	//----------------------------
	pointConstraint -o 0 0 0 -w 1 $rtThigh $rtLegStretchBaseLocRf1[0];
	pointConstraint -o 0 0 0 -w 1 $rtIkHandle $rtLegStretchTipLocRf1[0];
	
	//------------------
	//Stretch Main Nodes
	//------------------
	string $rtLegStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtLegStretchMDN"`;
	setAttr ($rtLegStretchMDN+".operation") 2; // Divide Operation
	connectAttr -f ($rtStretchDistTool+".distance") ($rtLegStretchMDN+".input1X");
	connectAttr -f ($globalMDN+".outputX") ($rtLegStretchMDN+".input2X");
	
	string $rtLegStretchCond = `shadingNode -asUtility condition -n "rtLegStretchCond"`;
	setAttr ($rtLegStretchCond+".operation") 3; // Greater than or Equal Operation
	connectAttr -f ($rtStretchDistTool+".distance") ($rtLegStretchCond+".firstTerm");
	connectAttr -f ($globalMDN+".outputX") ($rtLegStretchCond+".secondTerm");
	connectAttr -f ($rtLegStretchMDN+".outputX") ($rtLegStretchCond+".colorIfTrueR");
	
	//--------------------
	//Stretch Enable Nodes
	//--------------------
	string $rtLegStretchSwitch = `shadingNode -asUtility condition -n "rtLegStretchSwitch"`;
	setAttr ($rtLegStretchSwitch+".operation") 0; // Equal Operation
	setAttr ($rtLegStretchSwitch+".secondTerm") 1; 
	connectAttr -f ($rtSwitchCtrl+".legStretch") ($rtLegStretchSwitch+".firstTerm");
	//connectAttr -f ($rtLegStretchCond+".outColorR") ($rtLegStretchSwitch+".firstTerm");
	connectAttr -f ($rtLegStretchCond+".outColorR") ($rtLegStretchSwitch+".colorIfTrueR");
	
	//-------------------------------------------
	//Creates the local Nodes per joint to stretch
	//-------------------------------------------
	float $elbowTransY = `getAttr ($rtKnee+".ty")`;
	string $rtKneeStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtKneeStretchMDN"`;
	setAttr ($rtKneeStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($rtLegStretchSwitch+".outColorR") ($rtKneeStretchMDN+".input1X");
	setAttr ($rtKneeStretchMDN+".input2X") $elbowTransY;
	connectAttr -f ($rtKneeStretchMDN+".outputX") ($rtKnee+".ty");
	
	float $wristTransY = `getAttr ($rtAnkle+".ty")`;
	string $rtAnkleStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtAnkleStretchMDN"`;
	setAttr ($rtAnkleStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($rtLegStretchSwitch+".outColorR") ($rtAnkleStretchMDN+".input1X");
	setAttr ($rtAnkleStretchMDN+".input2X") $wristTransY;
	connectAttr -f ($rtAnkleStretchMDN+".outputX") ($rtAnkle+".ty");	
	
	string $dntGrp; 
	
	if(`objExists DO_NOT_TOUCH_GRP`)
	{
		$dntGrp = "DO_NOT_TOUCH_GRP";
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		string $stretchyLegParts = `group -em -n "stretchyLegParts"`;
		parent $lfStretchDistTool $lfLegStretchBaseLocRf1[0] $lfLegStretchTipLocRf1[0] $rtStretchDistTool $rtLegStretchBaseLocRf1[0] $rtLegStretchTipLocRf1[0] $stretchyLegParts;
		parent $stretchyLegParts $dntGrp;
	}
	else
	{
		//
		$dntGrp = `group -em -n "DO_NOT_TOUCH_GRP"`;
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		string $stretchyLegParts = `group -em -n "stretchyLegParts"`;
		parent $lfStretchDistTool $lfLegStretchBaseLocRf1[0] $lfLegStretchTipLocRf1[0] $rtStretchDistTool $rtLegStretchBaseLocRf1[0] $rtLegStretchTipLocRf1[0] $stretchyLegParts;
		parent $stretchyLegParts $dntGrp;
	}	
	
	//adding stretch parts to a set
	select -r $dntGrp;
	select -hi;
	sets -n legStretchComponents;
	select -r $globalMDN $lfLegStretchMDN $lfLegStretchCond $lfLegStretchSwitch $lfKneeStretchMDN $lfAnkleStretchMDN $rtLegStretchMDN $rtLegStretchCond $rtLegStretchSwitch $rtKneeStretchMDN $rtAnkleStretchMDN;
	sets -add legStretchComponents;
	
}//End of global proc jbBuildLegStretchy(END)

global proc jbBuildLegSwitch(string $lfFootRoll, string $lfLegPvCons, string $lfIkGrpCons, string $lfFootRollGrpCons, string $lfLegIkCtrl, string $lfLegFkCtrl, string $lfFkPvCtrl, string $lfIkPvCtrl, string $rtFootRoll, string $rtLegPvCons, string $rtIkGrpCons, string $rtFootRollGrpCons, string $rtLegIkCtrl, string $rtLegFkCtrl, string $rtFkPvCtrl, string $rtIkPvCtrl)
{
	clearCache -all;
	// creates the condition node and setups its initial parameters
	string $lfLegIkBlendCond = `shadingNode -asUtility condition -n "lfLegIkBlendCond"`;
	string $lfLegIkBlendRev = `shadingNode -asUtility reverse -n "lfLegIkBlendRev"`;
	setAttr ($lfLegIkBlendCond+".secondTerm") 1;
	setAttr ($lfLegIkBlendCond+".operation") 5;
	setAttr ($lfLegIkBlendCond+".colorIfFalseR") 0;
	select -r $lfLegIkBlendCond;
	
	//Connect the attributes to the shading nodes
	connectAttr -f ($lfFootRoll+".ikBlend") ($lfLegIkBlendCond+".firstTerm");
	connectAttr -f ($lfFootRoll+".ikBlend") ($lfLegIkBlendCond+".colorIfTrueR");
	connectAttr -f ($lfFootRoll+".ikBlend") ($lfLegIkBlendRev+".inputX");
	
	connectAttr -f ($lfLegIkBlendRev+".outputX") ($lfIkGrpCons+"."+$lfLegFkCtrl+"W0");
	connectAttr -f ($lfLegIkBlendRev+".outputX") ($lfFootRollGrpCons+"."+$lfLegFkCtrl+"W0");
	connectAttr -f ($lfLegIkBlendRev+".outputX") ($lfLegPvCons+"."+$lfFkPvCtrl+"W0");
	
	connectAttr -f ($lfLegIkBlendCond+".outColorR") ($lfIkGrpCons+"."+$lfLegIkCtrl+"W1");
	connectAttr -f ($lfLegIkBlendCond+".outColorR") ($lfFootRollGrpCons+"."+$lfLegIkCtrl+"W1");
	connectAttr -f ($lfLegIkBlendCond+".outColorR") ($lfLegPvCons+"."+$lfIkPvCtrl+"W1");
	
	// creates the condition node and setups its initial parameters
	string $rtLegIkBlendCond = `shadingNode -asUtility condition -n "rtLegIkBlendCond"`;
	string $rtLegIkBlendRev = `shadingNode -asUtility reverse -n "rtLegIkBlendRev"`;
	setAttr ($rtLegIkBlendCond+".secondTerm") 1;
	setAttr ($rtLegIkBlendCond+".operation") 5;
	setAttr ($rtLegIkBlendCond+".colorIfFalseR") 0;
	select -r $rtLegIkBlendCond;
	
	//Connect the attributes to the shading nodes
	connectAttr -f ($rtFootRoll+".ikBlend") ($rtLegIkBlendCond+".firstTerm");
	connectAttr -f ($rtFootRoll+".ikBlend") ($rtLegIkBlendCond+".colorIfTrueR");
	connectAttr -f ($rtFootRoll+".ikBlend") ($rtLegIkBlendRev+".inputX");
	
	connectAttr -f ($rtLegIkBlendRev+".outputX") ($rtIkGrpCons+"."+$rtLegFkCtrl+"W0");
	connectAttr -f ($rtLegIkBlendRev+".outputX") ($rtFootRollGrpCons+"."+$rtLegFkCtrl+"W0");
	connectAttr -f ($rtLegIkBlendRev+".outputX") ($rtLegPvCons+"."+$rtFkPvCtrl+"W0");
	
	connectAttr -f ($rtLegIkBlendCond+".outColorR") ($rtIkGrpCons+"."+$rtLegIkCtrl+"W1");
	connectAttr -f ($rtLegIkBlendCond+".outColorR") ($rtFootRollGrpCons+"."+$rtLegIkCtrl+"W1");
	connectAttr -f ($rtLegIkBlendCond+".outColorR") ($rtLegPvCons+"."+$rtIkPvCtrl+"W1");
	
	//-------------------
	//Cleans visibilities
	//-------------------
	connectAttr -f ($lfLegIkBlendCond+".outColorR") ($lfLegIkCtrl+".visibility");
	connectAttr -f ($lfLegIkBlendCond+".outColorR") ($lfIkPvCtrl+".visibility");
	connectAttr -f ($lfLegIkBlendRev+".outputX") ("lfThighJntFkCtrl.visibility");
	
	connectAttr -f ($rtLegIkBlendCond+".outColorR") ($rtLegIkCtrl+".visibility");
	connectAttr -f ($rtLegIkBlendCond+".outColorR") ($rtIkPvCtrl+".visibility");
	connectAttr -f ($rtLegIkBlendRev+".outputX") ("rtThighJntFkCtrl.visibility");
	
	//create sets for the leg switch items
	select -r $lfLegIkBlendCond $lfLegIkBlendRev $rtLegIkBlendCond $rtLegIkBlendRev;
	sets -n legSwitchPartsSet;
	
}//End of global proc jbBuildLegSwitch(END)

