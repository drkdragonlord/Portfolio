global proc jbDeleteNuRigUI()
{
	if(`objExists nuRigComponents`)
	{
		if(`window -ex jbDeleteNuRigPromptUI`)
			deleteUI jbDeleteNuRigPromptUI;
		if(`windowPref -ex jbDeleteNuRigPromptUI`)
			windowPref -e -w 200 -h 75 jbDeleteNuRigPromptUI;
			
		window -s 0 -w 200 -h 75 -t "Delete Nu-Rig" jbDeleteNuRigPromptUI;
			formLayout -nd 100 jbDeleteNuRigPromptUIForm;
				text -fn "boldLabelFont" -l "This will delete the Nu-Rig setup in the scene.\n Are you sure you want to continue?" cautionTxt;
				button -bgc 0.5 0.0 0.0 -w 100 -h 24 -l "Yes" -c "jbDeleteNuRig(); deleteUI jbDeleteNuRigPromptUI;" okBtn;
				button -bgc 0.3 0.3 0.3 -w 100 -h 24 -l "No" -c "deleteUI jbDeleteNuRigPromptUI;" cancelBtn;		
			setParent..;
			
			formLayout -e
				-af cautionTxt "top" 5
				-af cautionTxt "left" 5
				
				-af okBtn "top" 40
				-af okBtn "left" 20
				
				-af cancelBtn "top" 40
				-af cancelBtn "left" 140
			jbDeleteNuRigPromptUIForm;
			
		showWindow jbDeleteNuRigPromptUI;
	}
	else
	{
		error ("No Nu-Rig setup exists in the current scene.");
	}
}//End global proc jbDeleteNuRigUI(END)

