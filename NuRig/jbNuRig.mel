				warning ("Nu-Rig Arm Guides deleted successfully!\n");
			}
			else if(`objExists neckGuides`)
			{ 
				select -r neckGuides;
				doDelete;
				
				warning ("Nu-Rig Neck Guides deleted successfully!\n");
				if(`objExists jawGuides`)
				{
					select -r jawGuides;
					doDelete;
					
					warning ("Nu-Rig Jaw Guides deleted successfully!\n");
				}
			}
			select -r legGuides;
			doDelete;	
			
			image -e -i ($nuRigScriptPath+"noGuides.bmp")-w 190 -h 400 nuRigGuidesImg;
		}
		else
		{
			select -r legGuides;
			doDelete;
			
			image -e -i ($nuRigScriptPath+"noGuides.bmp")-w 190 -h 400 nuRigGuidesImg;
		}
		
		warning ("Nu-Rig Leg Guides deleted successfully!\n");
	}
	else if(`objExists neckGuides`)
	{
		if(`objExists jawGuides`)
		{
			select -r jawGuides;
			doDelete;
			
			warning ("Nu-Rig Jaw Guides deleted successfully!\n");
		}
		else if(`objExists spineGuides`)
		{
			select -r spineGuides;
			doDelete;	
			
			warning ("Nu-Rig Spine Guides deleted successfully!\n");
			if(`objExists legGuides`)
				{
					select -r legGuides;
					doDelete;
					
					warning ("Nu-Rig Leg Guides deleted successfully!\n");
				}
				else if(`objExists armGuides`)
				{
					select -r armGuides;
					doDelete;
					
					warning ("Nu-Rig Arm Guides deleted successfully!\n");
				}
			select -r neckGuides;
			doDelete;	
		}
		else
		{
			select -r neckGuides;
			doDelete;
			
			image -e -i ($nuRigScriptPath+"noGuides.bmp")-w 190 -h 400 nuRigGuidesImg;
		}
		
		image -e -i ($nuRigScriptPath+"noGuides.bmp")-w 190 -h 400 nuRigGuidesImg;
		
		warning ("Nu-Rig Neck Guides deleted successfully!\n");
	}
	else if(`objExists spineGuides`)
	{
		select -r spineGuides;
		doDelete;
		
		warning ("Nu-Rig Spine Guides deleted successfully!\n");
		
		image -e -i ($nuRigScriptPath+"noGuides.bmp")-w 190 -h 400 nuRigGuidesImg;
	}
	else
	{
		error ("No Nu-Rig Guides found!\n");
	}
	
}//End global proc jbDeleteNuRigGuides(END)

