global proc jbMatchIk()
{
	
	if(`objExists lfWristJntIkCtrl`)
	{
		if(`objExists lfWristJntFkCtrl`)
		{
			string $lfIkCtrl = "lfWristJntIkCtrl";
			string $lfWristFkCtrl = "lfWristJntFkCtrl";
			string $lfElbowFkCtrl = "lfElbowJntFkCtrl";
			string $lfShoulderFkCtrl = "lfShldJntFkCtrl";
			
			//Ensuring that controls are setup as IK
			float $lfSwitchQuery = `getAttr lfHandCtrl.ikBlend`;
			if($lfSwitchQuery != 1)
			{
				setAttr	"lfHandCtrl.ikBlend" 1;
			}
			//Querying joint rotations
			float $lfWristRot[] = `xform -q -ro "lfWristJnt"`;
			float $lfElbowRot[] = `xform -q -ro "lfElbowJnt"`;
			float $lfShoulderRot[] = `xform -q -ro "lfShldJnt"`;
			
			setAttr "lfHandCtrl.ikBlend" $lfSwitchQuery;
			
			for($i = 0; $i < 3; $i++)
			{
				if($i < 1)
				{
				setAttr ($lfWristFkCtrl+".rotateX") $lfWristRot[$i];
				setAttr ($lfElbowFkCtrl+".rotateX") $lfElbowRot[$i];
				setAttr ($lfShoulderFkCtrl+".rotateX") $lfShoulderRot[$i];
				}
				if($i < 2)
				{
					setAttr ($lfWristFkCtrl+".rotateY") $lfWristRot[$i];
					//setAttr ($lfElbowFkCtrl+".rotateY") $lfElbowRot[$i];
					setAttr ($lfShoulderFkCtrl+".rotateY") $lfShoulderRot[$i];
				}
				if($i < 3)
				{
					setAttr ($lfWristFkCtrl+".rotateZ") $lfWristRot[$i];
					//setAttr ($lfElbowFkCtrl+".rotateZ") $lfElbowRot[$i];
					setAttr ($lfShoulderFkCtrl+".rotateZ") $lfShoulderRot[$i];
				}	
			}
			setAttr "lfHandCtrl.ikBlend" 0;
			
		}
		else
		{
			error "No FK controls found or they have been renamed";
		}
	}
	else
	{
		error "No Ik controls found or they have been renamed";
	}	
	
	//-------------------
	//Right Side Matching
	//-------------------
	if(`objExists rtWristJntIkCtrl`)
	{
		if(`objExists rtWristJntFkCtrl`)
		{
			string $rtIkCtrl = "rtWristJntIkCtrl";
			string $rtWristFkCtrl = "rtWristJntFkCtrl";
			string $rtElbowFkCtrl = "rtElbowJntFkCtrl";
			string $rtShoulderFkCtrl = "rtShldJntFkCtrl";
			
			//Ensuring that controls are setup as IK
			float $rtSwitchQuery = `getAttr rtHandCtrl.ikBlend`;
			if($rtSwitchQuery != 1)
			{
				setAttr	"rtHandCtrl.ikBlend" 1;
			}
			//Querying joint rotations
			float $rtWristRot[] = `xform -q -ro "rtWristJnt"`;
			float $rtElbowRot[] = `xform -q -ro "rtElbowJnt"`;
			float $rtShoulderRot[] = `xform -q -ro "rtShldJnt"`;
			
			setAttr "rtHandCtrl.ikBlend" $rtSwitchQuery;
			
			for($i = 0; $i < 3; $i++)
			{
				if($i < 1)
				{
				setAttr ($rtWristFkCtrl+".rotateX") $rtWristRot[$i];
				setAttr ($rtElbowFkCtrl+".rotateX") $rtElbowRot[$i];
				setAttr ($rtShoulderFkCtrl+".rotateX") $rtShoulderRot[$i];
				}
				if($i < 2)
				{
					setAttr ($rtWristFkCtrl+".rotateY") $rtWristRot[$i];
					//setAttr ($rtElbowFkCtrl+".rotateY") $rtElbowRot[$i];
					setAttr ($rtShoulderFkCtrl+".rotateY") $rtShoulderRot[$i];
				}
				if($i < 3)
				{
					setAttr ($rtWristFkCtrl+".rotateZ") $rtWristRot[$i];
					//setAttr ($rtElbowFkCtrl+".rotateZ") $rtElbowRot[$i];
					setAttr ($rtShoulderFkCtrl+".rotateZ") $rtShoulderRot[$i];
				}	
			}
			setAttr "rtHandCtrl.ikBlend" 0;
			
			jbRtIkBlendOff();
		}
		else
		{
			error "No FK controls found or they have been renamed";
		}
	}
	else
	{
		error "No Ik controls found or they have been renamed";
	}	
}//End of global proc jbMatchIk(END)

global proc jbMatchFk()
{
	if(`objExists lfWristJntIkCtrl`)
	{
		if(`objExists lfWristJntFkCtrl`)
		{
			string $lfIkCtrl = "lfWristJntIkCtrl";
			string $lfWristFkCtrl = "lfWristJntFkCtrl";
			string $lfPvIkCtrl = "armLfPvIkCtrl";
			string $lfPvFkCtrl = "armLfPvFkCtrl";
			
			
			//Ensuring that controls are setup as IK
			float $lfSwitchQuery = `getAttr lfHandCtrl.ikBlend`;
			if($lfSwitchQuery != 0)
			{
				setAttr	"lfHandCtrl.ikBlend" 0;
			}
			//Querying joint rotations
			//float $lfWristRot[] = `xform -q -ro $lfWristFkCtrl`;
			//float $lfWristTran[] = `xform -q -ws -t $lfWristFkCtrl`;
			//float $lfFkPvPos[] = `xform -q -t $lfPvIkCtrl`;
			
			
			setAttr "lfHandCtrl.ikBlend" $lfSwitchQuery;
			
			delete`orientConstraint -o 0 0 0 -w 1	$lfWristFkCtrl $lfIkCtrl`;
			delete`pointConstraint -o 0 0 0 -w 1 	$lfWristFkCtrl $lfIkCtrl`;
			delete`pointConstraint -o 0 0 0 -w 1 	$lfPvFkCtrl $lfPvIkCtrl`;
			
			setAttr "lfHandCtrl.ikBlend" 1;
			
		}
		else
		{
			error "No FK controls found or they have been renamed";
		}
	}
	else
	{
		error "No Ik controls found or they have been renamed";
	}
	
	//-------------------
	//Right Side Matching
	//-------------------
	if(`objExists rtWristJntIkCtrl`)
	{
		if(`objExists rtWristJntFkCtrl`)
		{
			string $rtIkCtrl = "rtWristJntIkCtrl";
			string $rtWristFkCtrl = "rtWristJntFkCtrl";
			string $rtPvIkCtrl = "armRtPvIkCtrl";
			string $rtPvFkCtrl = "armRtPvFkCtrl";
			
			
			//Ensuring that controls are setup as IK
			float $rtSwitchQuery = `getAttr rtHandCtrl.ikBlend`;
			if($rtSwitchQuery != 0)
			{
				setAttr	"rtHandCtrl.ikBlend" 0;
			}
			//Querying joint rotations
			//float $rtWristRot[] = `xform -q -ro $rtWristFkCtrl`;
			//float $rtWristTran[] = `xform -q -ws -t $rtWristFkCtrl`;
			//float $rtFkPvPos[] = `xform -q -t $rtPvIkCtrl`;
			
			
			setAttr "rtHandCtrl.ikBlend" $rtSwitchQuery;
			
			delete`orientConstraint -o 0 0 0 -w 1	$rtWristFkCtrl $rtIkCtrl`;
			delete`pointConstraint -o 0 0 0 -w 1 	$rtWristFkCtrl $rtIkCtrl`;
			delete`pointConstraint -o 0 0 0 -w 1 	$rtPvFkCtrl $rtPvIkCtrl`;
			
			setAttr "rtHandCtrl.ikBlend" 1;
			
			jbRtIkBlendOn();
			
		}
		else
		{
			error "No FK controls found or they have been renamed";
		}
	}
	else
	{
		error "No Ik controls found or they have been renamed";
	}
	
}//End of global proc jbMatchFk(END)

