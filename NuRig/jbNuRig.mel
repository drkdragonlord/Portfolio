	pickWalk -d down;
	doDelete;
	parent $lfBaseCalfBendCtrl[0] $lfBaseCalfBendCtrlRf[0];
	parent $lfBaseCalfBendCtrlRf[0] $lfBaseCalfBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfBaseCalfBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfBaseCalfBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfBaseCalfBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfBaseCalfBendCtrl[0]+".v");
	
	string $lfMidCalfBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfMidCalfBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfCalfMidLoc $lfMidCalfBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfCalfMidLoc $lfMidCalfBendCtrl[0]`;
	select -r $lfMidCalfBendCtrl[0];
	string $lfMidCalfBendCtrlRf[] = `duplicate -rr -n ($lfMidCalfBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfMidCalfBendCtrl;
	string $lfMidCalfBendCtrlRf1[] = `duplicate -rr -n ($lfMidCalfBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfMidCalfBendCtrl[0] $lfMidCalfBendCtrlRf[0];
	parent $lfMidCalfBendCtrlRf[0] $lfMidCalfBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfMidCalfBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfMidCalfBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfMidCalfBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfMidCalfBendCtrl[0]+".v");
	
	string $lfTipCalfBendCtrl[] = `circle -nr 0 0 1 -r 1 -d 3 -s 8 -ch 0 -n "lfTipCalfBendCtrl"`;
	delete`pointConstraint -o 0 0 0 -w 1 $lfCalfTipLoc $lfTipCalfBendCtrl[0]`;
	delete`orientConstraint -o 0 0 0 -w 1 $lfCalfTipLoc $lfTipCalfBendCtrl[0]`;
	select -r $lfTipCalfBendCtrl[0];
	string $lfTipCalfBendCtrlRf[] = `duplicate -rr -n ($lfTipCalfBendCtrl[0]+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfTipCalfBendCtrl;
	string $lfTipCalfBendCtrlRf1[] = `duplicate -rr -n ($lfTipCalfBendCtrl[0]+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfTipCalfBendCtrl[0] $lfTipCalfBendCtrlRf[0];
	parent $lfTipCalfBendCtrlRf[0] $lfTipCalfBendCtrlRf1[0];
	setAttr -lock true -keyable false -channelBox false ($lfTipCalfBendCtrl[0]+".sx");
	setAttr -lock true -keyable false -channelBox false ($lfTipCalfBendCtrl[0]+".sy");
	setAttr -lock true -keyable false -channelBox false ($lfTipCalfBendCtrl[0]+".sz");
	setAttr -lock true -keyable false -channelBox false ($lfTipCalfBendCtrl[0]+".v");
	
	//------------------------------------
	//Constraints of Control Master Groups
	//------------------------------------
