global proc jbBuildStretchy(string $lfShoulder, string $lfElbow, string $lfWrist, string $lfIkHandle, string $switchCtrl, string $rtShoulder, string $rtElbow, string $rtWrist, string $rtIkHandle)
{
	clearCache -all;
	
	float $lfShoulderPos[] = `xform -q -ws -t $lfShoulder`;
	float $lfElbowPos[] = `xform -q -ws -t $lfElbow`;
	float $lfWristPos[] = `xform -q -ws -t $lfWrist`;
		
	distanceDimension -sp $lfShoulderPos[0] $lfShoulderPos[1] $lfShoulderPos[2] -ep $lfWristPos[0] $lfWristPos[1] $lfWristPos[2];
	string $lfStretchDistTool = `rename distanceDimension1 "lfArmDistDimension"`;
	float $lfStretchDist = `getAttr ($lfStretchDistTool+".distance")`;
	
	//------------------------------------------
	//Clearing the Distance Base locator for use
	//------------------------------------------
	string $lfArmStretchBaseLoc = `rename locator1 "lfArmStretchBaseLoc"`;
	string $lfArmStretchBaseLocRf[] = `duplicate -rr -n ($lfArmStretchBaseLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfArmStretchBaseLoc;
	string $lfArmStretchBaseLocRf1[] = `duplicate -rr -n ($lfArmStretchBaseLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfArmStretchBaseLoc $lfArmStretchBaseLocRf[0];
	parent $lfArmStretchBaseLocRf[0] $lfArmStretchBaseLocRf1[0];	
	
	//------------------------------------------
	//Clearing the Distance Tip locator for use
	//------------------------------------------
	string $lfArmStretchTipLoc = `rename locator2 "lfArmStretchTipLoc"`;	
	string $lfArmStretchTipLocRf[] = `duplicate -rr -n ($lfArmStretchTipLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $lfArmStretchTipLoc;
	string $lfArmStretchTipLocRf1[] = `duplicate -rr -n ($lfArmStretchTipLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $lfArmStretchTipLoc $lfArmStretchTipLocRf[0];
	parent $lfArmStretchTipLocRf[0] $lfArmStretchTipLocRf1[0];	
	
	delete ($lfArmStretchBaseLoc+"GRP1");
	
	//----------------------------
	//Constraints for the locators
	//----------------------------
	pointConstraint -o 0 0 0 -w 1 $lfShoulder $lfArmStretchBaseLocRf1[0];
	pointConstraint -o 0 0 0 -w 1 $lfIkHandle $lfArmStretchTipLocRf1[0];
	
	//--------------------------------------------------------------------
	//Adding a global scale attribute to the switch command for the moment
	//--------------------------------------------------------------------
	select -r $switchCtrl;
	addAttr -ln "globalScale" -at double -min 0.1 -dv 1 -k 1;
	addAttr -ln "lfArmStretch" -at "enum" -en "off:on:" -k 1;
	addAttr -ln "rtArmStretch" -at "enum" -en "off:on:" -k 1;
	
	//----------------------
	//Global scale fix nodes
	//----------------------
	string $globalMDN = `shadingNode -asUtility multiplyDivide -n "globalCharScaleMDN"`;
	connectAttr -f ($switchCtrl+".globalScale") ($globalMDN+".input1X");
	setAttr ($globalMDN+".operation") 1; // Multiply Operation
	setAttr ($globalMDN+".input2X") $lfStretchDist;
	
	//------------------
	//Stretch Main Nodes
	//------------------
	string $lfArmStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfArmStretchMDN"`;
	setAttr ($lfArmStretchMDN+".operation") 2; // Divide Operation
	connectAttr -f ($lfStretchDistTool+".distance") ($lfArmStretchMDN+".input1X");
	connectAttr -f ($globalMDN+".outputX") ($lfArmStretchMDN+".input2X");
	
	string $lfArmStretchCond = `shadingNode -asUtility condition -n "lfArmStretchCond"`;
	setAttr ($lfArmStretchCond+".operation") 3; // Greater than or Equal Operation
	connectAttr -f ($lfStretchDistTool+".distance") ($lfArmStretchCond+".firstTerm");
	connectAttr -f ($globalMDN+".outputX") ($lfArmStretchCond+".secondTerm");
	connectAttr -f ($lfArmStretchMDN+".outputX") ($lfArmStretchCond+".colorIfTrueR");
	
	//--------------------
	//Stretch Enable Nodes
	//--------------------
	string $lfArmStretchSwitch = `shadingNode -asUtility condition -n "lfArmStretchSwitch"`;
	setAttr ($lfArmStretchSwitch+".operation") 0; // Equal Operation
	setAttr ($lfArmStretchSwitch+".secondTerm") 1; 
	connectAttr -f ($switchCtrl+".lfArmStretch") ($lfArmStretchSwitch+".firstTerm");
	//connectAttr -f ($lfArmStretchCond+".outColorR") ($lfArmStretchSwitch+".firstTerm");
	connectAttr -f ($lfArmStretchCond+".outColorR") ($lfArmStretchSwitch+".colorIfTrueR");
	
	//-------------------------------------------
	//Creates the local Nodes per joint to stretch
	//-------------------------------------------
	float $elbowTransY = `getAttr ($lfElbow+".ty")`;
	string $lfElbowStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfElbowStretchMDN"`;
	setAttr ($lfElbowStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($lfArmStretchSwitch+".outColorR") ($lfElbowStretchMDN+".input1X");
	setAttr ($lfElbowStretchMDN+".input2X") $elbowTransY;
	connectAttr -f ($lfElbowStretchMDN+".outputX") ($lfElbow+".ty");
	
	float $wristTransY = `getAttr ($lfWrist+".ty")`;
	string $lfWristStretchMDN = `shadingNode -asUtility multiplyDivide -n "lfWristStretchMDN"`;
	setAttr ($lfWristStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($lfArmStretchSwitch+".outColorR") ($lfWristStretchMDN+".input1X");
	setAttr ($lfWristStretchMDN+".input2X") $wristTransY;
	connectAttr -f ($lfWristStretchMDN+".outputX") ($lfWrist+".ty");
	
	//-------------------
	//-------------------
	//Right side stretchy
	//-------------------
	//-------------------
	
	float $rtShoulderPos[] = `xform -q -ws -t $rtShoulder`;
	float $rtElbowPos[] = `xform -q -ws -t $rtElbow`;
	float $rtWristPos[] = `xform -q -ws -t $rtWrist`;
	
	distanceDimension -sp $rtShoulderPos[0] $rtShoulderPos[1] $rtShoulderPos[2] -ep $rtWristPos[0] $rtWristPos[1] $rtWristPos[2];
	string $rtStretchDistTool = `rename distanceDimension1 "rtArmDistDimension"`;
	float $rtStretchDist = `getAttr ($rtStretchDistTool+".distance")`;
	
	//------------------------------------------
	//Clearing the Distance Base locator for use
	//------------------------------------------
	string $rtArmStretchBaseLoc = `rename locator1 "rtArmStretchBaseLoc"`;
	string $rtArmStretchBaseLocRf[] = `duplicate -rr -n ($rtArmStretchBaseLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtArmStretchBaseLoc;
	string $rtArmStretchBaseLocRf1[] = `duplicate -rr -n ($rtArmStretchBaseLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $rtArmStretchBaseLoc $rtArmStretchBaseLocRf[0];
	parent $rtArmStretchBaseLocRf[0] $rtArmStretchBaseLocRf1[0];	
	
	//------------------------------------------
	//Clearing the Distance Tip locator for use
	//------------------------------------------
	string $rtArmStretchTipLoc = `rename locator2 "rtArmStretchTipLoc"`;	
	string $rtArmStretchTipLocRf[] = `duplicate -rr -n ($rtArmStretchTipLoc+"GRP")`;
	pickWalk -d down;
	doDelete;
	select -r $rtArmStretchTipLoc;
	string $rtArmStretchTipLocRf1[] = `duplicate -rr -n ($rtArmStretchTipLoc+"MasterGRP")`;
	pickWalk -d down;
	doDelete;
	parent $rtArmStretchTipLoc $rtArmStretchTipLocRf[0];
	parent $rtArmStretchTipLocRf[0] $rtArmStretchTipLocRf1[0];
	
	delete ($rtArmStretchBaseLoc+"GRP1");
	
	//----------------------------
	//Constraints for the locators
	//----------------------------
	pointConstraint -o 0 0 0 -w 1 $rtShoulder $rtArmStretchBaseLocRf1[0];
	pointConstraint -o 0 0 0 -w 1 $rtIkHandle $rtArmStretchTipLocRf1[0];
	
	//------------------
	//Stretch Main Nodes
	//------------------
	string $rtArmStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtArmStretchMDN"`;
	setAttr ($rtArmStretchMDN+".operation") 2; // Divide Operation
	connectAttr -f ($rtStretchDistTool+".distance") ($rtArmStretchMDN+".input1X");
	connectAttr -f ($globalMDN+".outputX") ($rtArmStretchMDN+".input2X");
	
	string $rtArmStretchCond = `shadingNode -asUtility condition -n "rtArmStretchCond"`;
	setAttr ($rtArmStretchCond+".operation") 3; // Greater than or Equal Operation
	connectAttr -f ($rtStretchDistTool+".distance") ($rtArmStretchCond+".firstTerm");
	connectAttr -f ($globalMDN+".outputX") ($rtArmStretchCond+".secondTerm");
	connectAttr -f ($rtArmStretchMDN+".outputX") ($rtArmStretchCond+".colorIfTrueR");
	
	//--------------------
	//Stretch Enable Nodes
	//--------------------
	string $rtArmStretchSwitch = `shadingNode -asUtility condition -n "rtArmStretchSwitch"`;
	setAttr ($rtArmStretchSwitch+".operation") 0; // Equal Operation
	setAttr ($rtArmStretchSwitch+".secondTerm") 1; 
	connectAttr -f ($switchCtrl+".rtArmStretch") ($rtArmStretchSwitch+".firstTerm");
	//connectAttr -f ($rtArmStretchCond+".outColorR") ($rtArmStretchSwitch+".firstTerm");
	connectAttr -f ($rtArmStretchCond+".outColorR") ($rtArmStretchSwitch+".colorIfTrueR");
	
	//-------------------------------------------
	//Creates the local Nodes per joint to stretch
	//-------------------------------------------
	float $elbowTransY = `getAttr ($rtElbow+".ty")`;
	string $rtElbowStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtElbowStretchMDN"`;
	setAttr ($rtElbowStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($rtArmStretchSwitch+".outColorR") ($rtElbowStretchMDN+".input1X");
	setAttr ($rtElbowStretchMDN+".input2X") $elbowTransY;
	connectAttr -f ($rtElbowStretchMDN+".outputX") ($rtElbow+".ty");
	
	float $wristTransY = `getAttr ($rtWrist+".ty")`;
	string $rtWristStretchMDN = `shadingNode -asUtility multiplyDivide -n "rtWristStretchMDN"`;
	setAttr ($rtWristStretchMDN+".operation") 1; // Multiply Operation
	connectAttr -f ($rtArmStretchSwitch+".outColorR") ($rtWristStretchMDN+".input1X");
	setAttr ($rtWristStretchMDN+".input2X") $wristTransY;
	connectAttr -f ($rtWristStretchMDN+".outputX") ($rtWrist+".ty");	
	
	string $dntGrp;
	
	if(`objExists DO_NOT_TOUCH_GRP`)
	{
		string $dntGrp = "DO_NOT_TOUCH_GRP";
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		string $stretchyArmParts = `group -em -n "stretchyArmParts"`;
		parent $lfStretchDistTool $lfArmStretchBaseLocRf1[0] $lfArmStretchTipLocRf1[0] $rtStretchDistTool $rtArmStretchBaseLocRf1[0] $rtArmStretchTipLocRf1[0] $stretchyArmParts;
		parent $stretchyArmParts $dntGrp;
	}
	else
	{
		//
		$dntGrp = `group -em -n "DO_NOT_TOUCH_GRP"`;
		setAttr ($dntGrp+".inheritsTransform") 0;
		setAttr ($dntGrp+".visibility") 0;
		string $stretchyArmParts = `group -em -n "stretchyArmParts"`;
		parent $lfStretchDistTool $lfArmStretchBaseLocRf1[0] $lfArmStretchTipLocRf1[0] $rtStretchDistTool $rtArmStretchBaseLocRf1[0] $rtArmStretchTipLocRf1[0] $stretchyArmParts;
		parent $stretchyArmParts $dntGrp;
	}
	
	select -r $dntGrp;
	select -hi;
	sets -n armStretchComponents;
	select -r $globalMDN $lfArmStretchMDN $lfArmStretchCond $lfArmStretchSwitch $lfElbowStretchMDN $lfWristStretchMDN $rtArmStretchMDN $rtArmStretchCond $rtArmStretchSwitch $rtElbowStretchMDN $rtWristStretchMDN;
	sets -add armStretchComponents;
	
	
}//End of global proc jbBuildStretchy(END)

