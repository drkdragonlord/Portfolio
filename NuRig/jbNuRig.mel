	sets -n armControlsSet;
	
	
	select -r -ne armJointsSet armGroupsSet armConstraintsSet armIkHandlesSet armIkEffectorsSet armBendyComponents armStretchComponents armSwitchPartsSet fingerComponentsSet armControlsSet ;
	sets -n armComponents;
	
