	setAttr -lock true -keyable false ($legRtNodes[2]+".sy");
	setAttr -lock true -keyable false ($legRtNodes[2]+".sz");
	setAttr -lock true -keyable false ($legRtNodes[2]+".v");
	
	setAttr -lock true -keyable false ($legRtNodes[3]+".sx");
	setAttr -lock true -keyable false ($legRtNodes[3]+".sy");
	setAttr -lock true -keyable false ($legRtNodes[3]+".sz");
	setAttr -lock true -keyable false ($legRtNodes[3]+".v");
	
	setAttr -lock true -keyable false ($legRtNodes[4]+".sx");
	setAttr -lock true -keyable false ($legRtNodes[4]+".sy");
	setAttr -lock true -keyable false ($legRtNodes[4]+".sz");
	setAttr -lock true -keyable false ($legRtNodes[4]+".v");
	
	setAttr -lock true -keyable false ($legRtPVNode+".tx");
	setAttr -lock true -keyable false ($legRtPVNode+".ty");
	setAttr -lock true -keyable false ($legRtPVNode+".rx");
	setAttr -lock true -keyable false ($legRtPVNode+".ry");
	setAttr -lock true -keyable false ($legRtPVNode+".rz");
	setAttr -lock true -keyable false ($legRtPVNode+".sx");
	setAttr -lock true -keyable false ($legRtPVNode+".sy");
	setAttr -lock true -keyable false ($legRtPVNode+".sz");
	setAttr -lock true -keyable false ($legRtPVNode+".v");
	
	//Locks the translate Y attributes of the Elbow and wrist joint to maintain co-planar effects. As well as locking the rotate X and Z of the Elbow Node
	setAttr -lock true -keyable false ($legRtNodes[1]+".tx");
	setAttr -lock true -keyable false ($legRtNodes[1]+".rz");
	setAttr -lock true -keyable false ($legRtNodes[1]+".ry");
	setAttr -lock true -keyable false ($legRtNodes[2]+".tx");
	
	//Creates sets to hold the Left Side Nodes for easy script access
	select -cl;
	select -r $legRtNodes;
	sets -n "rtLegGuideNodes";
	
	select -cl;
	select -r $legRtPVNode;
	sets -n "rtLegPVGuideNodes";
	
	select -cl;
	select -add $nullsRtGrp $rtPvNull;
	sets -n "rtLegGuideNodeGrps";
	
	select -cl;
	select -add $legRtBoneGrp $legRtBoneContainer $clusterTipHndls $clusterBaseHndls $clusterPVTipHndl[0] $clusterPVBaseHndl[0] legPVBoneRt01;
	sets -n "rtLegGuideElements";
	
	//Creates a Master set to hold all node sets of both sides
	select -cl;
	select -ne rtLegGuideNodes rtLegGuideNodeGrps rtLegGuideElements lfLegGuideNodes lfLegGuideNodeGrps lfLegGuideElements rtLegPVGuideNodes lfLegPVGuideNodes;
	sets -n legGuides;
	
	//----------------------------------
	//Creates the Pelvis and Root Guides
	//----------------------------------
	
	jbJntNode();
	select -r node01;
	string $rootNode = `rename node01 "rootNode01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("rootNodeShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("rootNodeShape"+($i+1));
			}						
		}	
	}
	setAttr ($rootNode+".ty") 12;
	
	jbJntNode();
	select -r node01;
	string $pelvisNode = `rename node01 "pelvisNode01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("pelvisNodeShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("pelvisNodeShape"+($i+1));
			}						
		}	
	}
	setAttr ($pelvisNode+".ty") 11;
	setAttr ($pelvisNode+".tz") -0.5;
	
	select -add $rootNode $pelvisNode;
	string $cogNodes[] = `ls -sl`;
	
	//Creates a variable to use to populate the leg nodes
	string $cogNullsGrp[];
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($cogNodes);$i++)
	{
		string $cogNulls = `group -em -n ($cogNodes[$i]+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $cogNodes[$i] $cogNulls`;		
		delete`pointConstraint -o 0 0 0 -w 1 $cogNodes[$i] $cogNulls`;
		$cogNullsGrp[$i] = $cogNulls;
	}
	parent $cogNodes[0] $cogNullsGrp[0];
	parent $cogNodes[1] $cogNullsGrp[1];
	
	parent $cogNullsGrp[1] $cogNodes[0];	
	
	//creates the COG Bone lines
	string $cogMainBone = `curve -d 1 -p 0 12 0 -p 0 11 -0.5 -k 0 -k 1 -n ("cogMainBone01")`;
	string $cogLfBone = `curve -d 1 -p 0 11 -0.5 -p 3 10 0 -k 0 -k 1 -n ("cogLfBone01")`;
	string $cogRtBone = `curve -d 1 -p 0 11 -0.5 -p -3 10 0 -k 0 -k 1 -n ("cogRtBone01")`;
	
	string $cogBoneGrp = `group -em -n "cogBoneGrp"`;
	parent $cogMainBone $cogLfBone $cogRtBone $cogBoneGrp;
	
	//Clusters for root to pelvis bone
	select -r ($cogMainBone+".cv[0]");
	string $cogClstrBase[] = `cluster -n ($cogMainBone+"Base")`;
	setAttr ($cogClstrBase[1]+".visibility") 0;
	select -r ($cogMainBone+".cv[1]");
	string $cogClstrTip[] = `cluster -n ($cogMainBone+"Tip")`;
	setAttr ($cogClstrTip[1]+".visibility") 0;
	
	//Cluster from pelvis to left thigh bone
	select -r ($cogLfBone+".cv[0]");
	string $cogLfClstrBase[] = `cluster -n ($cogLfBone+"Base")`;
	setAttr ($cogLfClstrBase[1]+".visibility") 0;
	select -r ($cogLfBone+".cv[1]");
	string $cogLfClstrTip[] = `cluster -n ($cogLfBone+"Tip")`;
	setAttr ($cogLfClstrTip[1]+".visibility") 0;
	
	//Cluster from pelvis to right thigh bone
	select -r ($cogRtBone+".cv[0]");
	string $cogRtClstrBase[] = `cluster -n ($cogRtBone+"Base")`;
	setAttr ($cogRtClstrBase[1]+".visibility") 0;
	select -r ($cogRtBone+".cv[1]");
	string $cogRtClstrTip[] = `cluster -n ($cogRtBone+"Tip")`;
	setAttr ($cogRtClstrTip[1]+".visibility") 0;
	
	parent -a $cogClstrBase[1] $cogNodes[0];
	
	parent -a $cogClstrTip[1] $cogNodes[1];
	parent -a $cogLfClstrBase[1] $cogNodes[1];
	parent -a $cogRtClstrBase[1] $cogNodes[1];
	
	parent -a $cogLfClstrTip[1] $legLfBaseNode;
	parent -a $cogRtClstrTip[1] $legRtBaseNode;
	
	parent $nullsLfGrp[0] $cogNodes[1];
	parent $nullsRtGrp[0] $cogNodes[1];
	
	//Creates sets for all Cog Elements
	select -cl;
	select -add $cogNodes[0] $cogNodes[1] $cogNullsGrp[1] $cogNullsGrp[0] $cogBoneGrp $cogMainBone $cogLfBone $cogRtBone $cogClstrBase[1] $cogClstrTip[1] $cogLfClstrBase[1] $cogRtClstrBase[1] $cogLfClstrTip[1] $cogRtClstrTip[1];
	sets -n "cogGuideNodes";
	select -ne cogGuideNodes;
	sets -add legGuides;
	
	if(`objExists node01Grp`)
	{
		string $spineToCogBone = `curve -d 1 -p 0 12 0 -p 0 13 0 -k 0 -k 1 -n ("spineToCogBone")`;
		parent "node01Grp" $cogNodes[0];
		
		//Cluster from pelvis to right thigh bone
		select -r ($spineToCogBone+".cv[0]");
		string $spineToCogBoneClstrBase[] = `cluster -n ($spineToCogBone+"Base")`;
		setAttr ($spineToCogBoneClstrBase[1]+".visibility") 0;
		select -r ($spineToCogBone+".cv[1]");
		string $spineToCogBoneClstrTip[] = `cluster -n ($spineToCogBone+"Tip")`;
		setAttr ($spineToCogBoneClstrTip[1]+".visibility") 0;
		
		setAttr ($spineToCogBone+".overrideEnabled") 1;
		setAttr ($spineToCogBone+".overrideDisplayType") 2;
		
		parent -a $spineToCogBoneClstrBase[1] $cogNodes[0];
		parent -a $spineToCogBoneClstrTip[1] "spineNode01";
		parent $spineToCogBone $cogBoneGrp;
		
		select -cl;
		select -add $spineToCogBone $spineToCogBoneClstrBase[1] $spineToCogBoneClstrTip[1];
		sets -add cogGuideNodes;
	}
	
	setAttr ($cogMainBone+".overrideEnabled") 1;
	setAttr ($cogMainBone+".overrideDisplayType") 2;
	setAttr ($cogLfBone+".overrideEnabled") 1;
	setAttr ($cogLfBone+".overrideDisplayType") 2;
	setAttr ($cogRtBone+".overrideEnabled") 1;
	setAttr ($cogRtBone+".overrideDisplayType") 2;
	
	//locking unecessary attributes
	select -r $cogNodes;
	for($i=0; $i<size($cogNodes);$i++)
	{		
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".rx");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".ry");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".rz");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".sx");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".sy");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".sz");
		setAttr -lock true -keyable false -channelBox false ($cogNodes[$i]+".v");
	}
	select -cl;
	
}//End of global proc jbCreateLegGuides(END)
	
global proc jbCreateSpineGuides()
{
	//creates the initial node
	jbJntNode();
	select -r node01;
	string $baseNode = `rename node01 "spineNode01"`;
	if(`objExists myShape01`)
	{
		for($i = 0; $i < 10; $i++)
		{
			if($i < 9)
			{
				rename ("myShape0"+($i+1)) ("spineShape0"+($i+1));
			}	
			else if($i == 9)
			{
				rename "myShape10" ("spineShape"+($i+1));
			}						
		}	
	}

	select -r $baseNode;
	
	//creates the nodes and places them accordingly
	for($i=0;$i<8;$i++)
	{
		string $nodes[] = `duplicate -rr -n ("spineNode0"+($i+2))`;
		setAttr ($nodes[0]+".ty") ($i+1);
						
	}
	
	//creates arrays of all the items created to be used in parenting procedures
	select -cl;
	
	select -add spineNode01 spineNode02 spineNode03 spineNode04 spineNode05 spineNode06 spineNode07 spineNode08 spineNode09;
	string $nodes[] = `ls -sl`;
	
	//creates constraints between the nodes and the empty groups
	for($i=0;$i<size($nodes);$i++)
	{
		string $nulls = `group -em -n ("node0"+($i+1)+"Grp")`;
		delete`orientConstraint -o 0 0 0 -w 1 $nodes[$i] $nulls`;		
		delete`pointConstraint -o 0 0 0 -w 1 $nodes[$i] $nulls`;
	}
	
	//creates arrays of all the items created to be used in parenting procedures
	select -cl;
	
	select -add node01Grp node02Grp node03Grp node04Grp node05Grp node06Grp node07Grp node08Grp node09Grp;
	string $nodeNulls[] = `ls -sl`;
	
	//creates the clusters and connecting bone lines
	for($i=0;$i<8;$i++)
	{
		string $bone = `curve -d 1 -p 0 $i 0 -p 0 ($i+1) 0 -k 0 -k 1 -n ("bone0"+($i+1))`;
		select -r ($bone+".cv[0]");
		string $clstrBase[] = `cluster -n ($bone+"Base")`;
		select -r ($bone+".cv[1]");
		string $clstrTip[] = `cluster -n ($bone+"Tip")`;
	}
	
	//creates arrays of all the items created to be used in parenting procedures
	select -cl;	
	select -add bone01 bone02 bone03 bone04 bone05 bone06 bone07 bone08;
	string $boneGrp[] = `ls -sl`;
	
	select -cl;
	select -add bone01TipHandle bone02TipHandle bone03TipHandle bone04TipHandle bone05TipHandle bone06TipHandle bone07TipHandle bone08TipHandle;
	string $clusterTipHndls[] = `ls -sl`;
	
	select -cl;	
	select -add bone01BaseHandle bone02BaseHandle bone03BaseHandle bone04BaseHandle bone05BaseHandle bone06BaseHandle bone07BaseHandle bone08BaseHandle;
	string $clusterBaseHndls[] = `ls -sl`;
		
	select -cl;
	
	//parents the clusters to the nodes and sets some item visibilities and node states
	for($i=0;$i<8;$i++)
	{
		parent -a $clusterTipHndls[$i] $nodes[$i+1];
		parent -a $clusterBaseHndls[$i] $nodes[$i];
		setAttr ($clusterTipHndls[$i]+".visibility") 0;
		setAttr ($clusterBaseHndls[$i]+".visibility") 0;
		setAttr ($boneGrp[$i]+".overrideEnabled") 1;
		setAttr ($boneGrp[$i]+".overrideDisplayType") 2;				
	}
	//creates a group for the bone curves to be parented into
	select -cl;
	select $boneGrp;
	string $boneContainer = `group -n "spineBoneGrp"`;
	
	//parents the nodes to the groups and the greater group to the lower node for an FK type setup
	for($i=0;$i<9;$i++)
	{
		parent -a $nodes[$i] $nodeNulls[$i];
		if($i<=7){
		parent -a $nodeNulls[$i+1] $nodes[$i] ;
		}
		else
		{
		}
	}
	
	//creates selection sets
	select -cl;
	select -add $clusterTipHndls $clusterBaseHndls $boneGrp $boneContainer;
	sets -n spineGuideElements;
	select -cl;
	select -add $nodes;
	sets -n spineGuideNodes;
	select -cl;
	select -add $nodeNulls;
	sets -n spineGuideNodeGrps;
	select -cl;
	select -add -ne spineGuideElements spineGuideNodes spineGuideNodeGrps;
	sets -n spineGuides;
	select -cl;
	
	select -r $nodeNulls[0];
	move -r 0 13 0 ;
	
	//locking of uneeded attributes
	select -r $nodes;
	for($i=0; $i<size($nodes);$i++)
	{		
		if($i<1)
		{
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".ry");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".rz");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sx");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sy");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sz");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".v");
		}
		else
		{
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".rx");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".ry");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".rz");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sx");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sy");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".sz");
			setAttr -lock true -keyable false -channelBox false ($nodes[$i]+".v");
		}
	}
	select -cl;
	
}//End of global proc jbCreateSpineGuides(END)

global proc jbCreateAllGuides()
