	pointConstraint -o 0 0 0 -w 1 $lfTipCalfBendCtrl[0] $lfCalfTipBendyJnt;
	orientConstraint -o 0 0 0 -w 1 $lfTipCalfBendCtrl[0] $lfCalfTipBendyJnt;
	
	//----------------
	//Right side Bendy
	//----------------
	float $rtThighPos[] = `xform -q -ws -t $rtThigh`;
	float $rtKneePos[] = `xform -q -ws -t $rtKnee`;
	float $rtAnklePos[] = `xform -q -ws -t $rtAnkle`;
	
	string $rtUpperLegSurf[] = `nurbsPlane -p 0 0 0 -ax 1 0 0 -w 1 -lr 1 -d 1 -u 1 -v 1 -ch 1 -n "rtUpperLegBendSurf"`;
	move -r 0 0.5 0 ;
	move 0 0 0 ($rtUpperLegSurf[0]+".scalePivot") ($rtUpperLegSurf[0]+".rotatePivot") ;
	setAttr ($rtUpperLegSurf[0]+".visibility") 0;
		
