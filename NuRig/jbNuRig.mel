global proc jbFixArmLength()
{
	float $charSize = `getAttr switchCtrl.globalScale`;
	if($charSize != 1)
	{
		float $arcLen = `getAttr lfArmDistDimensionShape.distance`;
		setAttr globalCharScaleMDN.input2X $arcLen;
	}
	else if($charSize == 1)
	{
		float $arcLen = `getAttr lfArmDistDimensionShape.distance`;
		setAttr globalCharScaleMDN.input2X $arcLen;
	}
	
	
}//End of global proc jbFixArmLength(END)

global proc jbFixSpineLength()
{
	float $charSize = `getAttr characterMainCtrl.characterSize`;
	if($charSize != 1)
	{
		float $arcLen = `getAttr spineCrvInfoNode.arcLength`;
		setAttr spineLengthMDN.input2X $arcLen;
		//setAttr stretchSwitch.colorIfFalseR $arcLen;
	}
	else if($charSize == 1)
	{
		float $arcLen = `getAttr spineCrvInfoNode.arcLength`;
		setAttr spineLengthMDN.input2X $arcLen;
	}
	
	
}//End of global proc jbFixSpineLength(END)

global proc jbFinalizeSetup()
{
	if(`objExists spineJnt01`)
	{
		//creation of variables to use
		string $dntGrp = "DO_NOT_TOUCH_GRP";
		string $skelGrp = `group -em -n "skeletons"`;
		string $bendyJntsGrp = `group -em -n "bendyJntsGrp"`;
		string $bendyCtrlsGrp = `group -em -n "bendyControlsGrp"`;
		string $controlsGrp = `group -em -n "controls"`;
		string $mainGrp = `group -em -n "MAIN"`;
		string $geoGrp = `group -em -n "GEO"`;
		string $godNode = `group -em -n "GOD"`;
		
		//placing bendy joints in an array and a group
		select -r lfArmBaseBendyJnt lfArmMidBendyJnt lfArmTipBendyJnt lfForearmBaseBendyJnt lfForearmMidBendyJnt lfForearmTipBendyJnt rtArmBaseBendyJnt rtArmMidBendyJnt rtArmTipBendyJnt rtForearmBaseBendyJnt rtForearmMidBendyJnt rtForearmTipBendyJnt lfLegBaseBendyJnt lfLegMidBendyJnt lfLegTipBendyJnt lfCalfBaseBendyJnt lfCalfMidBendyJnt lfCalfTipBendyJnt rtLegBaseBendyJnt rtLegMidBendyJnt rtLegTipBendyJnt rtCalfBaseBendyJnt rtCalfMidBendyJnt rtCalfTipBendyJnt;
		string $bendyJnts[] = `ls -sl`;
		parent $bendyJnts $bendyJntsGrp;
		
		//placing bendy controls in an array and a group
		select -r lfBaseArmBendCtrlMasterGRP lfMidArmBendCtrlMasterGRP lfTipArmBendCtrlMasterGRP	lfBaseForearmBendCtrlMasterGRP lfMidForearmBendCtrlMasterGRP lfTipForearmBendCtrlMasterGRP	rtBaseArmBendCtrlMasterGRP rtMidArmBendCtrlMasterGRP rtTipArmBendCtrlMasterGRP	rtBaseForearmBendCtrlMasterGRP	rtMidForearmBendCtrlMasterGRP rtTipForearmBendCtrlMasterGRP	lfBaseLegBendCtrlMasterGRP	lfMidLegBendCtrlMasterGRP lfTipLegBendCtrlMasterGRP	lfBaseCalfBendCtrlMasterGRP lfMidCalfBendCtrlMasterGRP lfTipCalfBendCtrlMasterGRP	rtBaseLegBendCtrlMasterGRP rtMidLegBendCtrlMasterGRP rtTipLegBendCtrlMasterGRP	rtBaseCalfBendCtrlMasterGRP rtMidCalfBendCtrlMasterGRP rtTipCalfBendCtrlMasterGRP;
		string $bendyCtrls[] = `ls -sl`;
		parent $bendyCtrls $bendyCtrlsGrp;
		
		//parenting DNT groups
		parent rtLegIkHandlesMasterGrp lfLegIkHandlesMasterGrp RIG_DNT $dntGrp;
		
		//parenting joints
		parent neckJnt01 lfClavJnt rtClavJnt spineJnt09;
		parent spineJnt01 rootJnt;
		parent $bendyJntsGrp rootJnt $skelGrp;
		
		//parenting controls
		parent lfClavJntCtrlMasterGRP rtClavJntCtrlMasterGRP spineHiCtrl;
		parent spineMainCtrlGrp rootCtrl;
		parent rtHandCtrlMasterGRP lfHandCtrlMasterGRP rtFootCtrlMasterGrp lfFootCtrlMasterGrp rtAnkleJntIkCtrlMasterGRP lfAnkleJntIkCtrlMasterGRP armLfPvIkCtrlGRP lfWristIkHandleMasterGRP lfWristJntIkCtrlMasterGRP armRtPvIkCtrlGRP rtWristIkHandleMasterGRP rtWristJntIkCtrlMasterGRP switchCtrlGRP rootCtrlMasterGRP $bendyCtrlsGrp $controlsGrp;
		parent $skelGrp $controlsGrp characterMainCtrl;
		parent characterMainCtrlGrp $mainGrp;
		
		//pareting to GOD node
		parent $mainGrp $geoGrp $dntGrp $godNode;
		
		//hiding the visibility of the bendy joints
		setAttr ($bendyJntsGrp+".visibility") 0;
		
		//fixing issues with the root control pelvic control and hand controls
		float $rootJntPos[] = `xform -q -ws -t rootJnt`;
		select -r rootCtrlShape.cv[0:7] rootCtrlShape1.cv[0:6] rootCtrlShape2.cv[0:6] rootCtrlShape3.cv[0:6] rootCtrlShape4.cv[0:6] rootCtrlShape5.cv[0:7] ;
		move -y $rootJntPos[1] ;
		
		float $pelvicJntPos[] = `xform -q -ws -t pelvicJnt`;
		select -r pelvicCtrlShape.cv[0:31] pelvicCtrlShape1.cv[0:31] ;
		move -y $pelvicJntPos[1];
		
		
		float $lfHandJntPos[] = `xform -q -ws -t lfHandJnt`;
		select -r lfHandCtrlShape.cv[0:18] ;
		//move -r $lfHandJntPos[0] 0 $lfHandJntPos[2];
		move -y ($lfHandJntPos[1]+2);
		//move -z ;
		
		float $rtHandJntPos[] = `xform -q -ws -t rtHandJnt`;
		select -r rtHandCtrlShape.cv[0:18] ;
		//move -r $rtHandJntPos[0] 0 $rtHandJntPos[2];
		move -y ($rtHandJntPos[1]+2);
		//move -z ;
		
		//Resizing neck controls
		select -r neckJnt01CtrlShape.cv[0:7] ;
		scale -oa 0rad 3.141593rad 0rad -r -p 0cm 22cm 0cm 2.25 2.25 2.25 ;
		
		float $neckJnt02Pos[] = `xform -q -ws -t neckJnt02`;
		select -r neckJnt02CtrlShape.cv[0:7] ;
		scale -oa 0rad 3.141593rad 0rad -r -p 0cm 22cm 0cm 2.25 2.25 2.25 ;
		move -y ($neckJnt02Pos[1]);
		
		float $neckJnt03Pos[] = `xform -q -ws -t headJnt01`;
		select -r neckJnt03CtrlShape.cv[0:7] ;
		scale -oa 0rad 3.141593rad 0rad -r -p 0cm 22cm 0cm 2.25 2.25 2.25 ;
		move -y ($neckJnt03Pos[1]+4);
		
		//Connecting the character size to the switch control attribute
		connectAttr -f characterMainCtrl.characterSize switchCtrl.globalScale;; 
	
		//cleaning the sets
		select -r $skelGrp $bendyJntsGrp $bendyCtrlsGrp $controlsGrp $mainGrp $geoGrp $godNode;
		sets -n mainRigComponents;
		
		select -r -ne armComponents legComponents spineComponents headComponents neckComponents mainRigComponents;
		sets -n nuRigComponents;
	}
	if(`objExists rootJnt`)
	{
		select -r rootJnt;
		string $rootJnt[] = `ls -sl`;
		jbCreateSkinJoints($rootJnt[0]);
	}
		
	select -cl;
	
}//End of global proc jbFinalizeSetup(END)

